[
  {
    "id": 5907,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "وَالسَّمَآءِ وَالطَّارِقِ ١",
    "text_simple": "والسمآء والطارق"
  },
  {
    "id": 5908,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لطَّارِقُ ٢",
    "text_simple": "ومآ أدريك ما الطارق"
  },
  {
    "id": 5909,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "text_tashkeel": "اُ۬لنَّجْمُ اُ۬لثَّاقِبُ ٣",
    "text_simple": "النجم الثاقب"
  },
  {
    "id": 5910,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": " إِن كُلُّ نَفْسٖ لَّمَا عَلَيْهَا حَافِظٞۖ ٤",
    "text_simple": "إن كل نفسٖ لما عليها حافظٞ"
  },
  {
    "id": 5911,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "فَلْيَنظُرِ اِ۬لِانسَٰنُ مِمَّ خُلِقَۖ ٥",
    "text_simple": "فلينظر الانسن مم خلق"
  },
  {
    "id": 5912,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 6,
    "text_tashkeel": " خُلِقَ مِن مَّآءٖ دَافِقٖ ٦",
    "text_simple": "خلق من مآءٖ دافقٖ"
  },
  {
    "id": 5913,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "يَخْرُجُ مِنۢ بَيْنِ اِ۬لصُّلْبِ وَالتَّرَآئِبِۖ ٧",
    "text_simple": "يخرج من بين الصلب والترآئب"
  },
  {
    "id": 5914,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 8,
    "text_tashkeel": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٞ ٨",
    "text_simple": "إنه على رجعه لقادرٞ"
  },
  {
    "id": 5915,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 9,
    "text_tashkeel": "يَوْمَ تُبْلَى اَ۬لسَّرَآئِرُ ٩",
    "text_simple": "يوم تبلى السرآئر"
  },
  {
    "id": 5916,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 10,
    "text_tashkeel": "فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖۖ ١٠",
    "text_simple": "فما له من قوةٖ ولا ناصرٖ"
  },
  {
    "id": 5917,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 11,
    "text_tashkeel": "وَالسَّمَآءِ ذَاتِ اِ۬لرَّجْعِ ١١",
    "text_simple": "والسمآء ذات الرجع"
  },
  {
    "id": 5918,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 12,
    "text_tashkeel": "وَالَارْضِ ذَاتِ اِ۬لصَّدْعِ ١٢",
    "text_simple": "والارض ذات الصدع"
  },
  {
    "id": 5919,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 13,
    "text_tashkeel": " إِنَّهُۥ لَقَوْلٞ فَصْلٞ ١٣",
    "text_simple": "إنه لقولٞ فصلٞ"
  },
  {
    "id": 5920,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 14,
    "text_tashkeel": "وَمَا هُوَ بِالْهَزْلِۖ ١٤",
    "text_simple": "وما هو بالهزل"
  },
  {
    "id": 5921,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 15,
    "text_tashkeel": "إِنَّهُمْ يَكِيدُونَ كَيْداٗ ١٥",
    "text_simple": "إنهم يكيدون كيداٗ"
  },
  {
    "id": 5922,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 16,
    "text_tashkeel": " وَأَكِيدُ كَيْداٗۖ ١٦",
    "text_simple": "وأكيد كيداٗ"
  },
  {
    "id": 5923,
    "jozz": 30,
    "page": "591",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 17,
    "text_tashkeel": "فَمَهِّلِ اِ۬لْكٰ۪فِرِينَ أَمْهِلْهُمْ رُوَيْداٗۖ ١٧",
    "text_simple": "فمهل الكفرين أمهلهم رويداٗ"
  }
]