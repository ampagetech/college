    "jozz": 30,
    "page": 590,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 20,
    "text_tashkeel": "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطُۢ ٢٠",
    "text_simple": "والله من ورائهم محيط",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
