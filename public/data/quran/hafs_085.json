    "page": 590,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 21,
    "text_tashkeel": "بَلۡ هُوَ قُرۡءَانٞ مَّجِيدٞ ٢١",
    "text_simple": "بل هو قرآن مجيد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
