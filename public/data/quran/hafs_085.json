    "jozz": 30,
    "page": 590,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 2,
    "text_tashkeel": "وَٱلۡيَوۡمِ ٱلۡمَوۡعُودِ ٢",
    "text_simple": "واليوم الموعود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
