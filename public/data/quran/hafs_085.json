  },
  {
    "id": 5912,
    "jozz": 30,
    "page": 590,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَشَاهِدٖ وَمَشۡهُودٖ ٣",
    "text_simple": "وشاهد ومشهود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5913,
    "jozz": 30,
    "page": 590,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": "قُتِلَ أَصۡحَٰبُ ٱلۡأُخۡدُودِ ٤",
    "text_simple": "قتل أصحاب الأخدود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5914,
    "jozz": 30,
    "page": 590,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "text_tashkeel": "ٱلنَّارِ ذَاتِ ٱلۡوَقُودِ ٥",
    "text_simple": "النار ذات الوقود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5915,
    "jozz": 30,
    "page": 590,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "إِذۡ هُمۡ عَلَيۡهَا قُعُودٞ ٦",
    "text_simple": "إذ هم عليها قعود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5916,
    "jozz": 30,
    "page": 590,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "وَهُمۡ عَلَىٰ مَا يَفۡعَلُونَ بِٱلۡمُؤۡمِنِينَ شُهُودٞ ٧",
    "text_simple": "وهم على ما يفعلون بالمؤمنين شهود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5917,
    "jozz": 30,
    "page": 590,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "وَمَا نَقَمُواْ مِنۡهُمۡ إِلَّآ أَن يُؤۡمِنُواْ بِٱللَّهِ ٱلۡعَزِيزِ ٱلۡحَمِيدِ ٨",
    "text_simple": "وما نقموا منهم إلا أن يؤمنوا بالله العزيز الحميد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5918,
    "jozz": 30,
    "page": 590,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 9,
    "text_tashkeel": "ٱلَّذِي لَهُۥ مُلۡكُ ٱلسَّمَٰوَٰتِ وَٱلۡأَرۡضِۚ وَٱللَّهُ عَلَىٰ كُلِّ شَيۡءٖ شَهِيدٌ ٩",
    "text_simple": "الذي له ملك السموات والأرض والله على كل شيء شهيد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5919,
    "jozz": 30,
    "page": 590,
    "line_start": 8,
    "line_end": 10,
    "aya_no": 10,
    "text_tashkeel": "إِنَّ ٱلَّذِينَ فَتَنُواْ ٱلۡمُؤۡمِنِينَ وَٱلۡمُؤۡمِنَٰتِ ثُمَّ لَمۡ يَتُوبُواْ فَلَهُمۡ عَذَابُ جَهَنَّمَ وَلَهُمۡ عَذَابُ ٱلۡحَرِيقِ ١٠",
    "text_simple": "إن الذين فتنوا المؤمنين والمؤمنات ثم لم يتوبوا فلهم عذاب جهنم ولهم عذاب الحريق",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5920,
    "jozz": 30,
    "page": 590,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 11,
    "text_tashkeel": "إِنَّ ٱلَّذِينَ ءَامَنُواْ وَعَمِلُواْ ٱلصَّٰلِحَٰتِ لَهُمۡ جَنَّٰتٞ تَجۡرِي مِن تَحۡتِهَا ٱلۡأَنۡهَٰرُۚ ذَٰلِكَ ٱلۡفَوۡزُ ٱلۡكَبِيرُ ١١",
    "text_simple": "إن الذين آمنوا وعملوا الصالحات لهم جنات تجري من تحتها الأنهار ذلك الفوز الكبير",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5921,
    "jozz": 30,
    "page": 590,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "إِنَّ بَطۡشَ رَبِّكَ لَشَدِيدٌ ١٢",
    "text_simple": "إن بطش ربك لشديد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5922,
    "jozz": 30,
    "page": 590,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 13,
    "text_tashkeel": "إِنَّهُۥ هُوَ يُبۡدِئُ وَيُعِيدُ ١٣",
    "text_simple": "إنه هو يبدئ ويعيد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5923,
    "jozz": 30,
    "page": 590,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 14,
    "text_tashkeel": "وَهُوَ ٱلۡغَفُورُ ٱلۡوَدُودُ ١٤",
    "text_simple": "وهو الغفور الودود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5924,
    "jozz": 30,
    "page": 590,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 15,
    "text_tashkeel": "ذُو ٱلۡعَرۡشِ ٱلۡمَجِيدُ ١٥",
    "text_simple": "ذو العرش المجيد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5925,
    "jozz": 30,
    "page": 590,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 16,
    "text_tashkeel": "فَعَّالٞ لِّمَا يُرِيدُ ١٦",
    "text_simple": "فعال لما يريد",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5926,
    "jozz": 30,
    "page": 590,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 17,
    "text_tashkeel": "هَلۡ أَتَىٰكَ حَدِيثُ ٱلۡجُنُودِ ١٧",
    "text_simple": "هل أتاك حديث الجنود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5927,
    "jozz": 30,
    "page": 590,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 18,
    "text_tashkeel": "فِرۡعَوۡنَ وَثَمُودَ ١٨",
    "text_simple": "فرعون وثمود",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
    "id": 5928,
    "jozz": 30,
    "page": 590,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 19,
    "text_tashkeel": "بَلِ ٱلَّذِينَ كَفَرُواْ فِي تَكۡذِيبٖ ١٩",
    "text_simple": "بل الذين كفروا في تكذيب",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج"
  },
  {
