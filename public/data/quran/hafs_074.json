  {
    "id": 5519,
    "jozz": 29,
    "page": 576,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 24,
    "text_tashkeel": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ ٢٤",
    "text_simple": "فقال إن هذا إلا سحر يؤثر",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5520,
    "jozz": 29,
    "page": 576,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ ٢٥",
    "text_simple": "إن هذا إلا قول البشر",
