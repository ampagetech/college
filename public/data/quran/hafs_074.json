    "text_simple": "لمن شاء منكم أن يتقدم أو يتأخر",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5533,
    "jozz": 29,
    "page": 576,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 38,
