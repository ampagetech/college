    "jozz": 29,
    "page": 576,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ ٢٣",
    "text_simple": "ثم أدبر واستكبر",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
