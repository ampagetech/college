    "jozz": 29,
    "page": 575,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ ١٠",
