    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5535,
    "jozz": 29,
    "page": 576,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 40,
    "text_tashkeel": "فِي جَنَّٰتٖ يَتَسَآءَلُونَ ٤٠",
