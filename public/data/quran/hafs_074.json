    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5523,
    "jozz": 29,
    "page": 576,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": "لَا تُبۡقِي وَلَا تَذَرُ ٢٨",
