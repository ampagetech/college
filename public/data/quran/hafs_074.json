    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5532,
    "jozz": 29,
    "page": 576,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 37,
