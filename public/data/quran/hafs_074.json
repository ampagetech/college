    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5530,
    "jozz": 29,
    "page": 576,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 35,
    "text_tashkeel": "إِنَّهَا لَإِحۡدَى ٱلۡكُبَرِ ٣٥",
    "text_simple": "إنها لإحدى الكبر",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر"
  },
  {
    "id": 5531,
    "jozz": 29,
    "page": 576,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 36,
    "text_tashkeel": "نَذِيرٗا لِّلۡبَشَرِ ٣٦",
