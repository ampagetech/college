    "jozz": 30,
    "page": 582,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 19,
    "text_tashkeel": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتۡ أَبۡوَٰبٗا ١٩",
    "text_simple": "وفتحت السماء فكانت أبوابا",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ"
