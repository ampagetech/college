    "page": 593,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 2,
    "text_tashkeel": "وَلَيَالٍ عَشۡرٖ ٢",
    "text_simple": "وليال عشر",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 5996,
    "jozz": 30,
    "page": 593,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 3,
    "text_tashkeel": "وَٱلشَّفۡعِ وَٱلۡوَتۡرِ ٣",
    "text_simple": "والشفع والوتر",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 5997,
    "jozz": 30,
    "page": 593,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": "وَٱلَّيۡلِ إِذَا يَسۡرِ ٤",
    "text_simple": "والليل إذا يسر",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 5998,
    "jozz": 30,
    "page": 593,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 5,
    "text_tashkeel": "هَلۡ فِي ذَٰلِكَ قَسَمٞ لِّذِي حِجۡرٍ ٥",
    "text_simple": "هل في ذلك قسم لذي حجر",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 5999,
    "jozz": 30,
    "page": 593,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "أَلَمۡ تَرَ كَيۡفَ فَعَلَ رَبُّكَ بِعَادٍ ٦",
    "text_simple": "ألم تر كيف فعل ربك بعاد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6000,
    "jozz": 30,
    "page": 593,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 7,
    "text_tashkeel": "إِرَمَ ذَاتِ ٱلۡعِمَادِ ٧",
    "text_simple": "إرم ذات العماد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6001,
    "jozz": 30,
    "page": 593,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "ٱلَّتِي لَمۡ يُخۡلَقۡ مِثۡلُهَا فِي ٱلۡبِلَٰدِ ٨",
    "text_simple": "التي لم يخلق مثلها في البلاد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6002,
    "jozz": 30,
    "page": 593,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 9,
    "text_tashkeel": "وَثَمُودَ ٱلَّذِينَ جَابُواْ ٱلصَّخۡرَ بِٱلۡوَادِ ٩",
    "text_simple": "وثمود الذين جابوا الصخر بالواد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6003,
    "jozz": 30,
    "page": 593,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 10,
    "text_tashkeel": "وَفِرۡعَوۡنَ ذِي ٱلۡأَوۡتَادِ ١٠",
    "text_simple": "وفرعون ذي الأوتاد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6004,
    "jozz": 30,
    "page": 593,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 11,
    "text_tashkeel": "ٱلَّذِينَ طَغَوۡاْ فِي ٱلۡبِلَٰدِ ١١",
    "text_simple": "الذين طغوا في البلاد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6005,
    "jozz": 30,
    "page": 593,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 12,
    "text_tashkeel": "فَأَكۡثَرُواْ فِيهَا ٱلۡفَسَادَ ١٢",
    "text_simple": "فأكثروا فيها الفساد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6006,
    "jozz": 30,
    "page": 593,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 13,
    "text_tashkeel": "فَصَبَّ عَلَيۡهِمۡ رَبُّكَ سَوۡطَ عَذَابٍ ١٣",
    "text_simple": "فصب عليهم ربك سوط عذاب",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6007,
    "jozz": 30,
    "page": 593,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 14,
    "text_tashkeel": "إِنَّ رَبَّكَ لَبِٱلۡمِرۡصَادِ ١٤",
    "text_simple": "إن ربك لبالمرصاد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6008,
    "jozz": 30,
    "page": 593,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 15,
    "text_tashkeel": "فَأَمَّا ٱلۡإِنسَٰنُ إِذَا مَا ٱبۡتَلَىٰهُ رَبُّهُۥ فَأَكۡرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّيٓ أَكۡرَمَنِ ١٥",
    "text_simple": "فأما الإنسان إذا ما ابتلاه ربه فأكرمه ونعمه فيقول ربي أكرمن",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6009,
    "jozz": 30,
    "page": 593,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 16,
    "text_tashkeel": "وَأَمَّآ إِذَا مَا ٱبۡتَلَىٰهُ فَقَدَرَ عَلَيۡهِ رِزۡقَهُۥ فَيَقُولُ رَبِّيٓ أَهَٰنَنِ ١٦",
    "text_simple": "وأما إذا ما ابتلاه فقدر عليه رزقه فيقول ربي أهانن",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6010,
    "jozz": 30,
    "page": 593,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 17,
    "text_tashkeel": "كـَلَّاۖ بَل لَّا تُكۡرِمُونَ ٱلۡيَتِيمَ ١٧",
    "text_simple": "كلا بل لا تكرمون اليتيم",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6011,
    "jozz": 30,
    "page": 593,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 18,
    "text_tashkeel": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلۡمِسۡكِينِ ١٨",
    "text_simple": "ولا تحاضون على طعام المسكين",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6012,
    "jozz": 30,
    "page": 593,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 19,
    "text_tashkeel": "وَتَأۡكُلُونَ ٱلتُّرَاثَ أَكۡلٗا لَّمّٗا ١٩",
    "text_simple": "وتأكلون التراث أكلا لما",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6013,
    "jozz": 30,
    "page": 593,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 20,
    "text_tashkeel": "وَتُحِبُّونَ ٱلۡمَالَ حُبّٗا جَمّٗا ٢٠",
    "text_simple": "وتحبون المال حبا جما",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6014,
    "jozz": 30,
    "page": 593,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 21,
    "text_tashkeel": "كـَلَّآۖ إِذَا دُكَّتِ ٱلۡأَرۡضُ دَكّٗا دَكّٗا ٢١",
    "text_simple": "كلا إذا دكت الأرض دكا دكا",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6015,
    "jozz": 30,
    "page": 593,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 22,
    "text_tashkeel": "وَجَآءَ رَبُّكَ وَٱلۡمَلَكُ صَفّٗا صَفّٗا ٢٢",
    "text_simple": "وجاء ربك والملك صفا صفا",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6016,
    "jozz": 30,
    "page": 594,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "وَجِاْيٓءَ يَوۡمَئِذِۭ بِجَهَنَّمَۚ يَوۡمَئِذٖ يَتَذَكَّرُ ٱلۡإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكۡرَىٰ ٢٣",
    "text_simple": "وجيء يومئذ بجهنم يومئذ يتذكر الإنسان وأنى له الذكرى",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6017,
    "jozz": 30,
    "page": 594,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 24,
    "text_tashkeel": "يَقُولُ يَٰلَيۡتَنِي قَدَّمۡتُ لِحَيَاتِي ٢٤",
    "text_simple": "يقول ياليتني قدمت لحياتي",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6018,
    "jozz": 30,
    "page": 594,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": "فَيَوۡمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ ٢٥",
    "text_simple": "فيومئذ لا يعذب عذابه أحد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6019,
    "jozz": 30,
    "page": 594,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 26,
    "text_tashkeel": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞ ٢٦",
    "text_simple": "ولا يوثق وثاقه أحد",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6020,
    "jozz": 30,
    "page": 594,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 27,
    "text_tashkeel": "يَٰٓأَيَّتُهَا ٱلنَّفۡسُ ٱلۡمُطۡمَئِنَّةُ ٢٧",
    "text_simple": "ياأيتها النفس المطمئنة",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6021,
    "jozz": 30,
    "page": 594,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": "ٱرۡجِعِيٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرۡضِيَّةٗ ٢٨",
    "text_simple": "ارجعي إلى ربك راضية مرضية",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6022,
    "jozz": 30,
    "page": 594,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 29,
    "text_tashkeel": "فَٱدۡخُلِي فِي عِبَٰدِي ٢٩",
    "text_simple": "فادخلي في عبادي",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 6023,
    "jozz": 30,
    "page": 594,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 30,
    "text_tashkeel": "وَٱدۡخُلِي جَنَّتِي ٣٠",
    "text_simple": "وادخلي جنتي",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  }
]