    "text_tashkeel": "وَٱلۡفَجۡرِ ١",
    "text_simple": "والفجر",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر"
  },
  {
    "id": 5995,
