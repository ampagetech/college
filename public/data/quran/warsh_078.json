[
  {
    "id": 5649,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "عَمَّۖ يَتَسَآءَلُونَ ١",
    "text_simple": "عم يتسآءلون"
  },
  {
    "id": 5650,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "عَنِ اِ۬لنَّبَإِ اِ۬لْعَظِيمِ ٢",
    "text_simple": "عن النبإ العظيم"
  },
  {
    "id": 5651,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "text_tashkeel": "اِ۬لذِے هُمْ فِيهِ مُخْتَلِفُونَۖ ٣",
    "text_simple": "الذے هم فيه مختلفون"
  },
  {
    "id": 5652,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": " كَلَّا سَيَعْلَمُونَ ٤",
    "text_simple": "كلا سيعلمون"
  },
  {
    "id": 5653,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "ثُمَّ كَلَّا سَيَعْلَمُونَۖ ٥",
    "text_simple": "ثم كلا سيعلمون"
  },
  {
    "id": 5654,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 6,
    "text_tashkeel": "أَلَمْ نَجْعَلِ اِ۬لَارْضَ مِهَٰداٗ ٦",
    "text_simple": "ألم نجعل الارض مهداٗ"
  },
  {
    "id": 5655,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "وَالْجِبَالَ أَوْتَاداٗ ٧",
    "text_simple": "والجبال أوتاداٗ"
  },
  {
    "id": 5656,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 8,
    "text_tashkeel": "وَخَلَقْنَٰكُمُۥٓ أَزْوَٰجاٗ ٨",
    "text_simple": "وخلقنكمٓ أزوجاٗ"
  },
  {
    "id": 5657,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 9,
    "text_tashkeel": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتاٗ ٩",
    "text_simple": "وجعلنا نومكم سباتاٗ"
  },
  {
    "id": 5658,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 10,
    "text_tashkeel": "وَجَعَلْنَا اَ۬ليْلَ لِبَاساٗ ١٠",
    "text_simple": "وجعلنا اليل لباساٗ"
  },
  {
    "id": 5659,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 11,
    "text_tashkeel": "وَجَعَلْنَا اَ۬لنَّهَارَ مَعَاشاٗ ١١",
    "text_simple": "وجعلنا النهار معاشاٗ"
  },
  {
    "id": 5660,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 12,
    "text_tashkeel": " وَبَنَيْنَا فَوْقَكُمْ سَبْعاٗ شِدَاداٗ ١٢",
    "text_simple": "وبنينا فوقكم سبعاٗ شداداٗ"
  },
  {
    "id": 5661,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 13,
    "text_tashkeel": "وَجَعَلْنَا سِرَاجاٗ وَهَّاجاٗ ١٣",
    "text_simple": "وجعلنا سراجاٗ وهاجاٗ"
  },
  {
    "id": 5662,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 14,
    "text_tashkeel": " وَأَنزَلْنَا مِنَ اَ۬لْمُعْصِرَٰتِ مَآءٗ ثَجَّاجاٗ ١٤",
    "text_simple": "وأنزلنا من المعصرت مآءٗ ثجاجاٗ"
  },
  {
    "id": 5663,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 15,
    "text_tashkeel": "لِّنُخْرِجَ بِهِۦ حَبّاٗ وَنَبَاتاٗ ١٥",
    "text_simple": "لنخرج به حباٗ ونباتاٗ"
  },
  {
    "id": 5664,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 16,
    "text_tashkeel": " وَجَنَّٰتٍ اَلْفَافاًۖ ١٦",
    "text_simple": "وجنت الفافا"
  },
  {
    "id": 5665,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 17,
    "text_tashkeel": "اِنَّ يَوْمَ اَ۬لْفَصْلِ كَانَ مِيقَٰتاٗ ١٧",
    "text_simple": "ان يوم الفصل كان ميقتاٗ"
  },
  {
    "id": 5666,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 18,
    "text_tashkeel": "يَوْمَ يُنفَخُ فِے اِ۬لصُّورِ فَتَاتُونَ أَفْوَاجاٗ ١٨",
    "text_simple": "يوم ينفخ فے الصور فتاتون أفواجاٗ"
  },
  {
    "id": 5667,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 19,
    "text_tashkeel": "وَفُتِّحَتِ اِ۬لسَّمَآءُ فَكَانَتَ اَبْوَٰباٗ ١٩",
    "text_simple": "وفتحت السمآء فكانت ابوباٗ"
  },
  {
    "id": 5668,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 20,
    "text_tashkeel": " وَسُيِّرَتِ اِ۬لْجِبَالُ فَكَانَتْ سَرَاباًۖ ٢٠",
    "text_simple": "وسيرت الجبال فكانت سرابا"
  },
  {
    "id": 5669,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 21,
    "text_tashkeel": "اِنَّ جَهَنَّمَ كَانَتْ مِرْصَاداٗ ٢١",
    "text_simple": "ان جهنم كانت مرصاداٗ"
  },
  {
    "id": 5670,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 22,
    "text_tashkeel": " لِّلطَّٰغِينَ مَـَٔاباٗ ٢٢",
    "text_simple": "للطغين مٔاباٗ"
  },
  {
    "id": 5671,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 23,
    "text_tashkeel": "لَّٰبِثِينَ فِيهَآ أَحْقَاباٗۖ ٢٣",
    "text_simple": "لبثين فيهآ أحقاباٗ"
  },
  {
    "id": 5672,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 24,
    "text_tashkeel": "لَّا يَذُوقُونَ فِيهَا بَرْداٗ وَلَا شَرَاباً ٢٤",
    "text_simple": "لا يذوقون فيها برداٗ ولا شرابا"
  },
  {
    "id": 5673,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 25,
    "text_tashkeel": "اِلَّا حَمِيماٗ وَغَسَاقاٗ ٢٥",
    "text_simple": "الا حميماٗ وغساقاٗ"
  },
  {
    "id": 5674,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 26,
    "text_tashkeel": "جَزَآءٗ وِفَاقاًۖ ٢٦",
    "text_simple": "جزآءٗ وفاقا"
  },
  {
    "id": 5675,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 27,
    "text_tashkeel": "اِنَّهُمْ كَانُواْ لَا يَرْجُونَ حِسَاباٗ ٢٧",
    "text_simple": "انهم كانوا لا يرجون حساباٗ"
  },
  {
    "id": 5676,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 28,
    "text_tashkeel": "وَكَذَّبُواْ بِـَٔايَٰتِنَا كِذَّاباٗۖ ٢٨",
    "text_simple": "وكذبوا بٔايتنا كذاباٗ"
  },
  {
    "id": 5677,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 29,
    "text_tashkeel": "وَكُلَّ شَےْءٍ اَحْصَيْنَٰهُ كِتَٰباٗۖ ٢٩",
    "text_simple": "وكل شےء احصينه كتباٗ"
  },
  {
    "id": 5678,
    "jozz": 30,
    "page": "582",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 30,
    "text_tashkeel": "فَذُوقُواْ فَلَن نَّزِيدَكُمُۥٓ إِلَّا عَذَاباًۖ ٣٠",
    "text_simple": "فذوقوا فلن نزيدكمٓ إلا عذابا"
  },
  {
    "id": 5679,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 31,
    "text_tashkeel": " اِنَّ لِلْمُتَّقِينَ مَفَازاً ٣١",
    "text_simple": "ان للمتقين مفازا"
  },
  {
    "id": 5680,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 32,
    "text_tashkeel": "حَدَآئِقَ وَأَعْنَٰباٗ ٣٢",
    "text_simple": "حدآئق وأعنباٗ"
  },
  {
    "id": 5681,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 33,
    "text_tashkeel": "وَكَوَاعِبَ أَتْرَاباٗ ٣٣",
    "text_simple": "وكواعب أتراباٗ"
  },
  {
    "id": 5682,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 34,
    "text_tashkeel": "وَكَأْساٗ دِهَاقاٗ ٣٤",
    "text_simple": "وكأساٗ دهاقاٗ"
  },
  {
    "id": 5683,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 35,
    "text_tashkeel": "لَّا يَسْمَعُونَ فِيهَا لَغْواٗ وَلَا كِذَّاباٗۖ ٣٥",
    "text_simple": "لا يسمعون فيها لغواٗ ولا كذاباٗ"
  },
  {
    "id": 5684,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 36,
    "text_tashkeel": "جَزَآءٗ مِّن رَّبِّكَ عَطَآءً حِسَاباٗۖ ٣٦",
    "text_simple": "جزآءٗ من ربك عطآء حساباٗ"
  },
  {
    "id": 5685,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 37,
    "text_tashkeel": "رَّبُّ اُ۬لسَّمَٰوَٰتِ وَالَارْضِ وَمَا بَيْنَهُمَاۖ اَ۬لرَّحْمَٰنُ لَا يَمْلِكُونَ مِنْهُ خِطَاباٗۖ ٣٧",
    "text_simple": "رب السموت والارض وما بينهما الرحمن لا يملكون منه خطاباٗ"
  },
  {
    "id": 5686,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 38,
    "text_tashkeel": "يَوْمَ يَقُومُ اُ۬لرُّوحُ وَالْمَلَٰٓئِكَةُ صَفّاٗ لَّا يَتَكَلَّمُونَ إِلَّا مَنَ اَذِنَ لَهُ اُ۬لرَّحْمَٰنُ وَقَالَ صَوَاباٗۖ ٣٨",
    "text_simple": "يوم يقوم الروح والملٓئكة صفاٗ لا يتكلمون إلا من اذن له الرحمن وقال صواباٗ"
  },
  {
    "id": 5687,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 39,
    "text_tashkeel": "ذَٰلِكَ اَ۬لْيَوْمُ اُ۬لْحَقُّۖ فَمَن شَآءَ اَ۪تَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔاباًۖ ٣٩",
    "text_simple": "ذلك اليوم الحق فمن شآء اتخذ إلى ربه مٔابا"
  },
  {
    "id": 5688,
    "jozz": 30,
    "page": "583",
    "sura_no": 78,
    "sura_name_en": "An-Naba’",
    "sura_name_ar": "النَّبَإ ",
