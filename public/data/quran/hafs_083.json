[
  {
    "id": 5849,
    "jozz": 30,
    "page": 587,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 1,
    "text_tashkeel": "وَيۡلٞ لِّلۡمُطَفِّفِينَ ١",
    "text_simple": "ويل للمطففين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5850,
    "jozz": 30,
    "page": 587,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 2,
    "text_tashkeel": "ٱلَّذِينَ إِذَا ٱكۡتَالُواْ عَلَى ٱلنَّاسِ يَسۡتَوۡفُونَ ٢",
    "text_simple": "الذين إذا اكتالوا على الناس يستوفون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5851,
    "jozz": 30,
    "page": 587,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا كَالُوهُمۡ أَو وَّزَنُوهُمۡ يُخۡسِرُونَ ٣",
    "text_simple": "وإذا كالوهم أو وزنوهم يخسرون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5852,
    "jozz": 30,
    "page": 587,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 4,
    "text_tashkeel": "أَلَا يَظُنُّ أُوْلَٰٓئِكَ أَنَّهُم مَّبۡعُوثُونَ ٤",
    "text_simple": "ألا يظن أولئك أنهم مبعوثون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5853,
    "jozz": 30,
    "page": 588,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 5,
    "text_tashkeel": "لِيَوۡمٍ عَظِيمٖ ٥",
    "text_simple": "ليوم عظيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5854,
    "jozz": 30,
    "page": 588,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 6,
    "text_tashkeel": "يَوۡمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلۡعَٰلَمِينَ ٦",
    "text_simple": "يوم يقوم الناس لرب العالمين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5855,
    "jozz": 30,
    "page": 588,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 7,
    "text_tashkeel": "كـَلَّآ إِنَّ كِتَٰبَ ٱلۡفُجَّارِ لَفِي سِجِّينٖ ٧",
    "text_simple": "كلا إن كتاب الفجار لفي سجين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5856,
    "jozz": 30,
    "page": 588,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 8,
    "text_tashkeel": "وَمَآ أَدۡرَىٰكَ مَا سِجِّينٞ ٨",
    "text_simple": "وما أدراك ما سجين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5857,
    "jozz": 30,
    "page": 588,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 9,
    "text_tashkeel": "كِتَٰبٞ مَّرۡقُومٞ ٩",
    "text_simple": "كتاب مرقوم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5858,
    "jozz": 30,
    "page": 588,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 10,
    "text_tashkeel": "وَيۡلٞ يَوۡمَئِذٖ لِّلۡمُكَذِّبِينَ ١٠",
    "text_simple": "ويل يومئذ للمكذبين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5859,
    "jozz": 30,
    "page": 588,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 11,
    "text_tashkeel": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوۡمِ ٱلدِّينِ ١١",
    "text_simple": "الذين يكذبون بيوم الدين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5860,
    "jozz": 30,
    "page": 588,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 12,
    "text_tashkeel": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعۡتَدٍ أَثِيمٍ ١٢",
    "text_simple": "وما يكذب به إلا كل معتد أثيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5861,
    "jozz": 30,
    "page": 588,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 13,
    "text_tashkeel": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ ١٣",
    "text_simple": "إذا تتلى عليه آياتنا قال أساطير الأولين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5862,
    "jozz": 30,
    "page": 588,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 14,
    "text_tashkeel": "كـَلَّاۖ بَلۡۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُواْ يَكۡسِبُونَ ١٤",
    "text_simple": "كلا بل ران على قلوبهم ما كانوا يكسبون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5863,
    "jozz": 30,
    "page": 588,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 15,
    "text_tashkeel": "كـَلَّآ إِنَّهُمۡ عَن رَّبِّهِمۡ يَوۡمَئِذٖ لَّمَحۡجُوبُونَ ١٥",
    "text_simple": "كلا إنهم عن ربهم يومئذ لمحجوبون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5864,
    "jozz": 30,
    "page": 588,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 16,
    "text_tashkeel": "ثُمَّ إِنَّهُمۡ لَصَالُواْ ٱلۡجَحِيمِ ١٦",
    "text_simple": "ثم إنهم لصالوا الجحيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5865,
    "jozz": 30,
    "page": 588,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 17,
    "text_tashkeel": "ثُمَّ يُقَالُ هَٰذَا ٱلَّذِي كُنتُم بِهِۦ تُكَذِّبُونَ ١٧",
    "text_simple": "ثم يقال هذا الذي كنتم به تكذبون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5866,
    "jozz": 30,
    "page": 588,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 18,
    "text_tashkeel": "كـَلَّآ إِنَّ كِتَٰبَ ٱلۡأَبۡرَارِ لَفِي عِلِّيِّينَ ١٨",
    "text_simple": "كلا إن كتاب الأبرار لفي عليين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5867,
    "jozz": 30,
    "page": 588,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 19,
    "text_tashkeel": "وَمَآ أَدۡرَىٰكَ مَا عِلِّيُّونَ ١٩",
    "text_simple": "وما أدراك ما عليون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5868,
    "jozz": 30,
    "page": 588,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 20,
    "text_tashkeel": "كِتَٰبٞ مَّرۡقُومٞ ٢٠",
    "text_simple": "كتاب مرقوم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5869,
    "jozz": 30,
    "page": 588,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 21,
    "text_tashkeel": "يَشۡهَدُهُ ٱلۡمُقَرَّبُونَ ٢١",
    "text_simple": "يشهده المقربون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5870,
    "jozz": 30,
    "page": 588,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 22,
    "text_tashkeel": "إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٍ ٢٢",
    "text_simple": "إن الأبرار لفي نعيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5871,
    "jozz": 30,
    "page": 588,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 23,
    "text_tashkeel": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ ٢٣",
    "text_simple": "على الأرائك ينظرون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5872,
    "jozz": 30,
    "page": 588,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 24,
    "text_tashkeel": "تَعۡرِفُ فِي وُجُوهِهِمۡ نَضۡرَةَ ٱلنَّعِيمِ ٢٤",
    "text_simple": "تعرف في وجوههم نضرة النعيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5873,
    "jozz": 30,
    "page": 588,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 25,
    "text_tashkeel": "يُسۡقَوۡنَ مِن رَّحِيقٖ مَّخۡتُومٍ ٢٥",
    "text_simple": "يسقون من رحيق مختوم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5874,
    "jozz": 30,
    "page": 588,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 26,
    "text_tashkeel": "خِتَٰمُهُۥ مِسۡكٞۚ وَفِي ذَٰلِكَ فَلۡيَتَنَافَسِ ٱلۡمُتَنَٰفِسُونَ ٢٦",
    "text_simple": "ختامه مسك وفي ذلك فليتنافس المتنافسون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5875,
    "jozz": 30,
    "page": 588,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 27,
    "text_tashkeel": "وَمِزَاجُهُۥ مِن تَسۡنِيمٍ ٢٧",
    "text_simple": "ومزاجه من تسنيم",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5876,
    "jozz": 30,
    "page": 588,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 28,
    "text_tashkeel": "عَيۡنٗا يَشۡرَبُ بِهَا ٱلۡمُقَرَّبُونَ ٢٨",
    "text_simple": "عينا يشرب بها المقربون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5877,
    "jozz": 30,
    "page": 588,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 29,
    "text_tashkeel": "إِنَّ ٱلَّذِينَ أَجۡرَمُواْ كَانُواْ مِنَ ٱلَّذِينَ ءَامَنُواْ يَضۡحَكُونَ ٢٩",
    "text_simple": "إن الذين أجرموا كانوا من الذين آمنوا يضحكون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5878,
    "jozz": 30,
    "page": 588,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 30,
    "text_tashkeel": "وَإِذَا مَرُّواْ بِهِمۡ يَتَغَامَزُونَ ٣٠",
    "text_simple": "وإذا مروا بهم يتغامزون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5879,
    "jozz": 30,
    "page": 588,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 31,
    "text_tashkeel": "وَإِذَا ٱنقَلَبُوٓاْ إِلَىٰٓ أَهۡلِهِمُ ٱنقَلَبُواْ فَكِهِينَ ٣١",
    "text_simple": "وإذا انقلبوا إلى أهلهم انقلبوا فكهين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5880,
    "jozz": 30,
    "page": 588,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 32,
    "text_tashkeel": "وَإِذَا رَأَوۡهُمۡ قَالُوٓاْ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ ٣٢",
    "text_simple": "وإذا رأوهم قالوا إن هؤلاء لضالون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5881,
    "jozz": 30,
    "page": 588,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 33,
    "text_tashkeel": "وَمَآ أُرۡسِلُواْ عَلَيۡهِمۡ حَٰفِظِينَ ٣٣",
    "text_simple": "وما أرسلوا عليهم حافظين",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5882,
    "jozz": 30,
    "page": 589,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 34,
    "text_tashkeel": "فَٱلۡيَوۡمَ ٱلَّذِينَ ءَامَنُواْ مِنَ ٱلۡكُفَّارِ يَضۡحَكُونَ ٣٤",
    "text_simple": "فاليوم الذين آمنوا من الكفار يضحكون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5883,
    "jozz": 30,
    "page": 589,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 35,
    "text_tashkeel": "عَلَى ٱلۡأَرَآئِكِ يَنظُرُونَ ٣٥",
    "text_simple": "على الأرائك ينظرون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  },
  {
    "id": 5884,
    "jozz": 30,
    "page": 589,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 36,
    "text_tashkeel": "هَلۡ ثُوِّبَ ٱلۡكُفَّارُ مَا كَانُواْ يَفۡعَلُونَ ٣٦",
    "text_simple": "هل ثوب الكفار ما كانوا يفعلون",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين"
  }
]