    "jozz": 30,
    "page": 586,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "وَإِذَا ٱلۡمَوۡءُۥدَةُ سُئِلَتۡ ٨",
    "text_simple": "وإذا الموءودة سئلت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
