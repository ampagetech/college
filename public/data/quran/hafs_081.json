    "jozz": 30,
    "page": 586,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "text_tashkeel": "وَإِذَا ٱلۡوُحُوشُ حُشِرَتۡ ٥",
    "text_simple": "وإذا الوحوش حشرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
