    "line_end": 5,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا ٱلۡجِبَالُ سُيِّرَتۡ ٣",
    "text_simple": "وإذا الجبال سيرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
