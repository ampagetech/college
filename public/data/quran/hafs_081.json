    "jozz": 30,
    "page": 586,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 10,
    "text_tashkeel": "وَإِذَا ٱلصُّحُفُ نُشِرَتۡ ١٠",
    "text_simple": "وإذا الصحف نشرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
