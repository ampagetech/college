    "jozz": 30,
    "page": 586,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": "وَإِذَا ٱلۡعِشَارُ عُطِّلَتۡ ٤",
    "text_simple": "وإذا العشار عطلت",
    "sura_no": 81,
