    "jozz": 30,
    "page": 586,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 11,
    "text_tashkeel": "وَإِذَا ٱلسَّمَآءُ كُشِطَتۡ ١١",
    "text_simple": "وإذا السماء كشطت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
