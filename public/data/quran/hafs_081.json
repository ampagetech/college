    "jozz": 30,
    "page": 586,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتۡ ٧",
    "text_simple": "وإذا النفوس زوجت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
