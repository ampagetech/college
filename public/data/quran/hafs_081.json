    "jozz": 30,
    "page": 586,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "وَإِذَا ٱلۡبِحَارُ سُجِّرَتۡ ٦",
    "text_simple": "وإذا البحار سجرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
