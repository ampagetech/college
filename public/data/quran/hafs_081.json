    "jozz": 30,
    "page": 586,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 12,
    "text_tashkeel": "وَإِذَا ٱلۡجَحِيمُ سُعِّرَتۡ ١٢",
    "text_simple": "وإذا الجحيم سعرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5813,
    "jozz": 30,
    "page": 586,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 13,
    "text_tashkeel": "وَإِذَا ٱلۡجَنَّةُ أُزۡلِفَتۡ ١٣",
    "text_simple": "وإذا الجنة أزلفت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5814,
    "jozz": 30,
    "page": 586,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 14,
    "text_tashkeel": "عَلِمَتۡ نَفۡسٞ مَّآ أَحۡضَرَتۡ ١٤",
    "text_simple": "علمت نفس ما أحضرت",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5815,
    "jozz": 30,
    "page": 586,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 15,
    "text_tashkeel": "فَلَآ أُقۡسِمُ بِٱلۡخُنَّسِ ١٥",
    "text_simple": "فلا أقسم بالخنس",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5816,
    "jozz": 30,
    "page": 586,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 16,
    "text_tashkeel": "ٱلۡجَوَارِ ٱلۡكُنَّسِ ١٦",
    "text_simple": "الجوار الكنس",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5817,
    "jozz": 30,
    "page": 586,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 17,
    "text_tashkeel": "وَٱلَّيۡلِ إِذَا عَسۡعَسَ ١٧",
    "text_simple": "والليل إذا عسعس",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5818,
    "jozz": 30,
    "page": 586,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 18,
    "text_tashkeel": "وَٱلصُّبۡحِ إِذَا تَنَفَّسَ ١٨",
    "text_simple": "والصبح إذا تنفس",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5819,
    "jozz": 30,
    "page": 586,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 19,
    "text_tashkeel": "إِنَّهُۥ لَقَوۡلُ رَسُولٖ كَرِيمٖ ١٩",
    "text_simple": "إنه لقول رسول كريم",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5820,
    "jozz": 30,
    "page": 586,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 20,
    "text_tashkeel": "ذِي قُوَّةٍ عِندَ ذِي ٱلۡعَرۡشِ مَكِينٖ ٢٠",
    "text_simple": "ذي قوة عند ذي العرش مكين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5821,
    "jozz": 30,
    "page": 586,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 21,
    "text_tashkeel": "مُّطَاعٖ ثَمَّ أَمِينٖ ٢١",
    "text_simple": "مطاع ثم أمين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5822,
    "jozz": 30,
    "page": 586,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 22,
    "text_tashkeel": "وَمَا صَاحِبُكُم بِمَجۡنُونٖ ٢٢",
    "text_simple": "وما صاحبكم بمجنون",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5823,
    "jozz": 30,
    "page": 586,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 23,
    "text_tashkeel": "وَلَقَدۡ رَءَاهُ بِٱلۡأُفُقِ ٱلۡمُبِينِ ٢٣",
    "text_simple": "ولقد رآه بالأفق المبين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5824,
    "jozz": 30,
    "page": 586,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 24,
    "text_tashkeel": "وَمَا هُوَ عَلَى ٱلۡغَيۡبِ بِضَنِينٖ ٢٤",
    "text_simple": "وما هو على الغيب بضنين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5825,
    "jozz": 30,
    "page": 586,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 25,
    "text_tashkeel": "وَمَا هُوَ بِقَوۡلِ شَيۡطَٰنٖ رَّجِيمٖ ٢٥",
    "text_simple": "وما هو بقول شيطان رجيم",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5826,
    "jozz": 30,
    "page": 586,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 26,
    "text_tashkeel": "فَأَيۡنَ تَذۡهَبُونَ ٢٦",
    "text_simple": "فأين تذهبون",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5827,
    "jozz": 30,
    "page": 586,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 27,
    "text_tashkeel": "إِنۡ هُوَ إِلَّا ذِكۡرٞ لِّلۡعَٰلَمِينَ ٢٧",
    "text_simple": "إن هو إلا ذكر للعالمين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5828,
    "jozz": 30,
    "page": 586,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 28,
    "text_tashkeel": "لِمَن شَآءَ مِنكُمۡ أَن يَسۡتَقِيمَ ٢٨",
    "text_simple": "لمن شاء منكم أن يستقيم",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  },
  {
    "id": 5829,
    "jozz": 30,
    "page": 586,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 29,
    "text_tashkeel": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلۡعَٰلَمِينَ ٢٩",
    "text_simple": "وما تشاءون إلا أن يشاء الله رب العالمين",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير"
  }
