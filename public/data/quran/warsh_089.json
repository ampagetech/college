[
  {
    "id": 5969,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 1,
    "text_tashkeel": "وَالْفَجْرِ ١",
    "text_simple": "والفجر"
  },
  {
    "id": 5970,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 2,
    "text_tashkeel": "وَلَيَالٍ عَشْرٖ ٢",
    "text_simple": "وليال عشرٖ"
  },
  {
    "id": 5971,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 3,
    "text_tashkeel": "وَالشَّفْعِ وَالْوَتْرِ ٣",
    "text_simple": "والشفع والوتر"
  },
  {
    "id": 5972,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": "وَاليْلِ إِذَا يَسْرِۦ ٤",
    "text_simple": "واليل إذا يسر"
  },
  {
    "id": 5973,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 5,
    "text_tashkeel": "هَلْ فِے ذَٰلِكَ قَسَمٞ لِّذِے حِجْرٍۖ ٥",
    "text_simple": "هل فے ذلك قسمٞ لذے حجر"
  },
  {
    "id": 5974,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ ٦",
    "text_simple": "الم تر كيف فعل ربك بعاد"
  },
  {
    "id": 5975,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 7,
    "text_tashkeel": " اِرَمَ ذَاتِ اِ۬لْعِمَادِ ٧",
    "text_simple": "ارم ذات العماد"
  },
  {
    "id": 5976,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "اِ۬لتِے لَمْ يُخْلَقْ مِثْلُهَا فِے اِ۬لْبِلَٰدِ ٨",
    "text_simple": "التے لم يخلق مثلها فے البلد"
  },
  {
    "id": 5977,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 9,
    "text_tashkeel": "وَثَمُودَ اَ۬لذِينَ جَابُواْ اُ۬لصَّخْرَ بِالْوَادِۦ ٩",
    "text_simple": "وثمود الذين جابوا الصخر بالواد"
  },
  {
    "id": 5978,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 10,
    "text_tashkeel": "وَفِرْعَوْنَ ذِے اِ۬لَاوْتَادِ ١٠",
    "text_simple": "وفرعون ذے الاوتاد"
  },
  {
    "id": 5979,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 11,
    "text_tashkeel": "اِ۬لذِينَ طَغَوْاْ فِے اِ۬لْبِلَٰدِ ١١",
    "text_simple": "الذين طغوا فے البلد"
  },
  {
    "id": 5980,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 12,
    "text_tashkeel": "فَأَكْثَرُواْ فِيهَا اَ۬لْفَسَادَ ١٢",
    "text_simple": "فأكثروا فيها الفساد"
  },
  {
    "id": 5981,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 13,
    "text_tashkeel": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍۖ ١٣",
    "text_simple": "فصب عليهم ربك سوط عذاب"
  },
  {
    "id": 5982,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 14,
    "text_tashkeel": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِۖ ١٤",
    "text_simple": "ان ربك لبالمرصاد"
  },
  {
    "id": 5983,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 15,
    "text_tashkeel": "فَأَمَّا اَ۬لِانسَٰنُ إِذَا مَا اَ۪بْتَل۪يٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ ١٥",
    "text_simple": "فأما الانسن إذا ما ابتليه ربه فأكرمه ونعمه"
  },
  {
    "id": 5984,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 16,
    "text_tashkeel": "فَيَقُولُ رَبِّيَ أَكْرَمَنِۦۖ ١٦",
    "text_simple": "فيقول ربي أكرمن"
  },
  {
    "id": 5985,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 17,
    "text_tashkeel": "وَأَمَّآ إِذَا مَا اَ۪بْتَل۪يٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ ١٧",
    "text_simple": "وأمآ إذا ما ابتليه فقدر عليه رزقه"
  },
  {
    "id": 5986,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 18,
    "text_tashkeel": "فَيَقُولُ رَبِّيَ أَهَٰنَنِۦ ١٨",
    "text_simple": "فيقول ربي أهنن"
  },
  {
    "id": 5987,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 19,
    "text_tashkeel": "كَلَّاۖ بَل لَّا تُكْرِمُونَ اَ۬لْيَتِيمَ ١٩",
    "text_simple": "كلا بل لا تكرمون اليتيم"
  },
  {
    "id": 5988,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 20,
    "text_tashkeel": "وَلَا تَحُضُّونَ عَلَىٰ طَعَامِ اِ۬لْمِسْكِينِ ٢٠",
    "text_simple": "ولا تحضون على طعام المسكين"
  },
  {
    "id": 5989,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 21,
    "text_tashkeel": "وَتَاكُلُونَ اَ۬لتُّرَاثَ أَكْلاٗ لَّمّاٗ ٢١",
    "text_simple": "وتاكلون التراث أكلاٗ لماٗ"
  },
  {
    "id": 5990,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 22,
    "text_tashkeel": "وَتُحِبُّونَ اَ۬لْمَالَ حُبّاٗ جَمّاٗ ٢٢",
    "text_simple": "وتحبون المال حباٗ جماٗ"
  },
  {
    "id": 5991,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 23,
    "text_tashkeel": "كَلَّآۖ إِذَا دُكَّتِ اِ۬لَارْضُ دَكّاٗ دَكّاٗ ٢٣",
    "text_simple": "كلآ إذا دكت الارض دكاٗ دكاٗ"
  },
  {
    "id": 5992,
    "jozz": 30,
    "page": "593",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 24,
    "text_tashkeel": "وَجَآءَ رَبُّكَ وَالْمَلَكُ صَفّاٗ صَفّاٗ ٢٤",
    "text_simple": "وجآء ربك والملك صفاٗ صفاٗ"
  },
  {
    "id": 5993,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 25,
    "text_tashkeel": " وَجِےٓءَ يَوْمَئِذِۢ بِجَهَنَّمَ ٢٥",
    "text_simple": "وجےٓء يومئذ بجهنم"
  },
  {
    "id": 5994,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 26,
    "text_tashkeel": "يَوْمَئِذٖ يَتَذَكَّرُ اُ۬لِانسَٰنُ وَأَنّ۪ىٰ لَهُ اُ۬لذِّكْر۪ىٰۖ ٢٦",
    "text_simple": "يومئذٖ يتذكر الانسن وأنى له الذكرى"
  },
  {
    "id": 5995,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 27,
    "text_tashkeel": "يَقُولُ يَٰلَيْتَنِے قَدَّمْتُ لِحَيَاتِےۖ ٢٧",
    "text_simple": "يقول يليتنے قدمت لحياتے"
  },
  {
    "id": 5996,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 28,
    "text_tashkeel": "فَيَوْمَئِذٖ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٞ ٢٨",
    "text_simple": "فيومئذٖ لا يعذب عذابهٓ أحدٞ"
  },
  {
    "id": 5997,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 29,
    "text_tashkeel": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٞۖ ٢٩",
    "text_simple": "ولا يوثق وثاقهٓ أحدٞ"
  },
  {
    "id": 5998,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 30,
    "text_tashkeel": "يَٰٓأَيَّتُهَا اَ۬لنَّفْسُ اُ۬لْمُطْمَئِنَّةُ ٣٠",
    "text_simple": "يٓأيتها النفس المطمئنة"
  },
  {
    "id": 5999,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 31,
    "text_tashkeel": "اُ۪رْجِعِےٓ إِلَىٰ رَبِّكِ رَاضِيَةٗ مَّرْضِيَّةٗ ٣١",
    "text_simple": "ارجعےٓ إلى ربك راضيةٗ مرضيةٗ"
  },
  {
    "id": 6000,
    "jozz": 30,
    "page": "594",
    "sura_no": 89,
    "sura_name_en": "Al-Fajr",
    "sura_name_ar": "الفَجر ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 32,
    "text_tashkeel": " فَادْخُلِے فِے عِبَٰدِے وَادْخُلِے جَنَّتِےۖ ٣٢",
    "text_simple": "فادخلے فے عبدے وادخلے جنتے"
  }
]