[
  {
    "id": 6084,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 1,
    "text_tashkeel": "اَ۪قْرَأْ بِاسْمِ رَبِّكَ اَ۬لذِے خَلَقَۖ ١",
    "text_simple": "اقرأ باسم ربك الذے خلق"
  },
  {
    "id": 6085,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 2,
    "text_tashkeel": "خَلَقَ اَ۬لِانسَٰنَ مِنْ عَلَقٍۖ ٢",
    "text_simple": "خلق الانسن من علق"
  },
  {
    "id": 6086,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 3,
    "text_tashkeel": "اِ۪قْرَأْ وَرَبُّكَ اَ۬لَاكْرَمُ ٣",
    "text_simple": "اقرأ وربك الاكرم"
  },
  {
    "id": 6087,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 4,
    "text_tashkeel": "اُ۬لذِے عَلَّمَ بِالْقَلَمِ ٤",
    "text_simple": "الذے علم بالقلم"
  },
  {
    "id": 6088,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 5,
    "text_tashkeel": "عَلَّمَ اَ۬لِانسَٰنَ مَا لَمْ يَعْلَمْۖ ٥",
    "text_simple": "علم الانسن ما لم يعلم"
  },
  {
    "id": 6089,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 6,
    "text_tashkeel": "كَلَّآ إِنَّ اَ۬لِانسَٰنَ لَيَطْغ۪ىٰٓ ٦",
    "text_simple": "كلآ إن الانسن ليطغىٓ"
  },
  {
    "id": 6090,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 7,
    "text_tashkeel": "أَن رّ۪ء۪اهُ اُ۪سْتَغْن۪ىٰٓۖ ٧",
    "text_simple": "أن رءاه استغنىٓ"
  },
  {
    "id": 6091,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 8,
    "text_tashkeel": " إِنَّ إِلَىٰ رَبِّكَ اَ۬لرُّجْع۪ىٰٓۖ ٨",
    "text_simple": "إن إلى ربك الرجعىٓ"
  },
  {
    "id": 6092,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 9,
    "text_tashkeel": "أَرَٰٓيْتَ اَ۬لذِے يَنْه۪ىٰ ٩",
    "text_simple": "أرٓيت الذے ينهى"
  },
  {
    "id": 6093,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 10,
    "text_tashkeel": "عَبْداً اِذَا صَلّ۪ىٰٓۖ ١٠",
    "text_simple": "عبدا اذا صلىٓ"
  },
  {
    "id": 6094,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 11,
    "text_tashkeel": "أَرَٰٓيْتَ إِن كَانَ عَلَى اَ۬لْهُد۪ىٰٓ ١١",
    "text_simple": "أرٓيت إن كان على الهدىٓ"
  },
  {
    "id": 6095,
    "jozz": 30,
    "page": "597",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 12,
    "text_tashkeel": "أَوَ اَمَرَ بِالتَّقْو۪ىٰٓۖ ١٢",
    "text_simple": "أو امر بالتقوىٓ"
  },
  {
    "id": 6096,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 13,
    "text_tashkeel": " أَرَٰٓيْتَ إِن كَذَّبَ وَتَوَلّ۪ىٰٓ ١٣",
    "text_simple": "أرٓيت إن كذب وتولىٓ"
  },
  {
    "id": 6097,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 14,
    "text_tashkeel": "أَلَمْ يَعْلَم بِأَنَّ اَ۬للَّهَ يَر۪ىٰۖ ١٤",
    "text_simple": "ألم يعلم بأن الله يرى"
  },
  {
    "id": 6098,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 15,
    "text_tashkeel": "كَلَّا لَئِن لَّمْ يَنتَهِ ١٥",
    "text_simple": "كلا لئن لم ينته"
  },
  {
    "id": 6099,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 16,
    "text_tashkeel": "لَنَسْفَعاَۢ بِالنَّاصِيَةِ ١٦",
    "text_simple": "لنسفعا بالناصية"
  },
  {
    "id": 6100,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 17,
    "text_tashkeel": "نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖۖ ١٧",
    "text_simple": "ناصيةٖ كذبة خاطئةٖ"
  },
  {
    "id": 6101,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 18,
    "text_tashkeel": "فَلْيَدْعُ نَادِيَهُۥ ١٨",
    "text_simple": "فليدع ناديه"
  },
  {
    "id": 6102,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 19,
    "text_tashkeel": "سَنَدْعُ اُ۬لزَّبَانِيَةَۖ ١٩",
    "text_simple": "سندع الزبانية"
  },
  {
    "id": 6103,
    "jozz": 30,
    "page": "598",
    "sura_no": 96,
    "sura_name_en": "Al-‘Alaq",
    "sura_name_ar": "العَلَق ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 20,
    "text_tashkeel": "كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبِۖ ٢٠",
    "text_simple": "كلا لا تطعه واسجد واقترب"
  }
]