[
  {
    "id": 5552,
    "jozz": 29,
    "page": 577,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 1,
    "text_tashkeel": "لَآ أُقۡسِمُ بِيَوۡمِ ٱلۡقِيَٰمَةِ ١",
    "text_simple": "لا أقسم بيوم القيامة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5553,
    "jozz": 29,
    "page": 577,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "وَلَآ أُقۡسِمُ بِٱلنَّفۡسِ ٱللَّوَّامَةِ ٢",
    "text_simple": "ولا أقسم بالنفس اللوامة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5554,
    "jozz": 29,
    "page": 577,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَلَّن نَّجۡمَعَ عِظَامَهُۥ ٣",
    "text_simple": "أيحسب الإنسان ألن نجمع عظامه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5555,
    "jozz": 29,
    "page": 577,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥ ٤",
    "text_simple": "بلى قادرين على أن نسوي بنانه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5556,
    "jozz": 29,
    "page": 577,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "بَلۡ يُرِيدُ ٱلۡإِنسَٰنُ لِيَفۡجُرَ أَمَامَهُۥ ٥",
    "text_simple": "بل يريد الإنسان ليفجر أمامه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5557,
    "jozz": 29,
    "page": 577,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "يَسۡـَٔلُ أَيَّانَ يَوۡمُ ٱلۡقِيَٰمَةِ ٦",
    "text_simple": "يسأل أيان يوم القيامة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5558,
    "jozz": 29,
    "page": 577,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "فَإِذَا بَرِقَ ٱلۡبَصَرُ ٧",
    "text_simple": "فإذا برق البصر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5559,
    "jozz": 29,
    "page": 577,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "وَخَسَفَ ٱلۡقَمَرُ ٨",
    "text_simple": "وخسف القمر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5560,
    "jozz": 29,
    "page": 577,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "وَجُمِعَ ٱلشَّمۡسُ وَٱلۡقَمَرُ ٩",
    "text_simple": "وجمع الشمس والقمر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5561,
    "jozz": 29,
    "page": 577,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "يَقُولُ ٱلۡإِنسَٰنُ يَوۡمَئِذٍ أَيۡنَ ٱلۡمَفَرُّ ١٠",
    "text_simple": "يقول الإنسان يومئذ أين المفر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5562,
    "jozz": 29,
    "page": 577,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "كـَلَّا لَا وَزَرَ ١١",
    "text_simple": "كلا لا وزر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5563,
    "jozz": 29,
    "page": 577,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمُسۡتَقَرُّ ١٢",
    "text_simple": "إلى ربك يومئذ المستقر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5564,
    "jozz": 29,
    "page": 577,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 13,
    "text_tashkeel": "يُنَبَّؤُاْ ٱلۡإِنسَٰنُ يَوۡمَئِذِۭ بِمَا قَدَّمَ وَأَخَّرَ ١٣",
    "text_simple": "ينبأ الإنسان يومئذ بما قدم وأخر",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5565,
    "jozz": 29,
    "page": 577,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 14,
    "text_tashkeel": "بَلِ ٱلۡإِنسَٰنُ عَلَىٰ نَفۡسِهِۦ بَصِيرَةٞ ١٤",
    "text_simple": "بل الإنسان على نفسه بصيرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5566,
    "jozz": 29,
    "page": 577,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 15,
    "text_tashkeel": "وَلَوۡ أَلۡقَىٰ مَعَاذِيرَهُۥ ١٥",
    "text_simple": "ولو ألقى معاذيره",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5567,
    "jozz": 29,
    "page": 577,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 16,
    "text_tashkeel": "لَا تُحَرِّكۡ بِهِۦ لِسَانَكَ لِتَعۡجَلَ بِهِۦٓ ١٦",
    "text_simple": "لا تحرك به لسانك لتعجل به",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5568,
    "jozz": 29,
    "page": 577,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 17,
    "text_tashkeel": "إِنَّ عَلَيۡنَا جَمۡعَهُۥ وَقُرۡءَانَهُۥ ١٧",
    "text_simple": "إن علينا جمعه وقرآنه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5569,
    "jozz": 29,
    "page": 577,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": "فَإِذَا قَرَأۡنَٰهُ فَٱتَّبِعۡ قُرۡءَانَهُۥ ١٨",
    "text_simple": "فإذا قرأناه فاتبع قرآنه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5570,
    "jozz": 29,
    "page": 577,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 19,
    "text_tashkeel": "ثُمَّ إِنَّ عَلَيۡنَا بَيَانَهُۥ ١٩",
    "text_simple": "ثم إن علينا بيانه",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5571,
    "jozz": 29,
    "page": 578,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": "كـَلَّا بَلۡ تُحِبُّونَ ٱلۡعَاجِلَةَ ٢٠",
    "text_simple": "كلا بل تحبون العاجلة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5572,
    "jozz": 29,
    "page": 578,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 21,
    "text_tashkeel": "وَتَذَرُونَ ٱلۡأٓخِرَةَ ٢١",
    "text_simple": "وتذرون الآخرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5573,
    "jozz": 29,
    "page": 578,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 22,
    "text_tashkeel": "وُجُوهٞ يَوۡمَئِذٖ نَّاضِرَةٌ ٢٢",
    "text_simple": "وجوه يومئذ ناضرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5574,
    "jozz": 29,
    "page": 578,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "إِلَىٰ رَبِّهَا نَاظِرَةٞ ٢٣",
    "text_simple": "إلى ربها ناظرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5575,
    "jozz": 29,
    "page": 578,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 24,
    "text_tashkeel": "وَوُجُوهٞ يَوۡمَئِذِۭ بَاسِرَةٞ ٢٤",
    "text_simple": "ووجوه يومئذ باسرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5576,
    "jozz": 29,
    "page": 578,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": "تَظُنُّ أَن يُفۡعَلَ بِهَا فَاقِرَةٞ ٢٥",
    "text_simple": "تظن أن يفعل بها فاقرة",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5577,
    "jozz": 29,
    "page": 578,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 26,
    "text_tashkeel": "كـَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِيَ ٢٦",
    "text_simple": "كلا إذا بلغت التراقي",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5578,
    "jozz": 29,
    "page": 578,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 27,
    "text_tashkeel": "وَقِيلَ مَنۡۜ رَاقٖ ٢٧",
    "text_simple": "وقيل من راق",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5579,
    "jozz": 29,
    "page": 578,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": "وَظَنَّ أَنَّهُ ٱلۡفِرَاقُ ٢٨",
    "text_simple": "وظن أنه الفراق",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5580,
    "jozz": 29,
    "page": 578,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 29,
    "text_tashkeel": "وَٱلۡتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ ٢٩",
    "text_simple": "والتفت الساق بالساق",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5581,
    "jozz": 29,
    "page": 578,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 30,
    "text_tashkeel": "إِلَىٰ رَبِّكَ يَوۡمَئِذٍ ٱلۡمَسَاقُ ٣٠",
    "text_simple": "إلى ربك يومئذ المساق",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5582,
    "jozz": 29,
    "page": 578,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 31,
    "text_tashkeel": "فَلَا صَدَّقَ وَلَا صَلَّىٰ ٣١",
    "text_simple": "فلا صدق ولا صلى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5583,
    "jozz": 29,
    "page": 578,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 32,
    "text_tashkeel": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ ٣٢",
    "text_simple": "ولكن كذب وتولى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5584,
    "jozz": 29,
    "page": 578,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 33,
    "text_tashkeel": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهۡلِهِۦ يَتَمَطَّىٰٓ ٣٣",
    "text_simple": "ثم ذهب إلى أهله يتمطى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5585,
    "jozz": 29,
    "page": 578,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 34,
    "text_tashkeel": "أَوۡلَىٰ لَكَ فَأَوۡلَىٰ ٣٤",
    "text_simple": "أولى لك فأولى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5586,
    "jozz": 29,
    "page": 578,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 35,
    "text_tashkeel": "ثُمَّ أَوۡلَىٰ لَكَ فَأَوۡلَىٰٓ ٣٥",
    "text_simple": "ثم أولى لك فأولى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5587,
    "jozz": 29,
    "page": 578,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 36,
    "text_tashkeel": "أَيَحۡسَبُ ٱلۡإِنسَٰنُ أَن يُتۡرَكَ سُدًى ٣٦",
    "text_simple": "أيحسب الإنسان أن يترك سدى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5588,
    "jozz": 29,
    "page": 578,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 37,
    "text_tashkeel": "أَلَمۡ يَكُ نُطۡفَةٗ مِّن مَّنِيّٖ يُمۡنَىٰ ٣٧",
    "text_simple": "ألم يك نطفة من مني يمنى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5589,
    "jozz": 29,
    "page": 578,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 38,
    "text_tashkeel": "ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوَّىٰ ٣٨",
    "text_simple": "ثم كان علقة فخلق فسوى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5590,
    "jozz": 29,
    "page": 578,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 39,
    "text_tashkeel": "فَجَعَلَ مِنۡهُ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰٓ ٣٩",
    "text_simple": "فجعل منه الزوجين الذكر والأنثى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  },
  {
    "id": 5591,
    "jozz": 29,
    "page": 578,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 40,
    "text_tashkeel": "أَلَيۡسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحۡـِۧيَ ٱلۡمَوۡتَىٰ ٤٠",
    "text_simple": "أليس ذلك بقادر على أن يحيي الموتى",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة"
  }
]