[
  {
    "id": 5943,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 1,
    "text_tashkeel": "هَلَ اَت۪يٰكَ حَدِيثُ اُ۬لْغَٰشِيَةِۖ ١",
    "text_simple": "هل اتيك حديث الغشية"
  },
  {
    "id": 5944,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 2,
    "text_tashkeel": "وُجُوهٞ يَوْمَئِذٍ خَٰشِعَةٌ ٢",
    "text_simple": "وجوهٞ يومئذ خشعة"
  },
  {
    "id": 5945,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 3,
    "text_tashkeel": "عَامِلَةٞ نَّاصِبَةٞ ٣",
    "text_simple": "عاملةٞ ناصبةٞ"
  },
  {
    "id": 5946,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 4,
    "text_tashkeel": "تَصْلَىٰ نَاراً حَامِيَةٗ ٤",
    "text_simple": "تصلى نارا حاميةٗ"
  },
  {
    "id": 5947,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 5,
    "text_tashkeel": "تُسْق۪ىٰ مِنْ عَيْنٍ اٰنِيَةٖۖ ٥",
    "text_simple": "تسقى من عين انيةٖ"
  },
  {
    "id": 5948,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 6,
    "text_tashkeel": "لَّيْسَ لَهُمْ طَعَامٌ اِلَّا مِن ضَرِيعٖ ٦",
    "text_simple": "ليس لهم طعام الا من ضريعٖ"
  },
  {
    "id": 5949,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 7,
    "text_tashkeel": "لَّا يُسْمِنُ وَلَا يُغْنِے مِن جُوعٖۖ ٧",
    "text_simple": "لا يسمن ولا يغنے من جوعٖ"
  },
  {
    "id": 5950,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 8,
    "text_tashkeel": "وُجُوهٞ يَوْمَئِذٖ نَّاعِمَةٞ ٨",
    "text_simple": "وجوهٞ يومئذٖ ناعمةٞ"
  },
  {
    "id": 5951,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 9,
    "text_tashkeel": "لِّسَعْيِهَا رَاضِيَةٞ ٩",
    "text_simple": "لسعيها راضيةٞ"
  },
  {
    "id": 5952,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 10,
    "text_tashkeel": "فِے جَنَّةٍ عَالِيَةٖ ١٠",
    "text_simple": "فے جنة عاليةٖ"
  },
  {
    "id": 5953,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 11,
    "text_tashkeel": "لَّا تُسْمَعُ فِيهَا لَٰغِيَةٞۖ ١١",
    "text_simple": "لا تسمع فيها لغيةٞ"
  },
  {
    "id": 5954,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 12,
    "text_tashkeel": "فِيهَا عَيْنٞ جَارِيَةٞۖ ١٢",
    "text_simple": "فيها عينٞ جاريةٞ"
  },
  {
    "id": 5955,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 13,
    "text_tashkeel": "فِيهَا سُرُرٞ مَّرْفُوعَةٞ ١٣",
    "text_simple": "فيها سررٞ مرفوعةٞ"
  },
  {
    "id": 5956,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 14,
    "text_tashkeel": "وَأَكْوَابٞ مَّوْضُوعَةٞ ١٤",
    "text_simple": "وأكوابٞ موضوعةٞ"
  },
  {
    "id": 5957,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 15,
    "text_tashkeel": "وَنَمَارِقُ مَصْفُوفَةٞ ١٥",
    "text_simple": "ونمارق مصفوفةٞ"
  },
  {
    "id": 5958,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 16,
    "text_tashkeel": "وَزَرَابِيُّ مَبْثُوثَةٌۖ ١٦",
    "text_simple": "وزرابي مبثوثة"
  },
  {
    "id": 5959,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 17,
    "text_tashkeel": "۞ اَفَلَا يَنظُرُونَ إِلَى اَ۬لِابِلِ كَيْفَ خُلِقَتْ ١٧",
    "text_simple": "افلا ينظرون إلى الابل كيف خلقت"
  },
  {
    "id": 5960,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 18,
    "text_tashkeel": "وَإِلَى اَ۬لسَّمَآءِ كَيْفَ رُفِعَتْ ١٨",
    "text_simple": "وإلى السمآء كيف رفعت"
  },
  {
    "id": 5961,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 19,
    "text_tashkeel": " وَإِلَى اَ۬لْجِبَالِ كَيْفَ نُصِبَتْ ١٩",
    "text_simple": "وإلى الجبال كيف نصبت"
  },
  {
    "id": 5962,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 20,
    "text_tashkeel": "وَإِلَى اَ۬لَارْضِ كَيْفَ سُطِحَتْۖ ٢٠",
    "text_simple": "وإلى الارض كيف سطحت"
  },
  {
    "id": 5963,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 21,
    "text_tashkeel": " فَذَكِّرِ اِنَّمَآ أَنتَ مُذَكِّرٞۖ ٢١",
    "text_simple": "فذكر انمآ أنت مذكرٞ"
  },
  {
    "id": 5964,
    "jozz": 30,
    "page": "592",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 22,
    "text_tashkeel": "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍۖ ٢٢",
    "text_simple": "لست عليهم بمصيطر"
  },
  {
    "id": 5965,
    "jozz": 30,
    "page": "593",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 23,
    "text_tashkeel": " اِلَّا مَن تَوَلّ۪ىٰ وَكَفَرَ ٢٣",
    "text_simple": "الا من تولى وكفر"
  },
  {
    "id": 5966,
    "jozz": 30,
    "page": "593",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 24,
    "text_tashkeel": "فَيُعَذِّبُهُ اُ۬للَّهُ اُ۬لْعَذَابَ اَ۬لَاكْبَرَۖ ٢٤",
    "text_simple": "فيعذبه الله العذاب الاكبر"
  },
  {
    "id": 5967,
    "jozz": 30,
    "page": "593",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 25,
    "text_tashkeel": " إِنَّ إِلَيْنَآ إِيَابَهُمْ ٢٥",
    "text_simple": "إن إلينآ إيابهم"
  },
  {
    "id": 5968,
    "jozz": 30,
    "page": "593",
    "sura_no": 88,
    "sura_name_en": "Al-Ghāshiyah",
    "sura_name_ar": "الغَاشِية ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 26,
    "text_tashkeel": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْۖ ٢٦",
    "text_simple": "ثم إن علينا حسابهم"
  }
]