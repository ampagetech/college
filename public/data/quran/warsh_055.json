    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 49,
    "text_tashkeel": " فِيهِمَا عَيْنَٰنِ تَجْرِيَٰنِۖ ٤٩",
