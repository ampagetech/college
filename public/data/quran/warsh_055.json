    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 45,
    "text_tashkeel": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَٰنِ ٤٥",
    "text_simple": "ولمن خاف مقام ربه جنتن"
