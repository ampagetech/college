    "text_tashkeel": " فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ ٣٧",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4916,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 38,
    "text_tashkeel": "فَيَوْمَئِذٖ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٞ وَلَا جَآنّٞۖ ٣٨",
    "text_simple": "فيومئذٖ لا يسٔل عن ذنبهٓ إنسٞ ولا جآنٞ"
  },
  {
    "id": 4917,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 39,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٣٩",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4918,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 40,
    "text_tashkeel": " ۞ يُعْرَفُ اُ۬لْمُجْرِمُونَ بِسِيمٰ۪هُمْ فَيُوخَذُ بِالنَّوَٰصِے وَالَاقْدَامِۖ ٤٠",
    "text_simple": "يعرف المجرمون بسيمهم فيوخذ بالنوصے والاقدام"
  },
  {
    "id": 4919,
    "jozz": 27,
    "page": "533",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 41,
    "text_tashkeel": " فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٤١",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4920,
    "jozz": 27,
    "page": "533",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 42,
    "text_tashkeel": "هَٰذِهِۦ جَهَنَّمُ اُ۬لتِے يُكَذِّبُ بِهَا اَ۬لْمُجْرِمُونَ ٤٢",
    "text_simple": "هذه جهنم التے يكذب بها المجرمون"
  },
  {
    "id": 4921,
    "jozz": 27,
    "page": "533",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 43,
    "text_tashkeel": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ اٰنٖۖ ٤٣",
    "text_simple": "يطوفون بينها وبين حميم انٖ"
  },
  {
    "id": 4922,
    "jozz": 27,
    "page": "533",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 44,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٤٤",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
