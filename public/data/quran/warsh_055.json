[
  {
    "id": 4879,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 1,
    "text_tashkeel": "اِ۬لرَّحْمَٰنُ عَلَّمَ اَ۬لْقُرْءَانَۖ ١",
    "text_simple": "الرحمن علم القرءان"
  },
  {
    "id": 4880,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 2,
    "text_tashkeel": "خَلَقَ اَ۬لِانسَٰنَ عَلَّمَهُ اُ۬لْبَيَانَۖ ٢",
    "text_simple": "خلق الانسن علمه البيان"
  },
  {
    "id": 4881,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 3,
    "text_tashkeel": " اَ۬لشَّمْسُ وَالْقَمَرُ بِحُسْبَانٖۖ ٣",
    "text_simple": "الشمس والقمر بحسبانٖ"
  },
  {
    "id": 4882,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 4,
    "text_tashkeel": "وَالنَّجْمُ وَالشَّجَرُ يَسْجُدَٰنِۖ ٤",
    "text_simple": "والنجم والشجر يسجدن"
  },
  {
    "id": 4883,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 5,
    "text_tashkeel": " وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ اَ۬لْمِيزَانَ ٥",
    "text_simple": "والسمآء رفعها ووضع الميزان"
  },
  {
    "id": 4884,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 6,
    "text_tashkeel": "أَلَّا تَطْغَوْاْ فِے اِ۬لْمِيزَانِ ٦",
    "text_simple": "ألا تطغوا فے الميزان"
  },
  {
    "id": 4885,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 7,
    "text_tashkeel": " وَأَقِيمُواْ اُ۬لْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُواْ اُ۬لْمِيزَانَۖ ٧",
    "text_simple": "وأقيموا الوزن بالقسط ولا تخسروا الميزان"
  },
  {
    "id": 4886,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "وَالَارْضَ وَضَعَهَا لِلَانَامِ ٨",
    "text_simple": "والارض وضعها للانام"
  },
  {
    "id": 4887,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "فِيهَا فَٰكِهَةٞ وَالنَّخْلُ ذَاتُ اُ۬لَاكْمَامِ ٩",
    "text_simple": "فيها فكهةٞ والنخل ذات الاكمام"
  },
  {
    "id": 4888,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": " وَالْحَبُّ ذُو اُ۬لْعَصْفِ وَالرَّيْحَانُۖ ١٠",
    "text_simple": "والحب ذو العصف والريحان"
  },
  {
    "id": 4889,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ١١",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4890,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 12,
    "text_tashkeel": " خَلَقَ اَ۬لِانسَٰنَ مِن صَلْصَٰلٖ كَالْفَخّ۪ارِ ١٢",
    "text_simple": "خلق الانسن من صلصلٖ كالفخار"
  },
  {
    "id": 4891,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 13,
    "text_tashkeel": "وَخَلَقَ اَ۬لْجَآنَّ مِن مَّارِجٖ مِّن نّ۪ارٖۖ ١٣",
    "text_simple": "وخلق الجآن من مارجٖ من نارٖ"
  },
  {
    "id": 4892,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 14,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ١٤",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4893,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 15,
    "text_tashkeel": "رَبُّ اُ۬لْمَشْرِقَيْنِ وَرَبُّ اُ۬لْمَغْرِبَيْنِۖ ١٥",
    "text_simple": "رب المشرقين ورب المغربين"
  },
  {
    "id": 4894,
    "jozz": 27,
    "page": "531",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 16,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ١٦",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4895,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 17,
    "text_tashkeel": " مَرَجَ اَ۬لْبَحْرَيْنِ يَلْتَقِيَٰنِ ١٧",
    "text_simple": "مرج البحرين يلتقين"
  },
  {
    "id": 4896,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 18,
    "text_tashkeel": "بَيْنَهُمَا بَرْزَخٞ لَّا يَبْغِيَٰنِۖ ١٨",
    "text_simple": "بينهما برزخٞ لا يبغين"
  },
  {
    "id": 4897,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 19,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ١٩",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4898,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 20,
    "text_tashkeel": "يُخْرَجُ مِنْهُمَا اَ۬للُّؤْلُؤُاْ وَالْمَرْجَانُۖ ٢٠",
    "text_simple": "يخرج منهما اللؤلؤا والمرجان"
  },
  {
    "id": 4899,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 21,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٢١",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4900,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 22,
    "text_tashkeel": "وَلَهُ اُ۬لْجَوَارِ اِ۬لْمُنشَأَٰتُ فِے اِ۬لْبَحْرِ كَالَاعْلَٰمِۖ ٢٢",
    "text_simple": "وله الجوار المنشأت فے البحر كالاعلم"
  },
  {
    "id": 4901,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 23,
    "text_tashkeel": " فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٢٣",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4902,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 24,
    "text_tashkeel": "كُلُّ مَنْ عَلَيْهَا فَانٖ ٢٤",
    "text_simple": "كل من عليها فانٖ"
  },
  {
    "id": 4903,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 25,
    "text_tashkeel": "وَيَبْق۪ىٰ وَجْهُ رَبِّكَ ذُو اُ۬لْجَلَٰلِ وَالِاكْرَامِۖ ٢٥",
    "text_simple": "ويبقى وجه ربك ذو الجلل والاكرام"
  },
  {
    "id": 4904,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 26,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٢٦",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4905,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 27,
    "text_tashkeel": " يَسْـَٔلُهُۥ مَن فِے اِ۬لسَّمَٰوَٰتِ وَالَارْضِ كُلَّ يَوْمٍ هُوَ فِے شَأْنٖۖ ٢٧",
    "text_simple": "يسٔله من فے السموت والارض كل يوم هو فے شأنٖ"
  },
  {
    "id": 4906,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 28,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٢٨",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4907,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 29,
    "text_tashkeel": "سَنَفْرُغُ لَكُمُۥٓ أَيُّهَ اَ۬لثَّقَلَٰنِۖ ٢٩",
    "text_simple": "سنفرغ لكمٓ أيه الثقلن"
  },
  {
    "id": 4908,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 30,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٣٠",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4909,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 8,
    "line_end": 10,
    "aya_no": 31,
    "text_tashkeel": "يَٰمَعْشَرَ اَ۬لْجِنِّ وَالِانسِ إِنِ اِ۪سْتَطَعْتُمُۥٓ أَن تَنفُذُواْ مِنَ اَقْط۪ارِ اِ۬لسَّمَٰوَٰتِ وَالَارْضِ فَانفُذُواْۖ لَا تَنفُذُونَ إِلَّا بِسُلْطَٰنٖۖ ٣١",
    "text_simple": "يمعشر الجن والانس إن استطعتمٓ أن تنفذوا من اقطار السموت والارض فانفذوا لا تنفذون إلا بسلطنٖ"
  },
  {
    "id": 4910,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 32,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٣٢",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4911,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 33,
    "text_tashkeel": "يُرْسَلُ عَلَيْكُمَا شُوَاظٞ مِّن نّ۪ارٖ ٣٣",
    "text_simple": "يرسل عليكما شواظٞ من نارٖ"
  },
  {
    "id": 4912,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 34,
    "text_tashkeel": "وَنُحَاسٞ فَلَا تَنتَصِرَٰنِۖ ٣٤",
    "text_simple": "ونحاسٞ فلا تنتصرن"
  },
  {
    "id": 4913,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 35,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِۖ ٣٥",
    "text_simple": "فبأي ءالآء ربكما تكذبان"
  },
  {
    "id": 4914,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 36,
    "text_tashkeel": "فَإِذَا اَ۪نشَقَّتِ اِ۬لسَّمَآءُ فَكَانَتْ وَرْدَةٗ كَالدِّهَانِ ٣٦",
    "text_simple": "فإذا انشقت السمآء فكانت وردةٗ كالدهان"
  },
  {
    "id": 4915,
    "jozz": 27,
    "page": "532",
    "sura_no": 55,
