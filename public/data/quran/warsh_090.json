[
  {
    "id": 6001,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 1,
    "text_tashkeel": "لَآ أُقْسِمُ بِهَٰذَا اَ۬لْبَلَدِ ١",
    "text_simple": "لآ أقسم بهذا البلد"
  },
  {
    "id": 6002,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "وَأَنتَ حِلُّۢ بِهَٰذَا اَ۬لْبَلَدِ ٢",
    "text_simple": "وأنت حل بهذا البلد"
  },
  {
    "id": 6003,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 3,
    "text_tashkeel": "وَوَالِدٖ وَمَا وَلَدَ ٣",
    "text_simple": "ووالدٖ وما ولد"
  },
  {
    "id": 6004,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": " لَقَدْ خَلَقْنَا اَ۬لِانسَٰنَ فِے كَبَدٍۖ ٤",
    "text_simple": "لقد خلقنا الانسن فے كبد"
  },
  {
    "id": 6005,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "اَيَحْسِبُ أَن لَّنْ يَّقْدِرَ عَلَيْهِ أَحَدٞ ٥",
    "text_simple": "ايحسب أن لن يقدر عليه أحدٞ"
  },
  {
    "id": 6006,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "يَقُولُ أَهْلَكْتُ مَالاٗ لُّبَداًۖ ٦",
    "text_simple": "يقول أهلكت مالاٗ لبدا"
  },
  {
    "id": 6007,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 7,
    "text_tashkeel": "اَيَحْسِبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌۖ ٧",
    "text_simple": "ايحسب أن لم يرهٓ أحد"
  },
  {
    "id": 6008,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": " اَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ ٨",
    "text_simple": "الم نجعل له عينين"
  },
  {
    "id": 6009,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "وَلِسَاناٗ وَشَفَتَيْنِ ٩",
    "text_simple": "ولساناٗ وشفتين"
  },
  {
    "id": 6010,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "وَهَدَيْنَٰهُ اُ۬لنَّجْدَيْنِۖ ١٠",
    "text_simple": "وهدينه النجدين"
  },
  {
    "id": 6011,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "فَلَا اَ۪قْتَحَمَ اَ۬لْعَقَبَةَۖ ١١",
    "text_simple": "فلا اقتحم العقبة"
  },
  {
    "id": 6012,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا اَ۬لْعَقَبَةُۖ ١٢",
    "text_simple": "ومآ أدريك ما العقبة"
  },
  {
    "id": 6013,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 13,
    "text_tashkeel": " فَكُّ رَقَبَةٍ ١٣",
    "text_simple": "فك رقبة"
  },
  {
    "id": 6014,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 14,
    "text_tashkeel": "اَوِ اِطْعَامٞ فِے يَوْمٖ ذِے مَسْغَبَةٖ ١٤",
    "text_simple": "او اطعامٞ فے يومٖ ذے مسغبةٖ"
  },
  {
    "id": 6015,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 15,
    "text_tashkeel": "يَتِيماٗ ذَا مَقْرَبَةٍ ١٥",
    "text_simple": "يتيماٗ ذا مقربة"
  },
  {
    "id": 6016,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 16,
    "text_tashkeel": " اَوْ مِسْكِيناٗ ذَا مَتْرَبَةٖۖ ١٦",
    "text_simple": "او مسكيناٗ ذا متربةٖ"
  },
  {
    "id": 6017,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 17,
    "text_tashkeel": "ثُمَّ كَانَ مِنَ اَ۬لذِينَ ءَامَنُواْ وَتَوَاصَوْاْ بِالصَّبْرِ وَتَوَاصَوْاْ بِالْمَرْحَمَةِۖ ١٧",
    "text_simple": "ثم كان من الذين ءامنوا وتواصوا بالصبر وتواصوا بالمرحمة"
  },
  {
    "id": 6018,
    "jozz": 30,
    "page": "594",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": "أُوْلَٰٓئِكَ أَصْحَٰبُ اُ۬لْمَيْمَنَةِۖ ١٨",
    "text_simple": "أولٓئك أصحب الميمنة"
  },
  {
    "id": 6019,
    "jozz": 30,
    "page": "595",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 19,
    "text_tashkeel": " وَالذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمُۥٓ أَصْحَٰبُ اُ۬لْمَشْـَٔمَةِۖ ١٩",
    "text_simple": "والذين كفروا بٔايتنا همٓ أصحب المشٔمة"
  },
  {
    "id": 6020,
    "jozz": 30,
    "page": "595",
    "sura_no": 90,
    "sura_name_en": "Al-Balad",
    "sura_name_ar": "البَلَد ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": "عَلَيْهِمْ نَارٞ مُّوصَدَةٞۖ ٢٠",
    "text_simple": "عليهم نارٞ موصدةٞ"
  }
]