[
  {
    "id": 5250,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 1,
    "text_tashkeel": "نُّٓۖ وَالْقَلَمِ وَمَا يَسْطُرُونَ ١",
    "text_simple": "نٓ والقلم وما يسطرون"
  },
  {
    "id": 5251,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٖ ٢",
    "text_simple": "مآ أنت بنعمة ربك بمجنونٖ"
  },
  {
    "id": 5252,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": " وَإِنَّ لَكَ لَأَجْراً غَيْرَ مَمْنُونٖ ٣",
    "text_simple": "وإن لك لأجرا غير ممنونٖ"
  },
  {
    "id": 5253,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٖۖ ٤",
    "text_simple": "وإنك لعلى خلق عظيمٖ"
  },
  {
    "id": 5254,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": " فَسَتُبْصِرُ وَيُبْصِرُونَ ٥",
    "text_simple": "فستبصر ويبصرون"
  },
  {
    "id": 5255,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "بِأَييِّكُمُ اُ۬لْمَفْتُونُۖ ٦",
    "text_simple": "بأييكم المفتون"
  },
  {
    "id": 5256,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦۖ وَهُوَ أَعْلَمُ بِالْمُهْتَدِينَۖ ٧",
    "text_simple": "إن ربك هو أعلم بمن ضل عن سبيله وهو أعلم بالمهتدين"
  },
  {
    "id": 5257,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 8,
    "text_tashkeel": "فَلَا تُطِعِ اِ۬لْمُكَذِّبِينَۖ ٨",
    "text_simple": "فلا تطع المكذبين"
  },
  {
    "id": 5258,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 9,
    "text_tashkeel": "وَدُّواْ لَوْ تُدْهِنُ فَيُدْهِنُونَۖ ٩",
    "text_simple": "ودوا لو تدهن فيدهنون"
  },
  {
    "id": 5259,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 10,
    "text_tashkeel": "وَلَا تُطِعْ كُلَّ حَلَّٰفٖ مَّهِينٍ ١٠",
    "text_simple": "ولا تطع كل حلفٖ مهين"
  },
  {
    "id": 5260,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 11,
    "text_tashkeel": "هَمَّازٖ مَّشَّآءِۢ بِنَمِيمٖ ١١",
    "text_simple": "همازٖ مشآء بنميمٖ"
  },
  {
    "id": 5261,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 12,
    "text_tashkeel": "مَّنَّاعٖ لِّلْخَيْرِ مُعْتَدٍ اَثِيمٍ ١٢",
    "text_simple": "مناعٖ للخير معتد اثيم"
  },
  {
    "id": 5262,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 13,
    "text_tashkeel": " عُتُلِّۢ بَعْدَ ذَٰلِكَ زَنِيمٍ ١٣",
    "text_simple": "عتل بعد ذلك زنيم"
  },
  {
    "id": 5263,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 14,
    "text_tashkeel": "اَن كَانَ ذَا مَالٖ وَبَنِينَ ١٤",
    "text_simple": "ان كان ذا مالٖ وبنين"
  },
  {
    "id": 5264,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 15,
    "text_tashkeel": "إِذَا تُتْل۪ىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ اُ۬لَاوَّلِينَۖ ١٥",
    "text_simple": "إذا تتلى عليه ءايتنا قال أسطير الاولين"
  },
  {
    "id": 5265,
    "jozz": 29,
    "page": "564",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 16,
    "text_tashkeel": "سَنَسِمُهُۥ عَلَى اَ۬لْخُرْطُومِۖ ١٦",
    "text_simple": "سنسمه على الخرطوم"
  },
  {
    "id": 5266,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 17,
    "text_tashkeel": " إِنَّا بَلَوْنَٰهُمْ كَمَا بَلَوْنَآ أَصْحَٰبَ اَ۬لْجَنَّةِ إِذَ اَقْسَمُواْ لَيَصْرِمُنَّهَا مُصْبِحِينَ ١٧",
    "text_simple": "إنا بلونهم كما بلونآ أصحب الجنة إذ اقسموا ليصرمنها مصبحين"
  },
  {
    "id": 5267,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 18,
    "text_tashkeel": "وَلَا يَسْتَثْنُونَۖ ١٨",
    "text_simple": "ولا يستثنون"
  },
  {
    "id": 5268,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 19,
    "text_tashkeel": "۞ فَطَافَ عَلَيْهَا طَآئِفٞ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ ١٩",
    "text_simple": "فطاف عليها طآئفٞ من ربك وهم نآئمون"
  },
  {
    "id": 5269,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 20,
    "text_tashkeel": "فَأَصْبَحَتْ كَالصَّرِيمِۖ ٢٠",
    "text_simple": "فأصبحت كالصريم"
  },
  {
    "id": 5270,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 21,
    "text_tashkeel": "فَتَنَادَوْاْ مُصْبِحِينَ ٢١",
    "text_simple": "فتنادوا مصبحين"
  },
  {
    "id": 5271,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 22,
    "text_tashkeel": "أَنُ اُ۟غْدُواْ عَلَىٰ حَرْثِكُمُۥٓ إِن كُنتُمْ صَٰرِمِينَۖ ٢٢",
    "text_simple": "أن اغدوا على حرثكمٓ إن كنتم صرمين"
  },
  {
    "id": 5272,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 23,
    "text_tashkeel": "فَانطَلَقُواْ وَهُمْ يَتَخَٰفَتُونَ ٢٣",
    "text_simple": "فانطلقوا وهم يتخفتون"
  },
  {
    "id": 5273,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 24,
    "text_tashkeel": "أَن لَّا يَدْخُلَنَّهَا اَ۬لْيَوْمَ عَلَيْكُم مِّسْكِينٞۖ ٢٤",
    "text_simple": "أن لا يدخلنها اليوم عليكم مسكينٞ"
  },
  {
    "id": 5274,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 25,
    "text_tashkeel": "وَغَدَوْاْ عَلَىٰ حَرْدٖ قَٰدِرِينَۖ ٢٥",
    "text_simple": "وغدوا على حردٖ قدرين"
  },
  {
    "id": 5275,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 26,
    "text_tashkeel": "فَلَمَّا رَأَوْهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ ٢٦",
    "text_simple": "فلما رأوها قالوٓا إنا لضآلون"
  },
  {
    "id": 5276,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 27,
    "text_tashkeel": "بَلْ نَحْنُ مَحْرُومُونَۖ ٢٧",
    "text_simple": "بل نحن محرومون"
  },
  {
    "id": 5277,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 28,
    "text_tashkeel": "قَالَ أَوْسَطُهُمُۥٓ أَلَمَ اَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَۖ ٢٨",
    "text_simple": "قال أوسطهمٓ ألم اقل لكم لولا تسبحون"
  },
  {
    "id": 5278,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 29,
    "text_tashkeel": "قَالُواْ سُبْحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَۖ ٢٩",
    "text_simple": "قالوا سبحن ربنآ إنا كنا ظلمين"
  },
  {
    "id": 5279,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 30,
    "text_tashkeel": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٖ يَتَلَٰوَمُونَۖ ٣٠",
    "text_simple": "فأقبل بعضهم على بعضٖ يتلومون"
  },
  {
    "id": 5280,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 31,
    "text_tashkeel": "قَالُواْ يَٰوَيْلَنَآ إِنَّا كُنَّا طَٰغِينَ ٣١",
    "text_simple": "قالوا يويلنآ إنا كنا طغين"
  },
  {
    "id": 5281,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 32,
    "text_tashkeel": "عَس۪ىٰ رَبُّنَآ أَنْ يُّبَدِّلَنَا خَيْراٗ مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَۖ ٣٢",
    "text_simple": "عسى ربنآ أن يبدلنا خيراٗ منهآ إنآ إلى ربنا رغبون"
  },
  {
    "id": 5282,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 33,
    "text_tashkeel": "كَذَٰلِكَ اَ۬لْعَذَابُۖ وَلَعَذَابُ اُ۬لَاخِرَةِ أَكْبَرُ لَوْ كَانُواْ يَعْلَمُونَۖ ٣٣",
    "text_simple": "كذلك العذاب ولعذاب الاخرة أكبر لو كانوا يعلمون"
  },
  {
    "id": 5283,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 34,
    "text_tashkeel": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّٰتِ اِ۬لنَّعِيمِۖ ٣٤",
    "text_simple": "إن للمتقين عند ربهم جنت النعيم"
  },
  {
    "id": 5284,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 35,
    "text_tashkeel": " أَفَنَجْعَلُ اُ۬لْمُسْلِمِينَ كَالْمُجْرِمِينَۖ ٣٥",
    "text_simple": "أفنجعل المسلمين كالمجرمين"
  },
  {
    "id": 5285,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 36,
    "text_tashkeel": "مَا لَكُمْۖ كَيْفَ تَحْكُمُونَۖ ٣٦",
    "text_simple": "ما لكم كيف تحكمون"
  },
  {
    "id": 5286,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 37,
    "text_tashkeel": "أَمْ لَكُمْ كِتَٰبٞ فِيهِ تَدْرُسُونَ ٣٧",
    "text_simple": "أم لكم كتبٞ فيه تدرسون"
  },
  {
    "id": 5287,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 38,
    "text_tashkeel": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَۖ ٣٨",
    "text_simple": "إن لكم فيه لما تخيرون"
  },
  {
    "id": 5288,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 39,
    "text_tashkeel": "أَمْ لَكُمُۥٓ أَيْمَٰنٌ عَلَيْنَا بَٰلِغَةٌ اِلَىٰ يَوْمِ اِ۬لْقِيَٰمَةِ إِنَّ لَكُمْ لَمَا تَحْكُمُونَۖ ٣٩",
    "text_simple": "أم لكمٓ أيمن علينا بلغة الى يوم القيمة إن لكم لما تحكمون"
  },
  {
    "id": 5289,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 40,
    "text_tashkeel": "سَلْهُمُۥٓ أَيُّهُم بِذَٰلِكَ زَعِيمٌۖ ٤٠",
    "text_simple": "سلهمٓ أيهم بذلك زعيم"
  },
  {
    "id": 5290,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 41,
    "text_tashkeel": "اَمْ لَهُمْ شُرَكَآءُ فَلْيَاتُواْ بِشُرَكَآئِهِمُۥٓ إِن كَانُواْ صَٰدِقِينَۖ ٤١",
    "text_simple": "ام لهم شركآء فلياتوا بشركآئهمٓ إن كانوا صدقين"
  },
  {
    "id": 5291,
    "jozz": 29,
    "page": "565",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 42,
    "text_tashkeel": " يَوْمَ يُكْشَفُ عَن سَاقٖ وَيُدْعَوْنَ إِلَى اَ۬لسُّجُودِ فَلَا يَسْتَطِيعُونَۖ ٤٢",
    "text_simple": "يوم يكشف عن ساقٖ ويدعون إلى السجود فلا يستطيعون"
  },
  {
    "id": 5292,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 43,
    "text_tashkeel": " خَٰشِعَةً اَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٞۖ وَقَدْ كَانُواْ يُدْعَوْنَ إِلَى اَ۬لسُّجُودِ وَهُمْ سَٰلِمُونَۖ ٤٣",
    "text_simple": "خشعة ابصرهم ترهقهم ذلةٞ وقد كانوا يدعون إلى السجود وهم سلمون"
  },
  {
    "id": 5293,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 44,
    "text_tashkeel": "فَذَرْنِے وَمَنْ يُّكَذِّبُ بِهَٰذَا اَ۬لْحَدِيثِۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ ٤٤",
    "text_simple": "فذرنے ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"
  },
  {
    "id": 5294,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 45,
    "text_tashkeel": "وَأُمْلِے لَهُمُۥٓۖ إِنَّ كَيْدِے مَتِينٌۖ ٤٥",
    "text_simple": "وأملے لهمٓ إن كيدے متين"
  },
  {
    "id": 5295,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 46,
    "text_tashkeel": "اَمْ تَسْـَٔلُهُمُۥٓ أَجْراٗ فَهُم مِّن مَّغْرَمٖ مُّثْقَلُونَۖ ٤٦",
    "text_simple": "ام تسٔلهمٓ أجراٗ فهم من مغرمٖ مثقلون"
  },
  {
    "id": 5296,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 47,
    "text_tashkeel": "أَمْ عِندَهُمُ اُ۬لْغَيْبُ فَهُمْ يَكْتُبُونَۖ ٤٧",
    "text_simple": "أم عندهم الغيب فهم يكتبون"
  },
  {
    "id": 5297,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 48,
    "text_tashkeel": " ۞ فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَٰحِبِ اِ۬لْحُوتِ إِذْ نَاد۪ىٰ وَهُوَ مَكْظُومٞۖ ٤٨",
    "text_simple": "فاصبر لحكم ربك ولا تكن كصحب الحوت إذ نادى وهو مكظومٞ"
  },
  {
    "id": 5298,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 49,
    "text_tashkeel": "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٞ مِّن رَّبِّهِۦ لَنُبِذَ بِالْعَرَآءِ وَهُوَ مَذْمُومٞۖ ٤٩",
    "text_simple": "لولآ أن تدركه نعمةٞ من ربه لنبذ بالعرآء وهو مذمومٞ"
  },
  {
    "id": 5299,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 50,
    "text_tashkeel": "فَاجْتَبٰ۪هُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ اَ۬لصَّٰلِحِينَۖ ٥٠",
    "text_simple": "فاجتبه ربه فجعله من الصلحين"
  },
  {
    "id": 5300,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 51,
    "text_tashkeel": " وَإِنْ يَّكَادُ اُ۬لذِينَ كَفَرُواْ لَيَزْلِقُونَكَ بِأَبْصٰ۪رِهِمْ لَمَّا سَمِعُواْ اُ۬لذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٞۖ ٥١",
    "text_simple": "وإن يكاد الذين كفروا ليزلقونك بأبصرهم لما سمعوا الذكر ويقولون إنه لمجنونٞ"
  },
  {
    "id": 5301,
    "jozz": 29,
    "page": "566",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 52,
    "text_tashkeel": "وَمَا هُوَ إِلَّا ذِكْرٞ لِّلْعَٰلَمِينَۖ ٥٢",
    "text_simple": "وما هو إلا ذكرٞ للعلمين"
  }
]