    "text_simple": "فإذا جاءت الصاخة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5792,
    "jozz": 30,
    "page": 585,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 34,
    "text_tashkeel": "يَوۡمَ يَفِرُّ ٱلۡمَرۡءُ مِنۡ أَخِيهِ ٣٤",
    "text_simple": "يوم يفر المرء من أخيه",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5793,
    "jozz": 30,
    "page": 585,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 35,
    "text_tashkeel": "وَأُمِّهِۦ وَأَبِيهِ ٣٥",
    "text_simple": "وأمه وأبيه",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5794,
    "jozz": 30,
    "page": 585,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 36,
    "text_tashkeel": "وَصَٰحِبَتِهِۦ وَبَنِيهِ ٣٦",
