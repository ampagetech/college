    "text_simple": "وعنبا وقضبا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5787,
    "jozz": 30,
    "page": 585,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 29,
    "text_tashkeel": "وَزَيۡتُونٗا وَنَخۡلٗا ٢٩",
    "text_simple": "وزيتونا ونخلا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5788,
    "jozz": 30,
    "page": 585,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 30,
    "text_tashkeel": "وَحَدَآئِقَ غُلۡبٗا ٣٠",
    "text_simple": "وحدائق غلبا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5789,
    "jozz": 30,
    "page": 585,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 31,
    "text_tashkeel": "وَفَٰكِهَةٗ وَأَبّٗا ٣١",
    "text_simple": "وفاكهة وأبا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5790,
    "jozz": 30,
    "page": 585,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 32,
    "text_tashkeel": "مَّتَٰعٗا لَّكُمۡ وَلِأَنۡعَٰمِكُمۡ ٣٢",
    "text_simple": "متاعا لكم ولأنعامكم",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5791,
    "jozz": 30,
    "page": 585,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 33,
