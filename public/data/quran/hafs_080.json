    "text_tashkeel": "عَبَسَ وَتَوَلَّىٰٓ ١",
    "text_simple": "عبس وتولى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5760,
    "jozz": 30,
    "page": 585,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "أَن جَآءَهُ ٱلۡأَعۡمَىٰ ٢",
    "text_simple": "أن جاءه الأعمى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5761,
    "jozz": 30,
    "page": 585,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "text_tashkeel": "وَمَا يُدۡرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ ٣",
    "text_simple": "وما يدريك لعله يزكى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5762,
    "jozz": 30,
    "page": 585,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "أَوۡ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكۡرَىٰٓ ٤",
    "text_simple": "أو يذكر فتنفعه الذكرى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5763,
    "jozz": 30,
    "page": 585,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "أَمَّا مَنِ ٱسۡتَغۡنَىٰ ٥",
    "text_simple": "أما من استغنى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5764,
    "jozz": 30,
    "page": 585,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 6,
    "text_tashkeel": "فَأَنتَ لَهُۥ تَصَدَّىٰ ٦",
    "text_simple": "فأنت له تصدى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5765,
    "jozz": 30,
    "page": 585,
    "line_start": 4,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "وَمَا عَلَيۡكَ أَلَّا يَزَّكَّىٰ ٧",
    "text_simple": "وما عليك ألا يزكى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5766,
    "jozz": 30,
    "page": 585,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 8,
    "text_tashkeel": "وَأَمَّا مَن جَآءَكَ يَسۡعَىٰ ٨",
    "text_simple": "وأما من جاءك يسعى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5767,
    "jozz": 30,
    "page": 585,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 9,
    "text_tashkeel": "وَهُوَ يَخۡشَىٰ ٩",
    "text_simple": "وهو يخشى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5768,
    "jozz": 30,
    "page": 585,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 10,
    "text_tashkeel": "فَأَنتَ عَنۡهُ تَلَهَّىٰ ١٠",
    "text_simple": "فأنت عنه تلهى",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5769,
    "jozz": 30,
    "page": 585,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 11,
    "text_tashkeel": "كـَلَّآ إِنَّهَا تَذۡكِرَةٞ ١١",
    "text_simple": "كلا إنها تذكرة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5770,
    "jozz": 30,
    "page": 585,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 12,
    "text_tashkeel": "فَمَن شَآءَ ذَكَرَهُۥ ١٢",
    "text_simple": "فمن شاء ذكره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5771,
    "jozz": 30,
    "page": 585,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 13,
    "text_tashkeel": "فِي صُحُفٖ مُّكَرَّمَةٖ ١٣",
    "text_simple": "في صحف مكرمة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5772,
    "jozz": 30,
    "page": 585,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 14,
    "text_tashkeel": "مَّرۡفُوعَةٖ مُّطَهَّرَةِۭ ١٤",
    "text_simple": "مرفوعة مطهرة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5773,
    "jozz": 30,
    "page": 585,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 15,
    "text_tashkeel": "بِأَيۡدِي سَفَرَةٖ ١٥",
    "text_simple": "بأيدي سفرة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5774,
    "jozz": 30,
    "page": 585,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 16,
    "text_tashkeel": "كِرَامِۭ بَرَرَةٖ ١٦",
    "text_simple": "كرام بررة",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5775,
    "jozz": 30,
    "page": 585,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 17,
    "text_tashkeel": "قُتِلَ ٱلۡإِنسَٰنُ مَآ أَكۡفَرَهُۥ ١٧",
    "text_simple": "قتل الإنسان ما أكفره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5776,
    "jozz": 30,
    "page": 585,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 18,
    "text_tashkeel": "مِنۡ أَيِّ شَيۡءٍ خَلَقَهُۥ ١٨",
    "text_simple": "من أي شيء خلقه",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5777,
    "jozz": 30,
    "page": 585,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 19,
    "text_tashkeel": "مِن نُّطۡفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ ١٩",
    "text_simple": "من نطفة خلقه فقدره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5778,
    "jozz": 30,
    "page": 585,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 20,
    "text_tashkeel": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ ٢٠",
    "text_simple": "ثم السبيل يسره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5779,
    "jozz": 30,
    "page": 585,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 21,
    "text_tashkeel": "ثُمَّ أَمَاتَهُۥ فَأَقۡبَرَهُۥ ٢١",
    "text_simple": "ثم أماته فأقبره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5780,
    "jozz": 30,
    "page": 585,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 22,
    "text_tashkeel": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ ٢٢",
    "text_simple": "ثم إذا شاء أنشره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5781,
    "jozz": 30,
    "page": 585,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 23,
    "text_tashkeel": "كـَلَّا لَمَّا يَقۡضِ مَآ أَمَرَهُۥ ٢٣",
    "text_simple": "كلا لما يقض ما أمره",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5782,
    "jozz": 30,
    "page": 585,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 24,
    "text_tashkeel": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ ٢٤",
    "text_simple": "فلينظر الإنسان إلى طعامه",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5783,
    "jozz": 30,
    "page": 585,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 25,
    "text_tashkeel": "أَنَّا صَبَبۡنَا ٱلۡمَآءَ صَبّٗا ٢٥",
    "text_simple": "أنا صببنا الماء صبا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5784,
    "jozz": 30,
    "page": 585,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 26,
    "text_tashkeel": "ثُمَّ شَقَقۡنَا ٱلۡأَرۡضَ شَقّٗا ٢٦",
    "text_simple": "ثم شققنا الأرض شقا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5785,
    "jozz": 30,
    "page": 585,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 27,
    "text_tashkeel": "فَأَنۢبَتۡنَا فِيهَا حَبّٗا ٢٧",
    "text_simple": "فأنبتنا فيها حبا",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ"
  },
  {
    "id": 5786,
    "jozz": 30,
    "page": 585,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 28,
