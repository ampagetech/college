    "jozz": 30,
    "page": 591,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ ٤",
    "text_simple": "إن كل نفس لما عليها حافظ",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
