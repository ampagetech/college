    "jozz": 30,
    "page": 591,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 8,
    "text_tashkeel": "إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ ٨",
    "text_simple": "إنه على رجعه لقادر",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
  },
  {
