    "jozz": 30,
    "page": 591,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 11,
    "text_tashkeel": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ ١١",
    "text_simple": "والسماء ذات الرجع",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
  },
