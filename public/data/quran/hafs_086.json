    "jozz": 30,
    "page": 591,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ ٧",
    "text_simple": "يخرج من بين الصلب والترائب",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
  },
  {
