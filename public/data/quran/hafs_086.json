    "page": 591,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ ٢",
    "text_simple": "وما أدراك ما الطارق",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
