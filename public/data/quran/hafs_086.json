    "jozz": 30,
    "page": 591,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ ٥",
    "text_simple": "فلينظر الإنسان مم خلق",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
