    "jozz": 30,
    "page": 591,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 12,
    "text_tashkeel": "وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ ١٢",
    "text_simple": "والأرض ذات الصدع",
    "sura_no": 86,
    "sura_name_en": "Aṭ-Ṭāriq",
    "sura_name_ar": "الطَّارق"
