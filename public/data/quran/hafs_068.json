[
  {
    "id": 5272,
    "jozz": 29,
    "page": 564,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 1,
    "text_tashkeel": "نٓۚ وَٱلۡقَلَمِ وَمَا يَسۡطُرُونَ ١",
    "text_simple": "ن والقلم وما يسطرون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5273,
    "jozz": 29,
    "page": 564,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "مَآ أَنتَ بِنِعۡمَةِ رَبِّكَ بِمَجۡنُونٖ ٢",
    "text_simple": "ما أنت بنعمة ربك بمجنون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5274,
    "jozz": 29,
    "page": 564,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": "وَإِنَّ لَكَ لَأَجۡرًا غَيۡرَ مَمۡنُونٖ ٣",
    "text_simple": "وإن لك لأجرا غير ممنون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5275,
    "jozz": 29,
    "page": 564,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٖ ٤",
    "text_simple": "وإنك لعلى خلق عظيم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5276,
    "jozz": 29,
    "page": 564,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "فَسَتُبۡصِرُ وَيُبۡصِرُونَ ٥",
    "text_simple": "فستبصر ويبصرون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5277,
    "jozz": 29,
    "page": 564,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "بِأَييِّكُمُ ٱلۡمَفۡتُونُ ٦",
    "text_simple": "بأيكم المفتون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5278,
    "jozz": 29,
    "page": 564,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِٱلۡمُهۡتَدِينَ ٧",
    "text_simple": "إن ربك هو أعلم بمن ضل عن سبيله وهو أعلم بالمهتدين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5279,
    "jozz": 29,
    "page": 564,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "فَلَا تُطِعِ ٱلۡمُكَذِّبِينَ ٨",
    "text_simple": "فلا تطع المكذبين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5280,
    "jozz": 29,
    "page": 564,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 9,
    "text_tashkeel": "وَدُّواْ لَوۡ تُدۡهِنُ فَيُدۡهِنُونَ ٩",
    "text_simple": "ودوا لو تدهن فيدهنون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5281,
    "jozz": 29,
    "page": 564,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "وَلَا تُطِعۡ كُلَّ حَلَّافٖ مَّهِينٍ ١٠",
    "text_simple": "ولا تطع كل حلاف مهين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5282,
    "jozz": 29,
    "page": 564,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 11,
    "text_tashkeel": "هَمَّازٖ مَّشَّآءِۭ بِنَمِيمٖ ١١",
    "text_simple": "هماز مشاء بنميم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5283,
    "jozz": 29,
    "page": 564,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 12,
    "text_tashkeel": "مَّنَّاعٖ لِّلۡخَيۡرِ مُعۡتَدٍ أَثِيمٍ ١٢",
    "text_simple": "مناع للخير معتد أثيم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5284,
    "jozz": 29,
    "page": 564,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 13,
    "text_tashkeel": "عُتُلِّۭ بَعۡدَ ذَٰلِكَ زَنِيمٍ ١٣",
    "text_simple": "عتل بعد ذلك زنيم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5285,
    "jozz": 29,
    "page": 564,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 14,
    "text_tashkeel": "أَن كَانَ ذَا مَالٖ وَبَنِينَ ١٤",
    "text_simple": "أن كان ذا مال وبنين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5286,
    "jozz": 29,
    "page": 564,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 15,
    "text_tashkeel": "إِذَا تُتۡلَىٰ عَلَيۡهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلۡأَوَّلِينَ ١٥",
    "text_simple": "إذا تتلى عليه آياتنا قال أساطير الأولين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5287,
    "jozz": 29,
    "page": 564,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 16,
    "text_tashkeel": "سَنَسِمُهُۥ عَلَى ٱلۡخُرۡطُومِ ١٦",
    "text_simple": "سنسمه على الخرطوم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5288,
    "jozz": 29,
    "page": 565,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 17,
    "text_tashkeel": "إِنَّا بَلَوۡنَٰهُمۡ كَمَا بَلَوۡنَآ أَصۡحَٰبَ ٱلۡجَنَّةِ إِذۡ أَقۡسَمُواْ لَيَصۡرِمُنَّهَا مُصۡبِحِينَ ١٧",
    "text_simple": "إنا بلوناهم كما بلونا أصحاب الجنة إذ أقسموا ليصرمنها مصبحين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5289,
    "jozz": 29,
    "page": 565,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 18,
    "text_tashkeel": "وَلَا يَسۡتَثۡنُونَ ١٨",
    "text_simple": "ولا يستثنون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5290,
    "jozz": 29,
    "page": 565,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 19,
    "text_tashkeel": "فَطَافَ عَلَيۡهَا طَآئِفٞ مِّن رَّبِّكَ وَهُمۡ نَآئِمُونَ ١٩",
    "text_simple": "فطاف عليها طائف من ربك وهم نائمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5291,
    "jozz": 29,
    "page": 565,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 20,
    "text_tashkeel": "فَأَصۡبَحَتۡ كَٱلصَّرِيمِ ٢٠",
    "text_simple": "فأصبحت كالصريم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5292,
    "jozz": 29,
    "page": 565,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 21,
    "text_tashkeel": "فَتَنَادَوۡاْ مُصۡبِحِينَ ٢١",
    "text_simple": "فتنادوا مصبحين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5293,
    "jozz": 29,
    "page": 565,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 22,
    "text_tashkeel": "أَنِ ٱغۡدُواْ عَلَىٰ حَرۡثِكُمۡ إِن كُنتُمۡ صَٰرِمِينَ ٢٢",
    "text_simple": "أن اغدوا على حرثكم إن كنتم صارمين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5294,
    "jozz": 29,
    "page": 565,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 23,
    "text_tashkeel": "فَٱنطَلَقُواْ وَهُمۡ يَتَخَٰفَتُونَ ٢٣",
    "text_simple": "فانطلقوا وهم يتخافتون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5295,
    "jozz": 29,
    "page": 565,
    "line_start": 4,
    "line_end": 5,
    "aya_no": 24,
    "text_tashkeel": "أَن لَّا يَدۡخُلَنَّهَا ٱلۡيَوۡمَ عَلَيۡكُم مِّسۡكِينٞ ٢٤",
    "text_simple": "أن لا يدخلنها اليوم عليكم مسكين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5296,
    "jozz": 29,
    "page": 565,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 25,
    "text_tashkeel": "وَغَدَوۡاْ عَلَىٰ حَرۡدٖ قَٰدِرِينَ ٢٥",
    "text_simple": "وغدوا على حرد قادرين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5297,
    "jozz": 29,
    "page": 565,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 26,
    "text_tashkeel": "فَلَمَّا رَأَوۡهَا قَالُوٓاْ إِنَّا لَضَآلُّونَ ٢٦",
    "text_simple": "فلما رأوها قالوا إنا لضالون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5298,
    "jozz": 29,
    "page": 565,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 27,
    "text_tashkeel": "بَلۡ نَحۡنُ مَحۡرُومُونَ ٢٧",
    "text_simple": "بل نحن محرومون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5299,
    "jozz": 29,
    "page": 565,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 28,
    "text_tashkeel": "قَالَ أَوۡسَطُهُمۡ أَلَمۡ أَقُل لَّكُمۡ لَوۡلَا تُسَبِّحُونَ ٢٨",
    "text_simple": "قال أوسطهم ألم أقل لكم لولا تسبحون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5300,
    "jozz": 29,
    "page": 565,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 29,
    "text_tashkeel": "قَالُواْ سُبۡحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ ٢٩",
    "text_simple": "قالوا سبحان ربنا إنا كنا ظالمين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5301,
    "jozz": 29,
    "page": 565,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 30,
    "text_tashkeel": "فَأَقۡبَلَ بَعۡضُهُمۡ عَلَىٰ بَعۡضٖ يَتَلَٰوَمُونَ ٣٠",
    "text_simple": "فأقبل بعضهم على بعض يتلاومون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5302,
    "jozz": 29,
    "page": 565,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 31,
    "text_tashkeel": "قَالُواْ يَٰوَيۡلَنَآ إِنَّا كُنَّا طَٰغِينَ ٣١",
    "text_simple": "قالوا ياويلنا إنا كنا طاغين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5303,
    "jozz": 29,
    "page": 565,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 32,
    "text_tashkeel": "عَسَىٰ رَبُّنَآ أَن يُبۡدِلَنَا خَيۡرٗا مِّنۡهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ ٣٢",
    "text_simple": "عسى ربنا أن يبدلنا خيرا منها إنا إلى ربنا راغبون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5304,
    "jozz": 29,
    "page": 565,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 33,
    "text_tashkeel": "كَذَٰلِكَ ٱلۡعَذَابُۖ وَلَعَذَابُ ٱلۡأٓخِرَةِ أَكۡبَرُۚ لَوۡ كَانُواْ يَعۡلَمُونَ ٣٣",
    "text_simple": "كذلك العذاب ولعذاب الآخرة أكبر لو كانوا يعلمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5305,
    "jozz": 29,
    "page": 565,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 34,
    "text_tashkeel": "إِنَّ لِلۡمُتَّقِينَ عِندَ رَبِّهِمۡ جَنَّٰتِ ٱلنَّعِيمِ ٣٤",
    "text_simple": "إن للمتقين عند ربهم جنات النعيم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5306,
    "jozz": 29,
    "page": 565,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 35,
    "text_tashkeel": "أَفَنَجۡعَلُ ٱلۡمُسۡلِمِينَ كَٱلۡمُجۡرِمِينَ ٣٥",
    "text_simple": "أفنجعل المسلمين كالمجرمين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5307,
    "jozz": 29,
    "page": 565,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 36,
    "text_tashkeel": "مَا لَكُمۡ كَيۡفَ تَحۡكُمُونَ ٣٦",
    "text_simple": "ما لكم كيف تحكمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5308,
    "jozz": 29,
    "page": 565,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 37,
    "text_tashkeel": "أَمۡ لَكُمۡ كِتَٰبٞ فِيهِ تَدۡرُسُونَ ٣٧",
    "text_simple": "أم لكم كتاب فيه تدرسون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5309,
    "jozz": 29,
    "page": 565,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 38,
    "text_tashkeel": "إِنَّ لَكُمۡ فِيهِ لَمَا تَخَيَّرُونَ ٣٨",
    "text_simple": "إن لكم فيه لما تخيرون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5310,
    "jozz": 29,
    "page": 565,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 39,
    "text_tashkeel": "أَمۡ لَكُمۡ أَيۡمَٰنٌ عَلَيۡنَا بَٰلِغَةٌ إِلَىٰ يَوۡمِ ٱلۡقِيَٰمَةِ إِنَّ لَكُمۡ لَمَا تَحۡكُمُونَ ٣٩",
    "text_simple": "أم لكم أيمان علينا بالغة إلى يوم القيامة إن لكم لما تحكمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5311,
    "jozz": 29,
    "page": 565,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 40,
    "text_tashkeel": "سَلۡهُمۡ أَيُّهُم بِذَٰلِكَ زَعِيمٌ ٤٠",
    "text_simple": "سلهم أيهم بذلك زعيم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5312,
    "jozz": 29,
    "page": 565,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 41,
    "text_tashkeel": "أَمۡ لَهُمۡ شُرَكَآءُ فَلۡيَأۡتُواْ بِشُرَكَآئِهِمۡ إِن كَانُواْ صَٰدِقِينَ ٤١",
    "text_simple": "أم لهم شركاء فليأتوا بشركائهم إن كانوا صادقين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5313,
    "jozz": 29,
    "page": 565,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 42,
    "text_tashkeel": "يَوۡمَ يُكۡشَفُ عَن سَاقٖ وَيُدۡعَوۡنَ إِلَى ٱلسُّجُودِ فَلَا يَسۡتَطِيعُونَ ٤٢",
    "text_simple": "يوم يكشف عن ساق ويدعون إلى السجود فلا يستطيعون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5314,
    "jozz": 29,
    "page": 566,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 43,
    "text_tashkeel": "خَٰشِعَةً أَبۡصَٰرُهُمۡ تَرۡهَقُهُمۡ ذِلَّةٞۖ وَقَدۡ كَانُواْ يُدۡعَوۡنَ إِلَى ٱلسُّجُودِ وَهُمۡ سَٰلِمُونَ ٤٣",
    "text_simple": "خاشعة أبصارهم ترهقهم ذلة وقد كانوا يدعون إلى السجود وهم سالمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5315,
    "jozz": 29,
    "page": 566,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 44,
    "text_tashkeel": "فَذَرۡنِي وَمَن يُكَذِّبُ بِهَٰذَا ٱلۡحَدِيثِۖ سَنَسۡتَدۡرِجُهُم مِّنۡ حَيۡثُ لَا يَعۡلَمُونَ ٤٤",
    "text_simple": "فذرني ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5316,
    "jozz": 29,
    "page": 566,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 45,
    "text_tashkeel": "وَأُمۡلِي لَهُمۡۚ إِنَّ كَيۡدِي مَتِينٌ ٤٥",
    "text_simple": "وأملي لهم إن كيدي متين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5317,
    "jozz": 29,
    "page": 566,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 46,
    "text_tashkeel": "أَمۡ تَسۡـَٔلُهُمۡ أَجۡرٗا فَهُم مِّن مَّغۡرَمٖ مُّثۡقَلُونَ ٤٦",
    "text_simple": "أم تسألهم أجرا فهم من مغرم مثقلون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5318,
    "jozz": 29,
    "page": 566,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 47,
    "text_tashkeel": "أَمۡ عِندَهُمُ ٱلۡغَيۡبُ فَهُمۡ يَكۡتُبُونَ ٤٧",
    "text_simple": "أم عندهم الغيب فهم يكتبون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5319,
    "jozz": 29,
    "page": 566,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 48,
    "text_tashkeel": "فَٱصۡبِرۡ لِحُكۡمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلۡحُوتِ إِذۡ نَادَىٰ وَهُوَ مَكۡظُومٞ ٤٨",
    "text_simple": "فاصبر لحكم ربك ولا تكن كصاحب الحوت إذ نادى وهو مكظوم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5320,
    "jozz": 29,
    "page": 566,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 49,
    "text_tashkeel": "لَّوۡلَآ أَن تَدَٰرَكَهُۥ نِعۡمَةٞ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلۡعَرَآءِ وَهُوَ مَذۡمُومٞ ٤٩",
    "text_simple": "لولا أن تداركه نعمة من ربه لنبذ بالعراء وهو مذموم",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5321,
    "jozz": 29,
    "page": 566,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 50,
    "text_tashkeel": "فَٱجۡتَبَٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّٰلِحِينَ ٥٠",
    "text_simple": "فاجتباه ربه فجعله من الصالحين",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
  {
    "id": 5322,
    "jozz": 29,
    "page": 566,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 51,
    "text_tashkeel": "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُواْ لَيُزۡلِقُونَكَ بِأَبۡصَٰرِهِمۡ لَمَّا سَمِعُواْ ٱلذِّكۡرَ وَيَقُولُونَ إِنَّهُۥ لَمَجۡنُونٞ ٥١",
    "text_simple": "وإن يكاد الذين كفروا ليزلقونك بأبصارهم لما سمعوا الذكر ويقولون إنه لمجنون",
    "sura_no": 68,
    "sura_name_en": "Al-Qalam",
    "sura_name_ar": "القَلَم"
  },
