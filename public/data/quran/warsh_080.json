[
  {
    "id": 5734,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "عَبَسَ وَتَوَلّ۪ىٰٓ ١",
    "text_simple": "عبس وتولىٓ"
  },
  {
    "id": 5735,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "أَن جَآءَهُ اُ۬لَاعْم۪ىٰۖ ٢",
    "text_simple": "أن جآءه الاعمى"
  },
  {
    "id": 5736,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "text_tashkeel": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكّ۪ىٰٓ ٣",
    "text_simple": "وما يدريك لعله يزكىٓ"
  },
  {
    "id": 5737,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "أَوْ يَذَّكَّرُ فَتَنفَعُهُ اُ۬لذِّكْر۪ىٰٓۖ ٤",
    "text_simple": "أو يذكر فتنفعه الذكرىٓ"
  },
  {
    "id": 5738,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "أَمَّا مَنِ اِ۪سْتَغْن۪ىٰ ٥",
    "text_simple": "أما من استغنى"
  },
  {
    "id": 5739,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 6,
    "text_tashkeel": "فَأَنتَ لَهُۥ تَصَّدّ۪ىٰ ٦",
    "text_simple": "فأنت له تصدى"
  },
  {
    "id": 5740,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "وَمَا عَلَيْكَ أَلَّا يَزَّكّ۪ىٰۖ ٧",
    "text_simple": "وما عليك ألا يزكى"
  },
  {
    "id": 5741,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 8,
    "text_tashkeel": "وَأَمَّا مَن جَآءَكَ يَسْع۪ىٰ ٨",
    "text_simple": "وأما من جآءك يسعى"
  },
  {
    "id": 5742,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 9,
    "text_tashkeel": "وَهُوَ يَخْش۪ىٰ ٩",
    "text_simple": "وهو يخشى"
  },
  {
    "id": 5743,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 10,
    "text_tashkeel": "فَأَنتَ عَنْهُ تَلَهّ۪ىٰ ١٠",
    "text_simple": "فأنت عنه تلهى"
  },
  {
    "id": 5744,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 11,
    "text_tashkeel": " كَلَّآۖ إِنَّهَا تَذْكِرَةٞۖ ١١",
    "text_simple": "كلآ إنها تذكرةٞ"
  },
  {
    "id": 5745,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 12,
    "text_tashkeel": "فَمَن شَآءَ ذَكَرَهُۥ ١٢",
    "text_simple": "فمن شآء ذكره"
  },
  {
    "id": 5746,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 13,
    "text_tashkeel": "فِے صُحُفٖ مُّكَرَّمَةٖ ١٣",
    "text_simple": "فے صحفٖ مكرمةٖ"
  },
  {
    "id": 5747,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 14,
    "text_tashkeel": "مَّرْفُوعَةٖ مُّطَهَّرَةِۢ ١٤",
    "text_simple": "مرفوعةٖ مطهرة"
  },
  {
    "id": 5748,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 15,
    "text_tashkeel": "بِأَيْدِے سَفَرَةٖ ١٥",
    "text_simple": "بأيدے سفرةٖ"
  },
  {
    "id": 5749,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 16,
    "text_tashkeel": "كِرَامِۢ بَرَرَةٖۖ ١٦",
    "text_simple": "كرام بررةٖ"
  },
  {
    "id": 5750,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 17,
    "text_tashkeel": "قُتِلَ اَ۬لِانسَٰنُ مَآ أَكْفَرَهُۥۖ ١٧",
    "text_simple": "قتل الانسن مآ أكفره"
  },
  {
    "id": 5751,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 18,
    "text_tashkeel": " مِنَ اَيِّ شَےْءٍ خَلَقَهُۥۖ ١٨",
    "text_simple": "من اي شےء خلقه"
  },
  {
    "id": 5752,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 19,
    "text_tashkeel": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ ١٩",
    "text_simple": "من نطفة خلقه فقدره"
  },
  {
    "id": 5753,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 20,
    "text_tashkeel": "ثُمَّ اَ۬لسَّبِيلَ يَسَّرَهُۥ ٢٠",
    "text_simple": "ثم السبيل يسره"
  },
  {
    "id": 5754,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 21,
    "text_tashkeel": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥۖ ٢١",
    "text_simple": "ثم أماته فأقبره"
  },
  {
    "id": 5755,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 22,
    "text_tashkeel": "ثُمَّ إِذَا شَآءَ انشَرَهُۥۖ ٢٢",
    "text_simple": "ثم إذا شآء انشره"
  },
  {
    "id": 5756,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 23,
    "text_tashkeel": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥۖ ٢٣",
    "text_simple": "كلا لما يقض مآ أمره"
  },
  {
    "id": 5757,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 24,
    "text_tashkeel": "فَلْيَنظُرِ اِ۬لِانسَٰنُ إِلَىٰ طَعَامِهِۦٓۖ ٢٤",
    "text_simple": "فلينظر الانسن إلى طعامهٓ"
  },
  {
    "id": 5758,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 25,
    "text_tashkeel": "إِنَّا صَبَبْنَا اَ۬لْمَآءَ صَبّاٗ ٢٥",
    "text_simple": "إنا صببنا المآء صباٗ"
  },
  {
    "id": 5759,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 26,
    "text_tashkeel": "ثُمَّ شَقَقْنَا اَ۬لَارْضَ شَقّاٗ ٢٦",
    "text_simple": "ثم شققنا الارض شقاٗ"
  },
  {
    "id": 5760,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 27,
    "text_tashkeel": "فَأَنۢبَتْنَا فِيهَا حَبّاٗ ٢٧",
    "text_simple": "فأنبتنا فيها حباٗ"
  },
  {
    "id": 5761,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 28,
    "text_tashkeel": "وَعِنَباٗ وَقَضْباٗ ٢٨",
    "text_simple": "وعنباٗ وقضباٗ"
  },
  {
    "id": 5762,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 29,
    "text_tashkeel": "وَزَيْتُوناٗ وَنَخْلاٗ ٢٩",
    "text_simple": "وزيتوناٗ ونخلاٗ"
  },
  {
    "id": 5763,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 30,
    "text_tashkeel": "وَحَدَآئِقَ غُلْباٗ ٣٠",
    "text_simple": "وحدآئق غلباٗ"
  },
  {
    "id": 5764,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 31,
    "text_tashkeel": "وَفَٰكِهَةٗ وَأَبّاٗ ٣١",
    "text_simple": "وفكهةٗ وأباٗ"
  },
  {
    "id": 5765,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 32,
    "text_tashkeel": "مَّتَٰعاٗ لَّكُمْ وَلِأَنْعَٰمِكُمْۖ ٣٢",
    "text_simple": "متعاٗ لكم ولأنعمكم"
  },
  {
    "id": 5766,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 33,
    "text_tashkeel": " فَإِذَا جَآءَتِ اِ۬لصَّآخَّةُ ٣٣",
    "text_simple": "فإذا جآءت الصآخة"
  },
  {
    "id": 5767,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 34,
    "text_tashkeel": "يَوْمَ يَفِرُّ اُ۬لْمَرْءُ مِنَ اَخِيهِ ٣٤",
    "text_simple": "يوم يفر المرء من اخيه"
  },
  {
    "id": 5768,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 35,
    "text_tashkeel": "وَأُمِّهِۦ وَأَبِيهِ ٣٥",
    "text_simple": "وأمه وأبيه"
  },
  {
    "id": 5769,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 36,
    "text_tashkeel": " وَصَٰحِبَتِهِۦ وَبَنِيهِۖ ٣٦",
    "text_simple": "وصحبته وبنيه"
  },
  {
    "id": 5770,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 37,
    "text_tashkeel": "لِكُلِّ اِ۪مْرِےٕٖ مِّنْهُمْ يَوْمَئِذٖ شَأْنٞ يُغْنِيهِۖ ٣٧",
    "text_simple": "لكل امرےٕٖ منهم يومئذٖ شأنٞ يغنيه"
  },
  {
    "id": 5771,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 38,
    "text_tashkeel": "وُجُوهٞ يَوْمَئِذٖ مُّسْفِرَةٞ ٣٨",
    "text_simple": "وجوهٞ يومئذٖ مسفرةٞ"
  },
  {
    "id": 5772,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 39,
    "text_tashkeel": "ضَاحِكَةٞ مُّسْتَبْشِرَةٞۖ ٣٩",
    "text_simple": "ضاحكةٞ مستبشرةٞ"
  },
  {
    "id": 5773,
    "jozz": 30,
    "page": "585",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 40,
    "text_tashkeel": "وَوُجُوهٞ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٞ ٤٠",
    "text_simple": "ووجوهٞ يومئذ عليها غبرةٞ"
  },
  {
    "id": 5774,
    "jozz": 30,
    "page": "586",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 41,
    "text_tashkeel": " تَرْهَقُهَا قَتَرَةٌۖ ٤١",
    "text_simple": "ترهقها قترة"
  },
  {
    "id": 5775,
    "jozz": 30,
    "page": "586",
    "sura_no": 80,
    "sura_name_en": "‘Abasa",
    "sura_name_ar": "عَبَسَ ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 42,
    "text_tashkeel": "ا۟وْلَٰٓئِكَ هُمُ اُ۬لْكَفَرَةُ اُ۬لْفَجَرَةُۖ ٤٢",
    "text_simple": "اولٓئك هم الكفرة الفجرة"
  }
]