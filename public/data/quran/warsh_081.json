[
  {
    "id": 5776,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 1,
    "text_tashkeel": "إِذَا اَ۬لشَّمْسُ كُوِّرَتْ ١",
    "text_simple": "إذا الشمس كورت"
  },
  {
    "id": 5777,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 2,
    "text_tashkeel": "وَإِذَا اَ۬لنُّجُومُ اُ۪نكَدَرَتْ ٢",
    "text_simple": "وإذا النجوم انكدرت"
  },
  {
    "id": 5778,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا اَ۬لْجِبَالُ سُيِّرَتْ ٣",
    "text_simple": "وإذا الجبال سيرت"
  },
  {
    "id": 5779,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": "وَإِذَا اَ۬لْعِشَارُ عُطِّلَتْ ٤",
    "text_simple": "وإذا العشار عطلت"
  },
  {
    "id": 5780,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "text_tashkeel": "وَإِذَا اَ۬لْوُحُوشُ حُشِرَتْ ٥",
    "text_simple": "وإذا الوحوش حشرت"
  },
  {
    "id": 5781,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": " وَإِذَا اَ۬لْبِحَارُ سُجِّرَتْ ٦",
    "text_simple": "وإذا البحار سجرت"
  },
  {
    "id": 5782,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "وَإِذَا اَ۬لنُّفُوسُ زُوِّجَتْ ٧",
    "text_simple": "وإذا النفوس زوجت"
  },
  {
    "id": 5783,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "وَإِذَا اَ۬لْمَوْءُۥدَةُ سُئِلَتْ ٨",
    "text_simple": "وإذا الموءدة سئلت"
  },
  {
    "id": 5784,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 9,
    "text_tashkeel": "بِأَيِّ ذَنۢبٖ قُتِلَتْ ٩",
    "text_simple": "بأي ذنبٖ قتلت"
  },
  {
    "id": 5785,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 10,
    "text_tashkeel": "وَإِذَا اَ۬لصُّحُفُ نُشِرَتْ ١٠",
    "text_simple": "وإذا الصحف نشرت"
  },
  {
    "id": 5786,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 11,
    "text_tashkeel": "وَإِذَا اَ۬لسَّمَآءُ كُشِطَتْ ١١",
    "text_simple": "وإذا السمآء كشطت"
  },
  {
    "id": 5787,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 12,
    "text_tashkeel": "وَإِذَا اَ۬لْجَحِيمُ سُعِّرَتْ ١٢",
    "text_simple": "وإذا الجحيم سعرت"
  },
  {
    "id": 5788,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 13,
    "text_tashkeel": "وَإِذَا اَ۬لْجَنَّةُ أُزْلِفَتْ ١٣",
    "text_simple": "وإذا الجنة أزلفت"
  },
  {
    "id": 5789,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 14,
    "text_tashkeel": " عَلِمَتْ نَفْسٞ مَّآ أَحْضَرَتْۖ ١٤",
    "text_simple": "علمت نفسٞ مآ أحضرت"
  },
  {
    "id": 5790,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 15,
    "text_tashkeel": "فَلَآ أُقْسِمُ بِالْخُنَّسِ ١٥",
    "text_simple": "فلآ أقسم بالخنس"
  },
  {
    "id": 5791,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 16,
    "text_tashkeel": "اِ۬لْجَوَارِ اِ۬لْكُنَّسِ ١٦",
    "text_simple": "الجوار الكنس"
  },
  {
    "id": 5792,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 17,
    "text_tashkeel": "وَاليْلِ إِذَا عَسْعَسَ ١٧",
    "text_simple": "واليل إذا عسعس"
  },
  {
    "id": 5793,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 18,
    "text_tashkeel": "وَالصُّبْحِ إِذَا تَنَفَّسَ ١٨",
    "text_simple": "والصبح إذا تنفس"
  },
  {
    "id": 5794,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 19,
    "text_tashkeel": "إِنَّهُۥ لَقَوْلُ رَسُولٖ كَرِيمٖ ١٩",
    "text_simple": "إنه لقول رسولٖ كريمٖ"
  },
  {
    "id": 5795,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 20,
    "text_tashkeel": "ذِے قُوَّةٍ عِندَ ذِے اِ۬لْعَرْشِ مَكِينٖ ٢٠",
    "text_simple": "ذے قوة عند ذے العرش مكينٖ"
  },
  {
    "id": 5796,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 21,
    "text_tashkeel": "مُّطَاعٖ ثَمَّ أَمِينٖۖ ٢١",
    "text_simple": "مطاعٖ ثم أمينٖ"
  },
  {
    "id": 5797,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 22,
    "text_tashkeel": "وَمَا صَٰحِبُكُم بِمَجْنُونٖۖ ٢٢",
    "text_simple": "وما صحبكم بمجنونٖ"
  },
  {
    "id": 5798,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 23,
    "text_tashkeel": "وَلَقَدْ ر۪ء۪اهُ بِالُافُقِ اِ۬لْمُبِينِۖ ٢٣",
    "text_simple": "ولقد رءاه بالافق المبين"
  },
  {
    "id": 5799,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 24,
    "text_tashkeel": " وَمَا هُوَ عَلَى اَ۬لْغَيْبِ بِضَنِينٖۖ ٢٤",
    "text_simple": "وما هو على الغيب بضنينٖ"
  },
  {
    "id": 5800,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 25,
    "text_tashkeel": "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٖ رَّجِيمٖ ٢٥",
    "text_simple": "وما هو بقول شيطنٖ رجيمٖ"
  },
  {
    "id": 5801,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 26,
    "text_tashkeel": " فَأَيْنَ تَذْهَبُونَۖ ٢٦",
    "text_simple": "فأين تذهبون"
  },
  {
    "id": 5802,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 27,
    "text_tashkeel": "إِنْ هُوَ إِلَّا ذِكْرٞ لِّلْعَٰلَمِينَ ٢٧",
    "text_simple": "إن هو إلا ذكرٞ للعلمين"
  },
  {
    "id": 5803,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 28,
    "text_tashkeel": "لِمَن شَآءَ مِنكُمُۥٓ أَنْ يَّسْتَقِيمَۖ ٢٨",
    "text_simple": "لمن شآء منكمٓ أن يستقيم"
  },
  {
    "id": 5804,
    "jozz": 30,
    "page": "586",
    "sura_no": 81,
    "sura_name_en": "At-Takwīr",
    "sura_name_ar": "التَّكوير ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 29,
    "text_tashkeel": "وَمَا تَشَآءُونَ إِلَّآ أَنْ يَّشَآءَ اَ۬للَّهُ رَبُّ اُ۬لْعَٰلَمِينَۖ ٢٩",
    "text_simple": "وما تشآءون إلآ أن يشآء الله رب العلمين"
  }
]