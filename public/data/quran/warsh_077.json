[
  {
    "id": 5599,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 1,
    "text_tashkeel": "وَالْمُرْسَلَٰتِ عُرْفاٗ ١",
    "text_simple": "والمرسلت عرفاٗ"
  },
  {
    "id": 5600,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 2,
    "text_tashkeel": "فَالْعَٰصِفَٰتِ عَصْفاٗ ٢",
    "text_simple": "فالعصفت عصفاٗ"
  },
  {
    "id": 5601,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": "وَالنَّٰشِرَٰتِ نَشْراٗ ٣",
    "text_simple": "والنشرت نشراٗ"
  },
  {
    "id": 5602,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 4,
    "text_tashkeel": " فَالْفَٰرِقَٰتِ فَرْقاٗ ٤",
    "text_simple": "فالفرقت فرقاٗ"
  },
  {
    "id": 5603,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "فَالْمُلْقِيَٰتِ ذِكْراً ٥",
    "text_simple": "فالملقيت ذكرا"
  },
  {
    "id": 5604,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "عُذْراً اَوْ نُذُراً ٦",
    "text_simple": "عذرا او نذرا"
  },
  {
    "id": 5605,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "اِنَّمَا تُوعَدُونَ لَوَٰقِعٞۖ ٧",
    "text_simple": "انما توعدون لوقعٞ"
  },
  {
    "id": 5606,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "فَإِذَا اَ۬لنُّجُومُ طُمِسَتْ ٨",
    "text_simple": "فإذا النجوم طمست"
  },
  {
    "id": 5607,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "وَإِذَا اَ۬لسَّمَآءُ فُرِجَتْ ٩",
    "text_simple": "وإذا السمآء فرجت"
  },
  {
    "id": 5608,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": " وَإِذَا اَ۬لْجِبَالُ نُسِفَتْ ١٠",
    "text_simple": "وإذا الجبال نسفت"
  },
  {
    "id": 5609,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "وَإِذَا اَ۬لرُّسُلُ أُقِّتَتْ ١١",
    "text_simple": "وإذا الرسل أقتت"
  },
  {
    "id": 5610,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "لِأَيِّ يَوْمٍ ا۟جِّلَتْ ١٢",
    "text_simple": "لأي يوم اجلت"
  },
  {
    "id": 5611,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 13,
    "text_tashkeel": " لِيَوْمِ اِ۬لْفَصْلِۖ ١٣",
    "text_simple": "ليوم الفصل"
  },
  {
    "id": 5612,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 14,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا يَوْمُ اُ۬لْفَصْلِۖ ١٤",
    "text_simple": "ومآ أدريك ما يوم الفصل"
  },
  {
    "id": 5613,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 15,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ١٥",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5614,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 16,
    "text_tashkeel": "۞ أَلَمْ نُهْلِكِ اِ۬لَاوَّلِينَۖ ١٦",
    "text_simple": "ألم نهلك الاولين"
  },
  {
    "id": 5615,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 17,
    "text_tashkeel": "ثُمَّ نُتْبِعُهُمُ اُ۬لَاخِرِينَۖ ١٧",
    "text_simple": "ثم نتبعهم الاخرين"
  },
  {
    "id": 5616,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": " كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَۖ ١٨",
    "text_simple": "كذلك نفعل بالمجرمين"
  },
  {
    "id": 5617,
    "jozz": 29,
    "page": "580",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 19,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ١٩",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5618,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": " أَلَمْ نَخْلُقكُّم مِّن مَّآءٖ مَّهِينٖ ٢٠",
    "text_simple": "ألم نخلقكم من مآءٖ مهينٖ"
  },
  {
    "id": 5619,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 21,
    "text_tashkeel": "فَجَعَلْنَٰهُ فِے قَر۪ارٖ مَّكِينٍ ٢١",
    "text_simple": "فجعلنه فے قرارٖ مكين"
  },
  {
    "id": 5620,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 22,
    "text_tashkeel": "اِلَىٰ قَدَرٖ مَّعْلُومٖ ٢٢",
    "text_simple": "الى قدرٖ معلومٖ"
  },
  {
    "id": 5621,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "فَقَدَّرْنَاۖ فَنِعْمَ اَ۬لْقَٰدِرُونَۖ ٢٣",
    "text_simple": "فقدرنا فنعم القدرون"
  },
  {
    "id": 5622,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 24,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ٢٤",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5623,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": " أَلَمْ نَجْعَلِ اِ۬لَارْضَ كِفَاتاً ٢٥",
    "text_simple": "ألم نجعل الارض كفاتا"
  },
  {
    "id": 5624,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 26,
    "text_tashkeel": "اَحْيَآءٗ وَأَمْوَٰتاٗ ٢٦",
    "text_simple": "احيآءٗ وأموتاٗ"
  },
  {
    "id": 5625,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 27,
    "text_tashkeel": "وَجَعَلْنَا فِيهَا رَوَٰسِيَ شَٰمِخَٰتٖ وَأَسْقَيْنَٰكُم مَّآءٗ فُرَاتاٗۖ ٢٧",
    "text_simple": "وجعلنا فيها روسي شمختٖ وأسقينكم مآءٗ فراتاٗ"
  },
  {
    "id": 5626,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ٢٨",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5627,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 29,
    "text_tashkeel": " اَ۪نطَلِقُوٓاْ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ ٢٩",
    "text_simple": "انطلقوٓا إلى ما كنتم به تكذبون"
  },
  {
    "id": 5628,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 30,
    "text_tashkeel": "اَ۪نطَلِقُوٓاْ إِلَىٰ ظِلّٖ ذِے ثَلَٰثِ شُعَبٖ ٣٠",
    "text_simple": "انطلقوٓا إلى ظلٖ ذے ثلث شعبٖ"
  },
  {
    "id": 5629,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 31,
    "text_tashkeel": "لَّا ظَلِيلٖ وَلَا يُغْنِے مِنَ اَ۬للَّهَبِۖ ٣١",
    "text_simple": "لا ظليلٖ ولا يغنے من اللهب"
  },
  {
    "id": 5630,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 32,
    "text_tashkeel": "إِنَّهَا تَرْمِے بِشَرَرٖ كَالْقَصْرِ ٣٢",
    "text_simple": "إنها ترمے بشررٖ كالقصر"
  },
  {
    "id": 5631,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 33,
    "text_tashkeel": "كَأَنَّهُۥ جِمَٰلَٰتٞ صُفْرٞۖ ٣٣",
    "text_simple": "كأنه جملتٞ صفرٞ"
  },
  {
    "id": 5632,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 34,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ٣٤",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5633,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 35,
    "text_tashkeel": " هَٰذَا يَوْمُ لَا يَنطِقُونَ ٣٥",
    "text_simple": "هذا يوم لا ينطقون"
  },
  {
    "id": 5634,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 36,
    "text_tashkeel": "وَلَا يُوذَنُ لَهُمْ فَيَعْتَذِرُونَۖ ٣٦",
    "text_simple": "ولا يوذن لهم فيعتذرون"
  },
  {
    "id": 5635,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 37,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ٣٧",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5636,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 38,
    "text_tashkeel": "هَٰذَا يَوْمُ اُ۬لْفَصْلِ جَمَعْنَٰكُمْ وَالَاوَّلِينَ ٣٨",
    "text_simple": "هذا يوم الفصل جمعنكم والاولين"
  },
  {
    "id": 5637,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 39,
    "text_tashkeel": "فَإِن كَانَ لَكُمْ كَيْدٞ فَكِيدُونِۖ ٣٩",
    "text_simple": "فإن كان لكم كيدٞ فكيدون"
  },
  {
    "id": 5638,
    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 40,
    "text_tashkeel": "وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَۖ ٤٠",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
