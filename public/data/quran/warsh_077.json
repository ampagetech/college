    "jozz": 29,
    "page": "581",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 42,
    "text_tashkeel": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَۖ ٤٢",
    "text_simple": "وفوكه مما يشتهون"
  },
  {
