    "jozz": 27,
    "page": 535,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 43,
    "text_tashkeel": "وَظِلّٖ مِّن يَحۡمُومٖ ٤٣",
    "text_simple": "وظل من يحموم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5023,
    "jozz": 27,
    "page": 535,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 44,
    "text_tashkeel": "لَّا بَارِدٖ وَلَا كَرِيمٍ ٤٤",
    "text_simple": "لا بارد ولا كريم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5024,
    "jozz": 27,
    "page": 535,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 45,
    "text_tashkeel": "إِنَّهُمۡ كَانُواْ قَبۡلَ ذَٰلِكَ مُتۡرَفِينَ ٤٥",
    "text_simple": "إنهم كانوا قبل ذلك مترفين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5025,
    "jozz": 27,
    "page": 535,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 46,
    "text_tashkeel": "وَكَانُواْ يُصِرُّونَ عَلَى ٱلۡحِنثِ ٱلۡعَظِيمِ ٤٦",
    "text_simple": "وكانوا يصرون على الحنث العظيم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5026,
    "jozz": 27,
    "page": 535,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 47,
    "text_tashkeel": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتۡنَا وَكُنَّا تُرَابٗا وَعِظَٰمًا أَءِنَّا لَمَبۡعُوثُونَ ٤٧",
    "text_simple": "وكانوا يقولون أئذا متنا وكنا ترابا وعظاما أئنا لمبعوثون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5027,
    "jozz": 27,
    "page": 535,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 48,
    "text_tashkeel": "أَوَءَابَآؤُنَا ٱلۡأَوَّلُونَ ٤٨",
    "text_simple": "أو آباؤنا الأولون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5028,
    "jozz": 27,
    "page": 535,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 49,
    "text_tashkeel": "قُلۡ إِنَّ ٱلۡأَوَّلِينَ وَٱلۡأٓخِرِينَ ٤٩",
    "text_simple": "قل إن الأولين والآخرين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5029,
    "jozz": 27,
    "page": 535,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 50,
    "text_tashkeel": "لَمَجۡمُوعُونَ إِلَىٰ مِيقَٰتِ يَوۡمٖ مَّعۡلُومٖ ٥٠",
    "text_simple": "لمجموعون إلى ميقات يوم معلوم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5030,
    "jozz": 27,
    "page": 536,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 51,
    "text_tashkeel": "ثُمَّ إِنَّكُمۡ أَيُّهَا ٱلضَّآلُّونَ ٱلۡمُكَذِّبُونَ ٥١",
    "text_simple": "ثم إنكم أيها الضالون المكذبون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5031,
    "jozz": 27,
    "page": 536,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 52,
    "text_tashkeel": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ ٥٢",
    "text_simple": "لآكلون من شجر من زقوم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5032,
    "jozz": 27,
    "page": 536,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 53,
    "text_tashkeel": "فَمَالِـُٔونَ مِنۡهَا ٱلۡبُطُونَ ٥٣",
    "text_simple": "فمالئون منها البطون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5033,
    "jozz": 27,
    "page": 536,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 54,
    "text_tashkeel": "فَشَٰرِبُونَ عَلَيۡهِ مِنَ ٱلۡحَمِيمِ ٥٤",
    "text_simple": "فشاربون عليه من الحميم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5034,
    "jozz": 27,
    "page": 536,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 55,
    "text_tashkeel": "فَشَٰرِبُونَ شُرۡبَ ٱلۡهِيمِ ٥٥",
    "text_simple": "فشاربون شرب الهيم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5035,
    "jozz": 27,
    "page": 536,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 56,
    "text_tashkeel": "هَٰذَا نُزُلُهُمۡ يَوۡمَ ٱلدِّينِ ٥٦",
    "text_simple": "هذا نزلهم يوم الدين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5036,
    "jozz": 27,
    "page": 536,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 57,
    "text_tashkeel": "نَحۡنُ خَلَقۡنَٰكُمۡ فَلَوۡلَا تُصَدِّقُونَ ٥٧",
    "text_simple": "نحن خلقناكم فلولا تصدقون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5037,
    "jozz": 27,
    "page": 536,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 58,
    "text_tashkeel": "أَفَرَءَيۡتُم مَّا تُمۡنُونَ ٥٨",
    "text_simple": "أفرأيتم ما تمنون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5038,
    "jozz": 27,
    "page": 536,
    "line_start": 4,
    "line_end": 5,
    "aya_no": 59,
    "text_tashkeel": "ءَأَنتُمۡ تَخۡلُقُونَهُۥٓ أَمۡ نَحۡنُ ٱلۡخَٰلِقُونَ ٥٩",
    "text_simple": "أأنتم تخلقونه أم نحن الخالقون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5039,
    "jozz": 27,
    "page": 536,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 60,
    "text_tashkeel": "نَحۡنُ قَدَّرۡنَا بَيۡنَكُمُ ٱلۡمَوۡتَ وَمَا نَحۡنُ بِمَسۡبُوقِينَ ٦٠",
    "text_simple": "نحن قدرنا بينكم الموت وما نحن بمسبوقين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5040,
    "jozz": 27,
    "page": 536,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 61,
    "text_tashkeel": "عَلَىٰٓ أَن نُّبَدِّلَ أَمۡثَٰلَكُمۡ وَنُنشِئَكُمۡ فِي مَا لَا تَعۡلَمُونَ ٦١",
    "text_simple": "على أن نبدل أمثالكم وننشئكم في ما لا تعلمون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5041,
    "jozz": 27,
    "page": 536,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 62,
    "text_tashkeel": "وَلَقَدۡ عَلِمۡتُمُ ٱلنَّشۡأَةَ ٱلۡأُولَىٰ فَلَوۡلَا تَذَكَّرُونَ ٦٢",
    "text_simple": "ولقد علمتم النشأة الأولى فلولا تذكرون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5042,
    "jozz": 27,
    "page": 536,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 63,
    "text_tashkeel": "أَفَرَءَيۡتُم مَّا تَحۡرُثُونَ ٦٣",
    "text_simple": "أفرأيتم ما تحرثون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5043,
    "jozz": 27,
    "page": 536,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 64,
    "text_tashkeel": "ءَأَنتُمۡ تَزۡرَعُونَهُۥٓ أَمۡ نَحۡنُ ٱلزَّٰرِعُونَ ٦٤",
    "text_simple": "أأنتم تزرعونه أم نحن الزارعون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5044,
    "jozz": 27,
    "page": 536,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 65,
    "text_tashkeel": "لَوۡ نَشَآءُ لَجَعَلۡنَٰهُ حُطَٰمٗا فَظَلۡتُمۡ تَفَكَّهُونَ ٦٥",
    "text_simple": "لو نشاء لجعلناه حطاما فظلتم تفكهون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5045,
    "jozz": 27,
    "page": 536,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 66,
    "text_tashkeel": "إِنَّا لَمُغۡرَمُونَ ٦٦",
    "text_simple": "إنا لمغرمون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5046,
    "jozz": 27,
    "page": 536,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 67,
    "text_tashkeel": "بَلۡ نَحۡنُ مَحۡرُومُونَ ٦٧",
    "text_simple": "بل نحن محرومون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5047,
    "jozz": 27,
    "page": 536,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 68,
    "text_tashkeel": "أَفَرَءَيۡتُمُ ٱلۡمَآءَ ٱلَّذِي تَشۡرَبُونَ ٦٨",
    "text_simple": "أفرأيتم الماء الذي تشربون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5048,
    "jozz": 27,
    "page": 536,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 69,
    "text_tashkeel": "ءَأَنتُمۡ أَنزَلۡتُمُوهُ مِنَ ٱلۡمُزۡنِ أَمۡ نَحۡنُ ٱلۡمُنزِلُونَ ٦٩",
    "text_simple": "أأنتم أنزلتموه من المزن أم نحن المنزلون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5049,
    "jozz": 27,
    "page": 536,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 70,
    "text_tashkeel": "لَوۡ نَشَآءُ جَعَلۡنَٰهُ أُجَاجٗا فَلَوۡلَا تَشۡكُرُونَ ٧٠",
    "text_simple": "لو نشاء جعلناه أجاجا فلولا تشكرون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5050,
    "jozz": 27,
    "page": 536,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 71,
    "text_tashkeel": "أَفَرَءَيۡتُمُ ٱلنَّارَ ٱلَّتِي تُورُونَ ٧١",
    "text_simple": "أفرأيتم النار التي تورون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5051,
    "jozz": 27,
    "page": 536,
    "line_start": 12,
    "line_end": 13,
    "aya_no": 72,
    "text_tashkeel": "ءَأَنتُمۡ أَنشَأۡتُمۡ شَجَرَتَهَآ أَمۡ نَحۡنُ ٱلۡمُنشِـُٔونَ ٧٢",
    "text_simple": "أأنتم أنشأتم شجرتها أم نحن المنشئون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5052,
    "jozz": 27,
    "page": 536,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 73,
    "text_tashkeel": "نَحۡنُ جَعَلۡنَٰهَا تَذۡكِرَةٗ وَمَتَٰعٗا لِّلۡمُقۡوِينَ ٧٣",
    "text_simple": "نحن جعلناها تذكرة ومتاعا للمقوين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5053,
    "jozz": 27,
    "page": 536,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 74,
    "text_tashkeel": "فَسَبِّحۡ بِٱسۡمِ رَبِّكَ ٱلۡعَظِيمِ ٧٤",
    "text_simple": "فسبح باسم ربك العظيم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5054,
    "jozz": 27,
    "page": 536,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 75,
    "text_tashkeel": "۞ فَلَآ أُقۡسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ ٧٥",
    "text_simple": "فلا أقسم بمواقع النجوم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5055,
    "jozz": 27,
    "page": 536,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 76,
    "text_tashkeel": "وَإِنَّهُۥ لَقَسَمٞ لَّوۡ تَعۡلَمُونَ عَظِيمٌ ٧٦",
    "text_simple": "وإنه لقسم لو تعلمون عظيم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5056,
    "jozz": 27,
    "page": 537,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 77,
    "text_tashkeel": "إِنَّهُۥ لَقُرۡءَانٞ كَرِيمٞ ٧٧",
    "text_simple": "إنه لقرآن كريم",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5057,
    "jozz": 27,
    "page": 537,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 78,
    "text_tashkeel": "فِي كِتَٰبٖ مَّكۡنُونٖ ٧٨",
    "text_simple": "في كتاب مكنون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5058,
    "jozz": 27,
    "page": 537,
    "line_start": 1,
    "line_end": 2,
    "aya_no": 79,
    "text_tashkeel": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلۡمُطَهَّرُونَ ٧٩",
    "text_simple": "لا يمسه إلا المطهرون",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5059,
    "jozz": 27,
    "page": 537,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 80,
    "text_tashkeel": "تَنزِيلٞ مِّن رَّبِّ ٱلۡعَٰلَمِينَ ٨٠",
    "text_simple": "تنزيل من رب العالمين",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة"
  },
  {
    "id": 5060,
    "jozz": 27,
