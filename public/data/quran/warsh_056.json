[
  {
    "id": 4956,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 1,
    "text_tashkeel": "إِذَا وَقَعَتِ اِ۬لْوَاقِعَةُ ١",
    "text_simple": "إذا وقعت الواقعة"
  },
  {
    "id": 4957,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 2,
    "text_tashkeel": "لَيْسَ لِوَقْعَتِهَا كَٰذِبَةٌۖ ٢",
    "text_simple": "ليس لوقعتها كذبة"
  },
  {
    "id": 4958,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": "خَافِضَةٞ رَّافِعَةٌ ٣",
    "text_simple": "خافضةٞ رافعة"
  },
  {
    "id": 4959,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 4,
    "text_tashkeel": " اِذَا رُجَّتِ اِ۬لَارْضُ رَجّاٗ ٤",
    "text_simple": "اذا رجت الارض رجاٗ"
  },
  {
    "id": 4960,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "وَبُسَّتِ اِ۬لْجِبَالُ بَسّاٗ ٥",
    "text_simple": "وبست الجبال بساٗ"
  },
  {
    "id": 4961,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 6,
    "text_tashkeel": "فَكَانَتْ هَبَآءٗ مُّنۢبَثّاٗ ٦",
    "text_simple": "فكانت هبآءٗ منبثاٗ"
  },
  {
    "id": 4962,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "وَكُنتُمُۥٓ أَزْوَٰجاٗ ثَلَٰثَةٗۖ ٧",
    "text_simple": "وكنتمٓ أزوجاٗ ثلثةٗ"
  },
  {
    "id": 4963,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "فَأَصْحَٰبُ اُ۬لْمَيْمَنَةِ ٨",
    "text_simple": "فأصحب الميمنة"
  },
  {
    "id": 4964,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 9,
    "text_tashkeel": " مَآ أَصْحَٰبُ اُ۬لْمَيْمَنَةِۖ ٩",
    "text_simple": "مآ أصحب الميمنة"
  },
  {
    "id": 4965,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "وَأَصْحَٰبُ اُ۬لْمَشْـَٔمَةِ ١٠",
    "text_simple": "وأصحب المشٔمة"
  },
  {
    "id": 4966,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 11,
    "text_tashkeel": "مَآ أَصْحَٰبُ اُ۬لْمَشْـَٔمَةِۖ ١١",
    "text_simple": "مآ أصحب المشٔمة"
  },
  {
    "id": 4967,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 12,
    "text_tashkeel": "وَالسَّٰبِقُونَ اَ۬لسَّٰبِقُونَۖ ١٢",
    "text_simple": "والسبقون السبقون"
  },
  {
    "id": 4968,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 13,
    "text_tashkeel": "أُوْلَٰٓئِكَ اَ۬لْمُقَرَّبُونَ ١٣",
    "text_simple": "أولٓئك المقربون"
  },
  {
    "id": 4969,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 14,
    "text_tashkeel": " فِے جَنَّٰتِ اِ۬لنَّعِيمِۖ ١٤",
    "text_simple": "فے جنت النعيم"
  },
  {
    "id": 4970,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 15,
    "text_tashkeel": "ثُلَّةٞ مِّنَ اَ۬لَاوَّلِينَ ١٥",
    "text_simple": "ثلةٞ من الاولين"
  },
  {
    "id": 4971,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 16,
    "text_tashkeel": "وَقَلِيلٞ مِّنَ اَ۬لَاخِرِينَ ١٦",
    "text_simple": "وقليلٞ من الاخرين"
  },
  {
    "id": 4972,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 17,
    "text_tashkeel": " عَلَىٰ سُرُرٖ مَّوْضُونَةٖ ١٧",
    "text_simple": "على سررٖ موضونةٖ"
  },
  {
    "id": 4973,
    "jozz": 27,
    "page": "534",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": "مُّتَّكِـِٕينَ عَلَيْهَا مُتَقَٰبِلِينَۖ ١٨",
    "text_simple": "متكٕين عليها متقبلين"
  },
  {
    "id": 4974,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 19,
    "text_tashkeel": " يَطُوفُ عَلَيْهِمْ وِلْدَٰنٞ مُّخَلَّدُونَ ١٩",
    "text_simple": "يطوف عليهم ولدنٞ مخلدون"
  },
  {
    "id": 4975,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": "بِأَكْوَابٖ وَأَبَارِيقَ ٢٠",
    "text_simple": "بأكوابٖ وأباريق"
  },
  {
    "id": 4976,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 21,
    "text_tashkeel": "وَكَأْسٖ مِّن مَّعِينٖ ٢١",
    "text_simple": "وكأسٖ من معينٖ"
  },
  {
    "id": 4977,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 22,
    "text_tashkeel": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزَفُونَ ٢٢",
    "text_simple": "لا يصدعون عنها ولا ينزفون"
  },
  {
    "id": 4978,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 23,
    "text_tashkeel": "وَفَٰكِهَةٖ مِّمَّا يَتَخَيَّرُونَ ٢٣",
    "text_simple": "وفكهةٖ مما يتخيرون"
  },
  {
    "id": 4979,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 24,
    "text_tashkeel": "وَلَحْمِ طَيْرٖ مِّمَّا يَشْتَهُونَۖ ٢٤",
    "text_simple": "ولحم طيرٖ مما يشتهون"
  },
  {
    "id": 4980,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 25,
    "text_tashkeel": "وَحُورٌ عِينٞ كَأَمْثَٰلِ اِ۬للُّؤْلُوِٕ اِ۬لْمَكْنُونِ ٢٥",
    "text_simple": "وحور عينٞ كأمثل اللؤلوٕ المكنون"
  },
  {
    "id": 4981,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 26,
    "text_tashkeel": "جَزَآءَۢ بِمَا كَانُواْ يَعْمَلُونَۖ ٢٦",
    "text_simple": "جزآء بما كانوا يعملون"
  },
  {
    "id": 4982,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 27,
    "text_tashkeel": "لَا يَسْمَعُونَ فِيهَا لَغْواٗ وَلَا تَاثِيماًۖ ٢٧",
    "text_simple": "لا يسمعون فيها لغواٗ ولا تاثيما"
  },
  {
    "id": 4983,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 28,
    "text_tashkeel": "اِلَّا قِيلاٗ سَلَٰماٗ سَلَٰماٗۖ ٢٨",
    "text_simple": "الا قيلاٗ سلماٗ سلماٗ"
  },
  {
    "id": 4984,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 29,
    "text_tashkeel": "وَأَصْحَٰبُ اُ۬لْيَمِينِ مَآ أَصْحَٰبُ اُ۬لْيَمِينِۖ ٢٩",
    "text_simple": "وأصحب اليمين مآ أصحب اليمين"
  },
  {
    "id": 4985,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 30,
    "text_tashkeel": "فِے سِدْرٖ مَّخْضُودٖ ٣٠",
    "text_simple": "فے سدرٖ مخضودٖ"
  },
  {
    "id": 4986,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 31,
    "text_tashkeel": "وَطَلْحٖ مَّنضُودٖ ٣١",
    "text_simple": "وطلحٖ منضودٖ"
  },
  {
    "id": 4987,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 32,
    "text_tashkeel": "وَظِلّٖ مَّمْدُودٖ ٣٢",
    "text_simple": "وظلٖ ممدودٖ"
  },
  {
    "id": 4988,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 33,
    "text_tashkeel": "وَمَآءٖ مَّسْكُوبٖ ٣٣",
    "text_simple": "ومآءٖ مسكوبٖ"
  },
  {
    "id": 4989,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 34,
    "text_tashkeel": "وَفَٰكِهَةٖ كَثِيرَةٖ ٣٤",
    "text_simple": "وفكهةٖ كثيرةٖ"
  },
  {
    "id": 4990,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 35,
    "text_tashkeel": "لَّا مَقْطُوعَةٖ وَلَا مَمْنُوعَةٖ ٣٥",
    "text_simple": "لا مقطوعةٖ ولا ممنوعةٖ"
  },
  {
    "id": 4991,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 36,
    "text_tashkeel": "وَفُرُشٖ مَّرْفُوعَةٍۖ ٣٦",
    "text_simple": "وفرشٖ مرفوعة"
  },
  {
    "id": 4992,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 37,
    "text_tashkeel": "اِنَّآ أَنشَأْنَٰهُنَّ إِنشَآءٗ ٣٧",
    "text_simple": "انآ أنشأنهن إنشآءٗ"
  },
  {
    "id": 4993,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 38,
    "text_tashkeel": " فَجَعَلْنَٰهُنَّ أَبْكَاراً ٣٨",
    "text_simple": "فجعلنهن أبكارا"
  },
  {
    "id": 4994,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 39,
    "text_tashkeel": "عُرُباً اَتْرَاباٗ ٣٩",
    "text_simple": "عربا اتراباٗ"
  },
  {
    "id": 4995,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 40,
    "text_tashkeel": "لِّأَصْحَٰبِ اِ۬لْيَمِينِۖ ٤٠",
    "text_simple": "لأصحب اليمين"
  },
  {
    "id": 4996,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 41,
    "text_tashkeel": "ثُلَّةٞ مِّنَ اَ۬لَاوَّلِينَ ٤١",
    "text_simple": "ثلةٞ من الاولين"
  },
  {
    "id": 4997,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 42,
    "text_tashkeel": "وَثُلَّةٞ مِّنَ اَ۬لَاخِرِينَۖ ٤٢",
    "text_simple": "وثلةٞ من الاخرين"
  },
  {
    "id": 4998,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 43,
    "text_tashkeel": "وَأَصْحَٰبُ اُ۬لشِّمَالِ ٤٣",
    "text_simple": "وأصحب الشمال"
  },
  {
    "id": 4999,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 44,
    "text_tashkeel": " مَآ أَصْحَٰبُ اُ۬لشِّمَالِۖ ٤٤",
    "text_simple": "مآ أصحب الشمال"
  },
  {
    "id": 5000,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 45,
    "text_tashkeel": "فِے سَمُومٖ وَحَمِيمٖ ٤٥",
    "text_simple": "فے سمومٖ وحميمٖ"
  },
  {
    "id": 5001,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 46,
    "text_tashkeel": "وَظِلّٖ مِّنْ يَّحْمُومٖ ٤٦",
    "text_simple": "وظلٖ من يحمومٖ"
  },
  {
    "id": 5002,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 47,
    "text_tashkeel": " لَّا بَارِدٖ وَلَا كَرِيمٍۖ ٤٧",
    "text_simple": "لا باردٖ ولا كريم"
  },
  {
    "id": 5003,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 48,
    "text_tashkeel": "اِنَّهُمْ كَانُواْ قَبْلَ ذَٰلِكَ مُتْرَفِينَۖ ٤٨",
    "text_simple": "انهم كانوا قبل ذلك مترفين"
  },
  {
    "id": 5004,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 49,
    "text_tashkeel": "وَكَانُواْ يُصِرُّونَ عَلَى اَ۬لْحِنثِ اِ۬لْعَظِيمِۖ ٤٩",
    "text_simple": "وكانوا يصرون على الحنث العظيم"
  },
  {
    "id": 5005,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 50,
    "text_tashkeel": "وَكَانُواْ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَاباٗ وَعِظَٰماً اِنَّا لَمَبْعُوثُونَ ٥٠",
    "text_simple": "وكانوا يقولون أئذا متنا وكنا تراباٗ وعظما انا لمبعوثون"
  },
  {
    "id": 5006,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 51,
    "text_tashkeel": "أَوَءَابَآؤُنَا اَ۬لَاوَّلُونَۖ ٥١",
    "text_simple": "أوءابآؤنا الاولون"
  },
  {
    "id": 5007,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 52,
    "text_tashkeel": "۞ قُلِ اِنَّ اَ۬لَاوَّلِينَ وَالَاخِرِينَ لَمَجْمُوعُونَ ٥٢",
    "text_simple": "قل ان الاولين والاخرين لمجموعون"
  },
  {
    "id": 5008,
    "jozz": 27,
    "page": "535",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 53,
    "text_tashkeel": "إِلَىٰ مِيقَٰتِ يَوْمٖ مَّعْلُومٖۖ ٥٣",
    "text_simple": "إلى ميقت يومٖ معلومٖ"
  },
  {
    "id": 5009,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 54,
    "text_tashkeel": " ثُمَّ إِنَّكُمُۥٓ أَيُّهَا اَ۬لضَّآلُّونَ اَ۬لْمُكَذِّبُونَ ٥٤",
    "text_simple": "ثم إنكمٓ أيها الضآلون المكذبون"
  },
  {
    "id": 5010,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 55,
    "text_tashkeel": "لَأٓكِلُونَ مِن شَجَرٖ مِّن زَقُّومٖ ٥٥",
    "text_simple": "لأٓكلون من شجرٖ من زقومٖ"
  },
  {
    "id": 5011,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 56,
    "text_tashkeel": "فَمَالِـُٔونَ مِنْهَا اَ۬لْبُطُونَ ٥٦",
    "text_simple": "فمالٔون منها البطون"
  },
  {
    "id": 5012,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 57,
    "text_tashkeel": "فَشَٰرِبُونَ عَلَيْهِ مِنَ اَ۬لْحَمِيمِ ٥٧",
    "text_simple": "فشربون عليه من الحميم"
  },
  {
    "id": 5013,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 58,
    "text_tashkeel": " فَشَٰرِبُونَ شُرْبَ اَ۬لْهِيمِۖ ٥٨",
    "text_simple": "فشربون شرب الهيم"
  },
  {
    "id": 5014,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 59,
    "text_tashkeel": "هَٰذَا نُزُلُهُمْ يَوْمَ اَ۬لدِّينِۖ ٥٩",
    "text_simple": "هذا نزلهم يوم الدين"
  },
  {
    "id": 5015,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 60,
    "text_tashkeel": "نَحْنُ خَلَقْنَٰكُمْۖ فَلَوْلَا تُصَدِّقُونَۖ ٦٠",
    "text_simple": "نحن خلقنكم فلولا تصدقون"
  },
  {
    "id": 5016,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 61,
    "text_tashkeel": "أَفَرَٰٓيْتُم مَّا تُمْنُونَ ٦١",
    "text_simple": "أفرٓيتم ما تمنون"
  },
  {
    "id": 5017,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 62,
    "text_tashkeel": "ءَآنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ اُ۬لْخَٰلِقُونَۖ ٦٢",
    "text_simple": "ءآنتم تخلقونهٓ أم نحن الخلقون"
  },
  {
    "id": 5018,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 63,
    "text_tashkeel": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ اُ۬لْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ ٦٣",
    "text_simple": "نحن قدرنا بينكم الموت وما نحن بمسبوقين"
  },
  {
    "id": 5019,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 64,
    "text_tashkeel": " عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَٰلَكُمْ وَنُنشِئَكُمْ فِے مَا لَا تَعْلَمُونَۖ ٦٤",
    "text_simple": "علىٓ أن نبدل أمثلكم وننشئكم فے ما لا تعلمون"
  },
  {
    "id": 5020,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 65,
    "text_tashkeel": "وَلَقَدْ عَلِمْتُمُ اُ۬لنَّشْأَةَ اَ۬لُاول۪ىٰۖ فَلَوْلَا تَذَّكَّرُونَۖ ٦٥",
    "text_simple": "ولقد علمتم النشأة الاولى فلولا تذكرون"
  },
  {
    "id": 5021,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 66,
    "text_tashkeel": "أَفَرَٰٓيْتُم مَّا تَحْرُثُونَ ٦٦",
    "text_simple": "أفرٓيتم ما تحرثون"
  },
  {
    "id": 5022,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 67,
    "text_tashkeel": " ءَآنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ اُ۬لزَّٰرِعُونَۖ ٦٧",
    "text_simple": "ءآنتم تزرعونهٓ أم نحن الزرعون"
  },
  {
    "id": 5023,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 68,
    "text_tashkeel": "لَوْ نَشَآءُ لَجَعَلْنَٰهُ حُطَٰماٗ فَظَلْتُمْ تَفَكَّهُونَ ٦٨",
    "text_simple": "لو نشآء لجعلنه حطماٗ فظلتم تفكهون"
  },
  {
    "id": 5024,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 69,
    "text_tashkeel": "إِنَّا لَمُغْرَمُونَ ٦٩",
    "text_simple": "إنا لمغرمون"
  },
  {
    "id": 5025,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 70,
    "text_tashkeel": "بَلْ نَحْنُ مَحْرُومُونَۖ ٧٠",
    "text_simple": "بل نحن محرومون"
  },
  {
    "id": 5026,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 71,
    "text_tashkeel": "أَفَرَٰٓيْتُمُ اُ۬لْمَآءَ اَ۬لذِے تَشْرَبُونَ ٧١",
    "text_simple": "أفرٓيتم المآء الذے تشربون"
  },
  {
    "id": 5027,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 72,
    "text_tashkeel": "ءَآنتُمُۥٓ أَنزَلْتُمُوهُ مِنَ اَ۬لْمُزْنِ أَمْ نَحْنُ اُ۬لْمُنزِلُونَۖ ٧٢",
    "text_simple": "ءآنتمٓ أنزلتموه من المزن أم نحن المنزلون"
  },
  {
    "id": 5028,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 73,
    "text_tashkeel": "لَوْ نَشَآءُ جَعَلْنَٰهُ أُجَاجاٗۖ فَلَوْلَا تَشْكُرُونَۖ ٧٣",
    "text_simple": "لو نشآء جعلنه أجاجاٗ فلولا تشكرون"
  },
  {
    "id": 5029,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 74,
    "text_tashkeel": "أَفَرَٰٓيْتُمُ اُ۬لنَّارَ اَ۬لتِے تُورُونَ ٧٤",
    "text_simple": "أفرٓيتم النار التے تورون"
  },
  {
    "id": 5030,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 75,
    "text_tashkeel": "ءَآنتُمُۥٓ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ اُ۬لْمُنشِـُٔونَۖ ٧٥",
    "text_simple": "ءآنتمٓ أنشأتم شجرتهآ أم نحن المنشٔون"
  },
  {
    "id": 5031,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 76,
    "text_tashkeel": "نَحْنُ جَعَلْنَٰهَا تَذْكِرَةٗ وَمَتَٰعاٗ لِّلْمُقْوِينَۖ ٧٦",
    "text_simple": "نحن جعلنها تذكرةٗ ومتعاٗ للمقوين"
  },
  {
    "id": 5032,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 77,
    "text_tashkeel": "فَسَبِّحْ بِاسْمِ رَبِّكَ اَ۬لْعَظِيمِۖ ٧٧",
    "text_simple": "فسبح باسم ربك العظيم"
  },
  {
    "id": 5033,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 78,
    "text_tashkeel": "۞ فَلَآ أُقْسِمُ بِمَوَٰقِعِ اِ۬لنُّجُومِ ٧٨",
    "text_simple": "فلآ أقسم بموقع النجوم"
  },
  {
    "id": 5034,
    "jozz": 27,
    "page": "536",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 79,
    "text_tashkeel": "وَإِنَّهُۥ لَقَسَمٞ لَّوْ تَعْلَمُونَ عَظِيمٌ ٧٩",
    "text_simple": "وإنه لقسمٞ لو تعلمون عظيم"
  },
  {
    "id": 5035,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 80,
    "text_tashkeel": " اِنَّهُۥ لَقُرْءَانٞ كَرِيمٞ ٨٠",
    "text_simple": "انه لقرءانٞ كريمٞ"
  },
  {
    "id": 5036,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 81,
    "text_tashkeel": "فِے كِتَٰبٖ مَّكْنُونٖ ٨١",
    "text_simple": "فے كتبٖ مكنونٖ"
  },
  {
    "id": 5037,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 82,
    "text_tashkeel": "لَّا يَمَسُّهُۥٓ إِلَّا اَ۬لْمُطَهَّرُونَۖ ٨٢",
    "text_simple": "لا يمسهٓ إلا المطهرون"
  },
  {
    "id": 5038,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 83,
    "text_tashkeel": "تَنزِيلٞ مِّن رَّبِّ اِ۬لْعَٰلَمِينَۖ ٨٣",
    "text_simple": "تنزيلٞ من رب العلمين"
  },
  {
    "id": 5039,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 84,
    "text_tashkeel": "أَفَبِهَٰذَا اَ۬لْحَدِيثِ أَنتُم مُّدْهِنُونَ ٨٤",
    "text_simple": "أفبهذا الحديث أنتم مدهنون"
  },
  {
    "id": 5040,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 85,
    "text_tashkeel": "وَتَجْعَلُونَ رِزْقَكُمُۥٓ أَنَّكُمْ تُكَذِّبُونَۖ ٨٥",
    "text_simple": "وتجعلون رزقكمٓ أنكم تكذبون"
  },
  {
    "id": 5041,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 86,
    "text_tashkeel": "فَلَوْلَآ إِذَا بَلَغَتِ اِ۬لْحُلْقُومَ ٨٦",
    "text_simple": "فلولآ إذا بلغت الحلقوم"
  },
  {
    "id": 5042,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 87,
    "text_tashkeel": "وَأَنتُمْ حِينَئِذٖ تَنظُرُونَ ٨٧",
    "text_simple": "وأنتم حينئذٖ تنظرون"
  },
  {
    "id": 5043,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 88,
    "text_tashkeel": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَٰكِن لَّا تُبْصِرُونَ ٨٨",
    "text_simple": "ونحن أقرب إليه منكم ولكن لا تبصرون"
  },
  {
    "id": 5044,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 89,
    "text_tashkeel": "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ ٨٩",
    "text_simple": "فلولآ إن كنتم غير مدينين"
  },
  {
    "id": 5045,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 90,
    "text_tashkeel": " تَرْجِعُونَهَآ إِن كُنتُمْ صَٰدِقِينَۖ ٩٠",
    "text_simple": "ترجعونهآ إن كنتم صدقين"
  },
  {
    "id": 5046,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 91,
    "text_tashkeel": "فَأَمَّآ إِن كَانَ مِنَ اَ۬لْمُقَرَّبِينَ ٩١",
    "text_simple": "فأمآ إن كان من المقربين"
  },
  {
    "id": 5047,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 92,
    "text_tashkeel": " فَرَوْحٞ وَرَيْحَانٞ وَجَنَّتُ نَعِيمٖۖ ٩٢",
    "text_simple": "فروحٞ وريحانٞ وجنت نعيمٖ"
  },
  {
    "id": 5048,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 93,
    "text_tashkeel": "وَأَمَّآ إِن كَانَ مِنَ اَصْحَٰبِ اِ۬لْيَمِينِ ٩٣",
    "text_simple": "وأمآ إن كان من اصحب اليمين"
  },
  {
    "id": 5049,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 94,
    "text_tashkeel": "فَسَلَٰمٞ لَّكَ مِنَ اَصْحَٰبِ اِ۬لْيَمِينِۖ ٩٤",
    "text_simple": "فسلمٞ لك من اصحب اليمين"
  },
  {
    "id": 5050,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 95,
    "text_tashkeel": "وَأَمَّآ إِن كَانَ مِنَ اَ۬لْمُكَذِّبِينَ اَ۬لضَّآلِّينَ ٩٥",
    "text_simple": "وأمآ إن كان من المكذبين الضآلين"
  },
  {
    "id": 5051,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 96,
    "text_tashkeel": "فَنُزُلٞ مِّنْ حَمِيمٖ ٩٦",
    "text_simple": "فنزلٞ من حميمٖ"
  },
  {
    "id": 5052,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 97,
    "text_tashkeel": "وَتَصْلِيَةُ جَحِيمٍۖ ٩٧",
    "text_simple": "وتصلية جحيم"
  },
  {
    "id": 5053,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 98,
    "text_tashkeel": " اِنَّ هَٰذَا لَهُوَ حَقُّ اُ۬لْيَقِينِۖ ٩٨",
    "text_simple": "ان هذا لهو حق اليقين"
  },
  {
    "id": 5054,
    "jozz": 27,
    "page": "537",
    "sura_no": 56,
    "sura_name_en": "Al-Wāqi‘ah",
    "sura_name_ar": "الوَاقِعة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 99,
    "text_tashkeel": "فَسَبِّحْ بِاسْمِ رَبِّكَ اَ۬لْعَظِيمِۖ ٩٩",
    "text_simple": "فسبح باسم ربك العظيم"
  }
]