    "jozz": 30,
    "page": 583,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 6,
    "text_tashkeel": "يَوۡمَ تَرۡجُفُ ٱلرَّاجِفَةُ ٦",
    "text_simple": "يوم ترجف الراجفة",
    "sura_no": 79,
    "sura_name_en": "An-Nāzi‘āt",
    "sura_name_ar": "النَّازعَات"
  },
  {
