    "aya_no": 1,
    "text_tashkeel": "وَالضُّح۪ىٰ ١",
    "text_simple": "والضحى"
  },
  {
    "id": 6058,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "وَاليْلِ إِذَا سَج۪ىٰ ٢",
    "text_simple": "واليل إذا سجى"
  },
  {
    "id": 6059,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 3,
    "text_tashkeel": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَل۪ىٰۖ ٣",
    "text_simple": "ما ودعك ربك وما قلى"
  },
  {
    "id": 6060,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": " وَلَلَاخِرَةُ خَيْرٞ لَّكَ مِنَ اَ۬لُاول۪ىٰۖ ٤",
    "text_simple": "وللاخرة خيرٞ لك من الاولى"
  },
  {
    "id": 6061,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْض۪ىٰٓۖ ٥",
    "text_simple": "ولسوف يعطيك ربك فترضىٓ"
  },
  {
    "id": 6062,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "أَلَمْ يَجِدْكَ يَتِيماٗ فَـَٔاو۪ىٰ ٦",
    "text_simple": "ألم يجدك يتيماٗ فٔاوى"
  },
  {
    "id": 6063,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "وَوَجَدَكَ ضَآلّاٗ فَهَد۪ىٰ ٧",
    "text_simple": "ووجدك ضآلاٗ فهدى"
  },
  {
    "id": 6064,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "وَوَجَدَكَ عَآئِلاٗ فَأَغْن۪ىٰۖ ٨",
    "text_simple": "ووجدك عآئلاٗ فأغنى"
  },
  {
    "id": 6065,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "فَأَمَّا اَ۬لْيَتِيمَ فَلَا تَقْهَرْۖ ٩",
    "text_simple": "فأما اليتيم فلا تقهر"
  },
  {
    "id": 6066,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": " وَأَمَّا اَ۬لسَّآئِلَ فَلَا تَنْهَرْۖ ١٠",
    "text_simple": "وأما السآئل فلا تنهر"
  },
  {
    "id": 6067,
    "jozz": 30,
    "page": "596",
    "sura_no": 93,
    "sura_name_en": "Aḍ-Ḍuḥā",
    "sura_name_ar": "الضُّحى ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثَۖ ١١",
    "text_simple": "وأما بنعمة ربك فحدث"
  }
]