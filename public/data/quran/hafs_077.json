    "jozz": 29,
    "page": 580,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "وَإِذَا ٱلسَّمَآءُ فُرِجَتۡ ٩",
    "text_simple": "وإذا السماء فرجت",
    "sura_no": 77,
    "sura_name_en": "Al-Mursalāt",
    "sura_name_ar": "المُرسَلات"
  },
  {
