[
  {
    "id": 4785,
    "jozz": 27,
    "page": 526,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "وَٱلنَّجۡمِ إِذَا هَوَىٰ ١",
    "text_simple": "والنجم إذا هوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4786,
    "jozz": 27,
    "page": 526,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "مَا ضَلَّ صَاحِبُكُمۡ وَمَا غَوَىٰ ٢",
    "text_simple": "ما ضل صاحبكم وما غوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4787,
    "jozz": 27,
    "page": 526,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَمَا يَنطِقُ عَنِ ٱلۡهَوَىٰٓ ٣",
    "text_simple": "وما ينطق عن الهوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4788,
    "jozz": 27,
    "page": 526,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "إِنۡ هُوَ إِلَّا وَحۡيٞ يُوحَىٰ ٤",
    "text_simple": "إن هو إلا وحي يوحى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4789,
    "jozz": 27,
    "page": 526,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "عَلَّمَهُۥ شَدِيدُ ٱلۡقُوَىٰ ٥",
    "text_simple": "علمه شديد القوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4790,
    "jozz": 27,
    "page": 526,
    "line_start": 4,
    "line_end": 5,
    "aya_no": 6,
    "text_tashkeel": "ذُو مِرَّةٖ فَٱسۡتَوَىٰ ٦",
    "text_simple": "ذو مرة فاستوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4791,
    "jozz": 27,
    "page": 526,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "وَهُوَ بِٱلۡأُفُقِ ٱلۡأَعۡلَىٰ ٧",
    "text_simple": "وهو بالأفق الأعلى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4792,
    "jozz": 27,
    "page": 526,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 8,
    "text_tashkeel": "ثُمَّ دَنَا فَتَدَلَّىٰ ٨",
    "text_simple": "ثم دنا فتدلى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4793,
    "jozz": 27,
    "page": 526,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 9,
    "text_tashkeel": "فَكَانَ قَابَ قَوۡسَيۡنِ أَوۡ أَدۡنَىٰ ٩",
    "text_simple": "فكان قاب قوسين أو أدنى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4794,
    "jozz": 27,
    "page": 526,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 10,
    "text_tashkeel": "فَأَوۡحَىٰٓ إِلَىٰ عَبۡدِهِۦ مَآ أَوۡحَىٰ ١٠",
    "text_simple": "فأوحى إلى عبده ما أوحى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4795,
    "jozz": 27,
    "page": 526,
    "line_start": 6,
    "line_end": 7,
    "aya_no": 11,
    "text_tashkeel": "مَا كَذَبَ ٱلۡفُؤَادُ مَا رَأَىٰٓ ١١",
    "text_simple": "ما كذب الفؤاد ما رأى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4796,
    "jozz": 27,
    "page": 526,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 12,
    "text_tashkeel": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ ١٢",
    "text_simple": "أفتمارونه على ما يرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4797,
    "jozz": 27,
    "page": 526,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 13,
    "text_tashkeel": "وَلَقَدۡ رَءَاهُ نَزۡلَةً أُخۡرَىٰ ١٣",
    "text_simple": "ولقد رآه نزلة أخرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4798,
    "jozz": 27,
    "page": 526,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 14,
    "text_tashkeel": "عِندَ سِدۡرَةِ ٱلۡمُنتَهَىٰ ١٤",
    "text_simple": "عند سدرة المنتهى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4799,
    "jozz": 27,
    "page": 526,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 15,
    "text_tashkeel": "عِندَهَا جَنَّةُ ٱلۡمَأۡوَىٰٓ ١٥",
    "text_simple": "عندها جنة المأوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4800,
    "jozz": 27,
    "page": 526,
    "line_start": 8,
    "line_end": 9,
    "aya_no": 16,
    "text_tashkeel": "إِذۡ يَغۡشَى ٱلسِّدۡرَةَ مَا يَغۡشَىٰ ١٦",
    "text_simple": "إذ يغشى السدرة ما يغشى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4801,
    "jozz": 27,
    "page": 526,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 17,
    "text_tashkeel": "مَا زَاغَ ٱلۡبَصَرُ وَمَا طَغَىٰ ١٧",
    "text_simple": "ما زاغ البصر وما طغى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4802,
    "jozz": 27,
    "page": 526,
    "line_start": 9,
    "line_end": 10,
    "aya_no": 18,
    "text_tashkeel": "لَقَدۡ رَأَىٰ مِنۡ ءَايَٰتِ رَبِّهِ ٱلۡكُبۡرَىٰٓ ١٨",
    "text_simple": "لقد رأى من آيات ربه الكبرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4803,
    "jozz": 27,
    "page": 526,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 19,
    "text_tashkeel": "أَفَرَءَيۡتُمُ ٱللَّٰتَ وَٱلۡعُزَّىٰ ١٩",
    "text_simple": "أفرأيتم اللات والعزى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4804,
    "jozz": 27,
    "page": 526,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 20,
    "text_tashkeel": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلۡأُخۡرَىٰٓ ٢٠",
    "text_simple": "ومناة الثالثة الأخرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4805,
    "jozz": 27,
    "page": 526,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 21,
    "text_tashkeel": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلۡأُنثَىٰ ٢١",
    "text_simple": "ألكم الذكر وله الأنثى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4806,
    "jozz": 27,
    "page": 526,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 22,
    "text_tashkeel": "تِلۡكَ إِذٗا قِسۡمَةٞ ضِيزَىٰٓ ٢٢",
    "text_simple": "تلك إذا قسمة ضيزى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4807,
    "jozz": 27,
    "page": 526,
    "line_start": 11,
    "line_end": 13,
    "aya_no": 23,
    "text_tashkeel": "إِنۡ هِيَ إِلَّآ أَسۡمَآءٞ سَمَّيۡتُمُوهَآ أَنتُمۡ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلۡطَٰنٍۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهۡوَى ٱلۡأَنفُسُۖ وَلَقَدۡ جَآءَهُم مِّن رَّبِّهِمُ ٱلۡهُدَىٰٓ ٢٣",
    "text_simple": "إن هي إلا أسماء سميتموها أنتم وآباؤكم ما أنزل الله بها من سلطان إن يتبعون إلا الظن وما تهوى الأنفس ولقد جاءهم من ربهم الهدى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4808,
    "jozz": 27,
    "page": 526,
    "line_start": 13,
    "line_end": 14,
    "aya_no": 24,
    "text_tashkeel": "أَمۡ لِلۡإِنسَٰنِ مَا تَمَنَّىٰ ٢٤",
    "text_simple": "أم للإنسان ما تمنى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4809,
    "jozz": 27,
    "page": 526,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 25,
    "text_tashkeel": "فَلِلَّهِ ٱلۡأٓخِرَةُ وَٱلۡأُولَىٰ ٢٥",
    "text_simple": "فلله الآخرة والأولى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4810,
    "jozz": 27,
    "page": 526,
    "line_start": 14,
    "line_end": 15,
    "aya_no": 26,
    "text_tashkeel": "۞ وَكَم مِّن مَّلَكٖ فِي ٱلسَّمَٰوَٰتِ لَا تُغۡنِي شَفَٰعَتُهُمۡ شَيۡـًٔا إِلَّا مِنۢ بَعۡدِ أَن يَأۡذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرۡضَىٰٓ ٢٦",
    "text_simple": "وكم من ملك في السموات لا تغني شفاعتهم شيئا إلا من بعد أن يأذن الله لمن يشاء ويرضى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4811,
    "jozz": 27,
    "page": 527,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 27,
    "text_tashkeel": "إِنَّ ٱلَّذِينَ لَا يُؤۡمِنُونَ بِٱلۡأٓخِرَةِ لَيُسَمُّونَ ٱلۡمَلَٰٓئِكَةَ تَسۡمِيَةَ ٱلۡأُنثَىٰ ٢٧",
    "text_simple": "إن الذين لا يؤمنون بالآخرة ليسمون الملائكة تسمية الأنثى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4812,
    "jozz": 27,
    "page": 527,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 28,
    "text_tashkeel": "وَمَا لَهُم بِهِۦ مِنۡ عِلۡمٍۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّۖ وَإِنَّ ٱلظَّنَّ لَا يُغۡنِي مِنَ ٱلۡحَقِّ شَيۡـٔٗا ٢٨",
    "text_simple": "وما لهم به من علم إن يتبعون إلا الظن وإن الظن لا يغني من الحق شيئا",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4813,
    "jozz": 27,
    "page": 527,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 29,
    "text_tashkeel": "فَأَعۡرِضۡ عَن مَّن تَوَلَّىٰ عَن ذِكۡرِنَا وَلَمۡ يُرِدۡ إِلَّا ٱلۡحَيَوٰةَ ٱلدُّنۡيَا ٢٩",
    "text_simple": "فأعرض عن من تولى عن ذكرنا ولم يرد إلا الحياة الدنيا",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4814,
    "jozz": 27,
    "page": 527,
    "line_start": 4,
    "line_end": 5,
    "aya_no": 30,
    "text_tashkeel": "ذَٰلِكَ مَبۡلَغُهُم مِّنَ ٱلۡعِلۡمِۚ إِنَّ رَبَّكَ هُوَ أَعۡلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعۡلَمُ بِمَنِ ٱهۡتَدَىٰ ٣٠",
    "text_simple": "ذلك مبلغهم من العلم إن ربك هو أعلم بمن ضل عن سبيله وهو أعلم بمن اهتدى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4815,
    "jozz": 27,
    "page": 527,
    "line_start": 5,
    "line_end": 7,
    "aya_no": 31,
    "text_tashkeel": "وَلِلَّهِ مَا فِي ٱلسَّمَٰوَٰتِ وَمَا فِي ٱلۡأَرۡضِ لِيَجۡزِيَ ٱلَّذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجۡزِيَ ٱلَّذِينَ أَحۡسَنُواْ بِٱلۡحُسۡنَى ٣١",
    "text_simple": "ولله ما في السموات وما في الأرض ليجزي الذين أساءوا بما عملوا ويجزي الذين أحسنوا بالحسنى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4816,
    "jozz": 27,
    "page": 527,
    "line_start": 7,
    "line_end": 10,
    "aya_no": 32,
    "text_tashkeel": "ٱلَّذِينَ يَجۡتَنِبُونَ كَبَٰٓئِرَ ٱلۡإِثۡمِ وَٱلۡفَوَٰحِشَ إِلَّا ٱللَّمَمَۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلۡمَغۡفِرَةِۚ هُوَ أَعۡلَمُ بِكُمۡ إِذۡ أَنشَأَكُم مِّنَ ٱلۡأَرۡضِ وَإِذۡ أَنتُمۡ أَجِنَّةٞ فِي بُطُونِ أُمَّهَٰتِكُمۡۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمۡۖ هُوَ أَعۡلَمُ بِمَنِ ٱتَّقَىٰٓ ٣٢",
    "text_simple": "الذين يجتنبون كبائر الإثم والفواحش إلا اللمم إن ربك واسع المغفرة هو أعلم بكم إذ أنشأكم من الأرض وإذ أنتم أجنة في بطون أمهاتكم فلا تزكوا أنفسكم هو أعلم بمن اتقى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4817,
    "jozz": 27,
    "page": 527,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 33,
    "text_tashkeel": "أَفَرَءَيۡتَ ٱلَّذِي تَوَلَّىٰ ٣٣",
    "text_simple": "أفرأيت الذي تولى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4818,
    "jozz": 27,
    "page": 527,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 34,
    "text_tashkeel": "وَأَعۡطَىٰ قَلِيلٗا وَأَكۡدَىٰٓ ٣٤",
    "text_simple": "وأعطى قليلا وأكدى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4819,
    "jozz": 27,
    "page": 527,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 35,
    "text_tashkeel": "أَعِندَهُۥ عِلۡمُ ٱلۡغَيۡبِ فَهُوَ يَرَىٰٓ ٣٥",
    "text_simple": "أعنده علم الغيب فهو يرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4820,
    "jozz": 27,
    "page": 527,
    "line_start": 11,
    "line_end": 12,
    "aya_no": 36,
    "text_tashkeel": "أَمۡ لَمۡ يُنَبَّأۡ بِمَا فِي صُحُفِ مُوسَىٰ ٣٦",
    "text_simple": "أم لم ينبأ بما في صحف موسى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4821,
    "jozz": 27,
    "page": 527,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 37,
    "text_tashkeel": "وَإِبۡرَٰهِيمَ ٱلَّذِي وَفَّىٰٓ ٣٧",
    "text_simple": "وإبراهيم الذي وفى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4822,
    "jozz": 27,
    "page": 527,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 38,
    "text_tashkeel": "أَلَّا تَزِرُ وَازِرَةٞ وِزۡرَ أُخۡرَىٰ ٣٨",
    "text_simple": "ألا تزر وازرة وزر أخرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4823,
    "jozz": 27,
    "page": 527,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 39,
    "text_tashkeel": "وَأَن لَّيۡسَ لِلۡإِنسَٰنِ إِلَّا مَا سَعَىٰ ٣٩",
    "text_simple": "وأن ليس للإنسان إلا ما سعى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4824,
    "jozz": 27,
    "page": 527,
    "line_start": 13,
    "line_end": 13,
    "aya_no": 40,
    "text_tashkeel": "وَأَنَّ سَعۡيَهُۥ سَوۡفَ يُرَىٰ ٤٠",
    "text_simple": "وأن سعيه سوف يرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4825,
    "jozz": 27,
    "page": 527,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 41,
    "text_tashkeel": "ثُمَّ يُجۡزَىٰهُ ٱلۡجَزَآءَ ٱلۡأَوۡفَىٰ ٤١",
    "text_simple": "ثم يجزاه الجزاء الأوفى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4826,
    "jozz": 27,
    "page": 527,
    "line_start": 14,
    "line_end": 14,
    "aya_no": 42,
    "text_tashkeel": "وَأَنَّ إِلَىٰ رَبِّكَ ٱلۡمُنتَهَىٰ ٤٢",
    "text_simple": "وأن إلى ربك المنتهى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4827,
    "jozz": 27,
    "page": 527,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 43,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ أَضۡحَكَ وَأَبۡكَىٰ ٤٣",
    "text_simple": "وأنه هو أضحك وأبكى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4828,
    "jozz": 27,
    "page": 527,
    "line_start": 15,
    "line_end": 15,
    "aya_no": 44,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحۡيَا ٤٤",
    "text_simple": "وأنه هو أمات وأحيا",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4829,
    "jozz": 27,
    "page": 528,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 45,
    "text_tashkeel": "وَأَنَّهُۥ خَلَقَ ٱلزَّوۡجَيۡنِ ٱلذَّكَرَ وَٱلۡأُنثَىٰ ٤٥",
    "text_simple": "وأنه خلق الزوجين الذكر والأنثى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4830,
    "jozz": 27,
    "page": 528,
    "line_start": 1,
    "line_end": 1,
    "aya_no": 46,
    "text_tashkeel": "مِن نُّطۡفَةٍ إِذَا تُمۡنَىٰ ٤٦",
    "text_simple": "من نطفة إذا تمنى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4831,
    "jozz": 27,
    "page": 528,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 47,
    "text_tashkeel": "وَأَنَّ عَلَيۡهِ ٱلنَّشۡأَةَ ٱلۡأُخۡرَىٰ ٤٧",
    "text_simple": "وأن عليه النشأة الأخرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4832,
    "jozz": 27,
    "page": 528,
    "line_start": 2,
    "line_end": 2,
    "aya_no": 48,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ أَغۡنَىٰ وَأَقۡنَىٰ ٤٨",
    "text_simple": "وأنه هو أغنى وأقنى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4833,
    "jozz": 27,
    "page": 528,
    "line_start": 2,
    "line_end": 3,
    "aya_no": 49,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ رَبُّ ٱلشِّعۡرَىٰ ٤٩",
    "text_simple": "وأنه هو رب الشعرى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4834,
    "jozz": 27,
    "page": 528,
    "line_start": 3,
    "line_end": 3,
    "aya_no": 50,
    "text_tashkeel": "وَأَنَّهُۥٓ أَهۡلَكَ عَادًا ٱلۡأُولَىٰ ٥٠",
    "text_simple": "وأنه أهلك عادا الأولى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4835,
    "jozz": 27,
    "page": 528,
    "line_start": 3,
    "line_end": 4,
    "aya_no": 51,
    "text_tashkeel": "وَثَمُودَاْ فَمَآ أَبۡقَىٰ ٥١",
    "text_simple": "وثمود فما أبقى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4836,
    "jozz": 27,
    "page": 528,
    "line_start": 4,
    "line_end": 4,
    "aya_no": 52,
    "text_tashkeel": "وَقَوۡمَ نُوحٖ مِّن قَبۡلُۖ إِنَّهُمۡ كَانُواْ هُمۡ أَظۡلَمَ وَأَطۡغَىٰ ٥٢",
    "text_simple": "وقوم نوح من قبل إنهم كانوا هم أظلم وأطغى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4837,
    "jozz": 27,
    "page": 528,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 53,
    "text_tashkeel": "وَٱلۡمُؤۡتَفِكَةَ أَهۡوَىٰ ٥٣",
    "text_simple": "والمؤتفكة أهوى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4838,
    "jozz": 27,
    "page": 528,
    "line_start": 5,
    "line_end": 5,
    "aya_no": 54,
    "text_tashkeel": "فَغَشَّىٰهَا مَا غَشَّىٰ ٥٤",
    "text_simple": "فغشاها ما غشى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4839,
    "jozz": 27,
    "page": 528,
    "line_start": 5,
    "line_end": 6,
    "aya_no": 55,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَارَىٰ ٥٥",
    "text_simple": "فبأي آلاء ربك تتمارى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4840,
    "jozz": 27,
    "page": 528,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 56,
    "text_tashkeel": "هَٰذَا نَذِيرٞ مِّنَ ٱلنُّذُرِ ٱلۡأُولَىٰٓ ٥٦",
    "text_simple": "هذا نذير من النذر الأولى",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4841,
    "jozz": 27,
    "page": 528,
    "line_start": 6,
    "line_end": 6,
    "aya_no": 57,
    "text_tashkeel": "أَزِفَتِ ٱلۡأٓزِفَةُ ٥٧",
    "text_simple": "أزفت الآزفة",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4842,
    "jozz": 27,
    "page": 528,
    "line_start": 7,
    "line_end": 7,
    "aya_no": 58,
    "text_tashkeel": "لَيۡسَ لَهَا مِن دُونِ ٱللَّهِ كَاشِفَةٌ ٥٨",
    "text_simple": "ليس لها من دون الله كاشفة",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4843,
    "jozz": 27,
    "page": 528,
    "line_start": 7,
    "line_end": 8,
    "aya_no": 59,
    "text_tashkeel": "أَفَمِنۡ هَٰذَا ٱلۡحَدِيثِ تَعۡجَبُونَ ٥٩",
    "text_simple": "أفمن هذا الحديث تعجبون",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4844,
    "jozz": 27,
    "page": 528,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 60,
    "text_tashkeel": "وَتَضۡحَكُونَ وَلَا تَبۡكُونَ ٦٠",
    "text_simple": "وتضحكون ولا تبكون",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4845,
    "jozz": 27,
    "page": 528,
    "line_start": 8,
    "line_end": 8,
    "aya_no": 61,
    "text_tashkeel": "وَأَنتُمۡ سَٰمِدُونَ ٦١",
    "text_simple": "وأنتم سامدون",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  },
  {
    "id": 4846,
    "jozz": 27,
    "page": 528,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 62,
    "text_tashkeel": "فَٱسۡجُدُواْۤ لِلَّهِۤ وَٱعۡبُدُواْ۩ ٦٢",
    "text_simple": "فاسجدوا لله واعبدوا",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم"
  }
]