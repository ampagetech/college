[
  {
    "id": 5805,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "إِذَا اَ۬لسَّمَآءُ اُ۪نفَطَرَتْ ١",
    "text_simple": "إذا السمآء انفطرت"
  },
  {
    "id": 5806,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "وَإِذَا اَ۬لْكَوَاكِبُ اُ۪نتَثَرَتْ ٢",
    "text_simple": "وإذا الكواكب انتثرت"
  },
  {
    "id": 5807,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا اَ۬لْبِحَارُ فُجِّرَتْ ٣",
    "text_simple": "وإذا البحار فجرت"
  },
  {
    "id": 5808,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "وَإِذَا اَ۬لْقُبُورُ بُعْثِرَتْ ٤",
    "text_simple": "وإذا القبور بعثرت"
  },
  {
    "id": 5809,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "عَلِمَتْ نَفْسٞ مَّا قَدَّمَتْ وَأَخَّرَتْۖ ٥",
    "text_simple": "علمت نفسٞ ما قدمت وأخرت"
  },
  {
    "id": 5810,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 6,
    "text_tashkeel": " يَٰٓأَيُّهَا اَ۬لِانسَٰنُ مَا غَرَّكَ بِرَبِّكَ اَ۬لْكَرِيمِ ٦",
    "text_simple": "يٓأيها الانسن ما غرك بربك الكريم"
  },
  {
    "id": 5811,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "اِ۬لذِے خَلَقَكَ فَسَوّ۪يٰكَ فَعَدَّلَكَ ٧",
    "text_simple": "الذے خلقك فسويك فعدلك"
  },
  {
    "id": 5812,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 8,
    "text_tashkeel": "فِےٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَۖ ٨",
    "text_simple": "فےٓ أي صورةٖ ما شآء ركبك"
  },
  {
    "id": 5813,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 9,
    "text_tashkeel": " كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ ٩",
    "text_simple": "كلا بل تكذبون بالدين"
  },
  {
    "id": 5814,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 10,
    "text_tashkeel": "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ ١٠",
    "text_simple": "وإن عليكم لحفظين"
  },
  {
    "id": 5815,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 11,
    "text_tashkeel": " كِرَاماٗ كَٰتِبِينَ ١١",
    "text_simple": "كراماٗ كتبين"
  },
  {
    "id": 5816,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 12,
    "text_tashkeel": "يَعْلَمُونَ مَا تَفْعَلُونَۖ ١٢",
    "text_simple": "يعلمون ما تفعلون"
  },
  {
    "id": 5817,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 13,
    "text_tashkeel": "إِنَّ اَ۬لَابْرَارَ لَفِے نَعِيمٖۖ ١٣",
    "text_simple": "إن الابرار لفے نعيمٖ"
  },
  {
    "id": 5818,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 14,
    "text_tashkeel": " وَإِنَّ اَ۬لْفُجَّارَ لَفِے جَحِيمٖ ١٤",
    "text_simple": "وإن الفجار لفے جحيمٖ"
  },
  {
    "id": 5819,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 15,
    "text_tashkeel": "يَصْلَوْنَهَا يَوْمَ اَ۬لدِّينِ ١٥",
    "text_simple": "يصلونها يوم الدين"
  },
  {
    "id": 5820,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 16,
    "text_tashkeel": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَۖ ١٦",
    "text_simple": "وما هم عنها بغآئبين"
  },
  {
    "id": 5821,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 17,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا يَوْمُ اُ۬لدِّينِ ١٧",
    "text_simple": "ومآ أدريك ما يوم الدين"
  },
  {
    "id": 5822,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 18,
    "text_tashkeel": "ثُمَّ مَآ أَدْر۪يٰكَ مَا يَوْمُ اُ۬لدِّينِۖ ١٨",
    "text_simple": "ثم مآ أدريك ما يوم الدين"
  },
  {
    "id": 5823,
    "jozz": 30,
    "page": "587",
    "sura_no": 82,
    "sura_name_en": "Al-Infiṭār",
    "sura_name_ar": "الانفِطَار ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 19,
    "text_tashkeel": "يَوْمَ لَا تَمْلِكُ نَفْسٞ لِّنَفْسٖ شَئْاٗۖ وَالَامْرُ يَوْمَئِذٖ لِّلهِۖ ١٩",
    "text_simple": "يوم لا تملك نفسٞ لنفسٖ شئاٗ والامر يومئذٖ لله"
  }
]