[
  {
    "id": 5924,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 1,
    "text_tashkeel": "سَبِّحِ اِ۪سْمَ رَبِّكَ اَ۬لَاعْلَي ١",
    "text_simple": "سبح اسم ربك الاعلي"
  },
  {
    "id": 5925,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 2,
    "text_tashkeel": "اَ۬لذِے خَلَقَ فَسَوّ۪ىٰ ٢",
    "text_simple": "الذے خلق فسوى"
  },
  {
    "id": 5926,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 3,
    "text_tashkeel": "وَالذِے قَدَّرَ فَهَد۪ىٰ ٣",
    "text_simple": "والذے قدر فهدى"
  },
  {
    "id": 5927,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 4,
    "text_tashkeel": "وَالذِےٓ أَخْرَجَ اَ۬لْمَرْع۪ىٰ ٤",
    "text_simple": "والذےٓ أخرج المرعى"
  },
  {
    "id": 5928,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 5,
    "text_tashkeel": "فَجَعَلَهُۥ غُثَآءً اَحْو۪ىٰۖ ٥",
    "text_simple": "فجعله غثآء احوى"
  },
  {
    "id": 5929,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 6,
    "text_tashkeel": "سَنُقْرِئُكَ فَلَا تَنس۪ىٰٓ ٦",
    "text_simple": "سنقرئك فلا تنسىٓ"
  },
  {
    "id": 5930,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 7,
    "text_tashkeel": "إِلَّا مَا شَآءَ اَ۬للَّهُۖ إِنَّهُۥ يَعْلَمُ اُ۬لْجَهْرَ وَمَا يَخْف۪ىٰۖ ٧",
    "text_simple": "إلا ما شآء الله إنه يعلم الجهر وما يخفى"
  },
  {
    "id": 5931,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 8,
    "text_tashkeel": "وَنُيَسِّرُكَ لِلْيُسْر۪ىٰۖ ٨",
    "text_simple": "ونيسرك لليسرى"
  },
  {
    "id": 5932,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 9,
    "text_tashkeel": "فَذَكِّرِ اِن نَّفَعَتِ اِ۬لذِّكْر۪ىٰۖ ٩",
    "text_simple": "فذكر ان نفعت الذكرى"
  },
  {
    "id": 5933,
    "jozz": 30,
    "page": "591",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 10,
    "text_tashkeel": "سَيَذَّكَّرُ مَنْ يَّخْش۪ىٰ ١٠",
    "text_simple": "سيذكر من يخشى"
  },
  {
    "id": 5934,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 11,
    "text_tashkeel": " وَيَتَجَنَّبُهَا اَ۬لَاشْقَي ١١",
    "text_simple": "ويتجنبها الاشقي"
  },
  {
    "id": 5935,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 12,
    "text_tashkeel": "اَ۬لذِے يَصْلَى اَ۬لنَّارَ اَ۬لْكُبْر۪ىٰ ١٢",
    "text_simple": "الذے يصلى النار الكبرى"
  },
  {
    "id": 5936,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 13,
    "text_tashkeel": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْي۪ىٰۖ ١٣",
    "text_simple": "ثم لا يموت فيها ولا يحيى"
  },
  {
    "id": 5937,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 14,
    "text_tashkeel": "قَدَ اَفْلَحَ مَن تَزَكّ۪ىٰ ١٤",
    "text_simple": "قد افلح من تزكى"
  },
  {
    "id": 5938,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 15,
    "text_tashkeel": "وَذَكَرَ اَ۪سْمَ رَبِّهِۦ فَصَلّ۪ىٰۖ ١٥",
    "text_simple": "وذكر اسم ربه فصلى"
  },
  {
    "id": 5939,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 16,
    "text_tashkeel": " بَلْ تُوثِرُونَ اَ۬لْحَيَوٰةَ اَ۬لدُّنْي۪ا ١٦",
    "text_simple": "بل توثرون الحيوة الدنيا"
  },
  {
    "id": 5940,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 17,
    "text_tashkeel": "وَالَاخِرَةُ خَيْرٞ وَأَبْق۪ىٰٓۖ ١٧",
    "text_simple": "والاخرة خيرٞ وأبقىٓ"
  },
  {
    "id": 5941,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 18,
    "text_tashkeel": "إِنَّ هَٰذَا لَفِے اِ۬لصُّحُفِ اِ۬لُاول۪ىٰ ١٨",
    "text_simple": "إن هذا لفے الصحف الاولى"
  },
  {
    "id": 5942,
    "jozz": 30,
    "page": "592",
    "sura_no": 87,
    "sura_name_en": "Al-A‘lā",
    "sura_name_ar": "الأعلى ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 19,
    "text_tashkeel": "صُحُفِ إِبْرَٰهِيمَ وَمُوس۪ىٰۖ ١٩",
    "text_simple": "صحف إبرهيم وموسى"
  }
]