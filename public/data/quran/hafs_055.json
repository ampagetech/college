  },
  {
    "id": 4909,
    "jozz": 27,
    "page": 531,
    "line_start": 9,
    "line_end": 9,
    "aya_no": 8,
    "text_tashkeel": "أَلَّا تَطۡغَوۡاْ فِي ٱلۡمِيزَانِ ٨",
    "text_simple": "ألا تطغوا في الميزان",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن"
  },
  {
    "id": 4910,
    "jozz": 27,
    "page": 531,
    "line_start": 10,
    "line_end": 10,
    "aya_no": 9,
    "text_tashkeel": "وَأَقِيمُواْ ٱلۡوَزۡنَ بِٱلۡقِسۡطِ وَلَا تُخۡسِرُواْ ٱلۡمِيزَانَ ٩",
    "text_simple": "وأقيموا الوزن بالقسط ولا تخسروا الميزان",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن"
  },
  {
    "id": 4911,
    "jozz": 27,
    "page": 531,
    "line_start": 10,
    "line_end": 11,
    "aya_no": 10,
    "text_tashkeel": "وَٱلۡأَرۡضَ وَضَعَهَا لِلۡأَنَامِ ١٠",
    "text_simple": "والأرض وضعها للأنام",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن"
  },
  {
    "id": 4912,
    "jozz": 27,
    "page": 531,
    "line_start": 11,
    "line_end": 11,
    "aya_no": 11,
    "text_tashkeel": "فِيهَا فَٰكِهَةٞ وَٱلنَّخۡلُ ذَاتُ ٱلۡأَكۡمَامِ ١١",
    "text_simple": "فيها فاكهة والنخل ذات الأكمام",
