    "jozz": 27,
    "page": 532,
    "line_start": 12,
    "line_end": 12,
    "aya_no": 37,
    "text_tashkeel": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتۡ وَرۡدَةٗ كَٱلدِّهَانِ ٣٧",
    "text_simple": "فإذا انشقت السماء فكانت وردة كالدهان",
    "sura_no": 55,
    "sura_name_en": "Ar-Raḥmān",
    "sura_name_ar": "الرَّحمٰن"
  },
  {
    "id": 4939,
