[
  {
    "id": 5860,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 1,
    "text_tashkeel": "إِذَا اَ۬لسَّمَآءُ اُ۪نشَقَّتْ ١",
    "text_simple": "إذا السمآء انشقت"
  },
  {
    "id": 5861,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 2,
    "text_tashkeel": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ ٢",
    "text_simple": "وأذنت لربها وحقت"
  },
  {
    "id": 5862,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا اَ۬لَارْضُ مُدَّتْ ٣",
    "text_simple": "وإذا الارض مدت"
  },
  {
    "id": 5863,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 4,
    "text_tashkeel": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ ٤",
    "text_simple": "وألقت ما فيها وتخلت"
  },
  {
    "id": 5864,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 5,
    "text_tashkeel": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْۖ ٥",
    "text_simple": "وأذنت لربها وحقت"
  },
  {
    "id": 5865,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 6,
    "text_tashkeel": " يَٰٓأَيُّهَا اَ۬لِانسَٰنُ إِنَّكَ كَادِحٌ اِلَىٰ رَبِّكَ كَدْحاٗ فَمُلَٰقِيهِۖ ٦",
    "text_simple": "يٓأيها الانسن إنك كادح الى ربك كدحاٗ فملقيه"
  },
  {
    "id": 5866,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 7,
    "text_tashkeel": "فَأَمَّا مَنُ ا۟وتِيَ كِتَٰبَهُۥ بِيَمِينِهِۦ ٧",
    "text_simple": "فأما من اوتي كتبه بيمينه"
  },
  {
    "id": 5867,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 8,
    "text_tashkeel": "فَسَوْفَ يُحَاسَبُ حِسَاباٗ يَسِيراٗ ٨",
    "text_simple": "فسوف يحاسب حساباٗ يسيراٗ"
  },
  {
    "id": 5868,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 9,
    "text_tashkeel": " وَيَنقَلِبُ إِلَىٰٓ أَهْلِهِۦ مَسْرُوراٗۖ ٩",
    "text_simple": "وينقلب إلىٓ أهله مسروراٗ"
  },
  {
    "id": 5869,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 10,
    "text_tashkeel": "وَأَمَّا مَنُ ا۟وتِيَ كِتَٰبَهُۥ وَرَآءَ ظَهْرِهِۦ ١٠",
    "text_simple": "وأما من اوتي كتبه ورآء ظهره"
  },
  {
    "id": 5870,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 11,
    "text_tashkeel": " فَسَوْفَ يَدْعُواْ ثُبُوراٗ ١١",
    "text_simple": "فسوف يدعوا ثبوراٗ"
  },
  {
    "id": 5871,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 12,
    "text_tashkeel": "وَيُصَلَّىٰ سَعِيراًۖ ١٢",
    "text_simple": "ويصلى سعيرا"
  },
  {
    "id": 5872,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 13,
    "text_tashkeel": "اِنَّهُۥ كَانَ فِےٓ أَهْلِهِۦ مَسْرُوراًۖ ١٣",
    "text_simple": "انه كان فےٓ أهله مسرورا"
  },
  {
    "id": 5873,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 14,
    "text_tashkeel": "اِنَّهُۥ ظَنَّ أَن لَّنْ يَّحُورَ ١٤",
    "text_simple": "انه ظن أن لن يحور"
  },
  {
    "id": 5874,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 15,
    "text_tashkeel": "بَل۪ىٰٓۖ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيراٗۖ ١٥",
    "text_simple": "بلىٓ إن ربه كان به بصيراٗ"
  },
  {
    "id": 5875,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 16,
    "text_tashkeel": " ۞ فَلَآ أُقْسِمُ بِالشَّفَقِ ١٦",
    "text_simple": "فلآ أقسم بالشفق"
  },
  {
    "id": 5876,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 17,
    "text_tashkeel": "وَاليْلِ وَمَا وَسَقَ ١٧",
    "text_simple": "واليل وما وسق"
  },
  {
    "id": 5877,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 18,
    "text_tashkeel": "وَالْقَمَرِ إِذَا اَ۪تَّسَقَ ١٨",
    "text_simple": "والقمر إذا اتسق"
  },
  {
    "id": 5878,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 19,
    "text_tashkeel": "لَتَرْكَبُنَّ طَبَقاً عَن طَبَقٖۖ ١٩",
    "text_simple": "لتركبن طبقا عن طبقٖ"
  },
  {
    "id": 5879,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 20,
    "text_tashkeel": "فَمَا لَهُمْ لَا يُومِنُونَ ٢٠",
    "text_simple": "فما لهم لا يومنون"
  },
  {
    "id": 5880,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 21,
    "text_tashkeel": " وَإِذَا قُرِۓَ عَلَيْهِمُ اُ۬لْقُرْءَانُ لَا يَسْجُدُونَۖ ٢١",
    "text_simple": "وإذا قرۓ عليهم القرءان لا يسجدون"
  },
  {
    "id": 5881,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 22,
    "text_tashkeel": "بَلِ اِ۬لذِينَ كَفَرُواْ يُكَذِّبُونَ ٢٢",
    "text_simple": "بل الذين كفروا يكذبون"
  },
  {
    "id": 5882,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 23,
    "text_tashkeel": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَۖ ٢٣",
    "text_simple": "والله أعلم بما يوعون"
  },
  {
    "id": 5883,
    "jozz": 30,
    "page": "589",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 24,
    "text_tashkeel": "فَبَشِّرْهُم بِعَذَابٍ اَلِيمٍۖ ٢٤",
    "text_simple": "فبشرهم بعذاب اليم"
  },
  {
    "id": 5884,
    "jozz": 30,
    "page": "590",
    "sura_no": 84,
    "sura_name_en": "Al-Inshiqāq",
    "sura_name_ar": "الانشِقَاق ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 25,
    "text_tashkeel": " اِلَّا اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ لَهُمُۥٓ أَجْرٌ غَيْرُ مَمْنُونٖۖ ٢٥",
    "text_simple": "الا الذين ءامنوا وعملوا الصلحت لهمٓ أجر غير ممنونٖ"
  }
]