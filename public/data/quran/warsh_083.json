[
  {
    "id": 5824,
    "jozz": 30,
    "page": "587",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 1,
    "text_tashkeel": "وَيْلٞ لِّلْمُطَفِّفِينَ ١",
    "text_simple": "ويلٞ للمطففين"
  },
  {
    "id": 5825,
    "jozz": 30,
    "page": "587",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 2,
    "text_tashkeel": "اَ۬لذِينَ إِذَا اَ۪كْتَالُواْ عَلَى اَ۬لنَّاسِ يَسْتَوْفُونَ ٢",
    "text_simple": "الذين إذا اكتالوا على الناس يستوفون"
  },
  {
    "id": 5826,
    "jozz": 30,
    "page": "587",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 3,
    "text_tashkeel": "وَإِذَا كَالُوهُمُۥٓ أَو وَّزَنُوهُمْ يُخْسِرُونَۖ ٣",
    "text_simple": "وإذا كالوهمٓ أو وزنوهم يخسرون"
  },
  {
    "id": 5827,
    "jozz": 30,
    "page": "587",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 4,
    "text_tashkeel": "أَلَا يَظُنُّ أُوْلَٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ ٤",
    "text_simple": "ألا يظن أولٓئك أنهم مبعوثون"
  },
  {
    "id": 5828,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 5,
    "text_tashkeel": " لِيَوْمٍ عَظِيمٖ ٥",
    "text_simple": "ليوم عظيمٖ"
  },
  {
    "id": 5829,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 6,
    "text_tashkeel": "يَوْمَ يَقُومُ اُ۬لنَّاسُ لِرَبِّ اِ۬لْعَٰلَمِينَۖ ٦",
    "text_simple": "يوم يقوم الناس لرب العلمين"
  },
  {
    "id": 5830,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 7,
    "text_tashkeel": "كَلَّآ إِنَّ كِتَٰبَ اَ۬لْفُجّ۪ارِ لَفِے سِجِّينٖۖ ٧",
    "text_simple": "كلآ إن كتب الفجار لفے سجينٖ"
  },
  {
    "id": 5831,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 8,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا سِجِّينٞ ٨",
    "text_simple": "ومآ أدريك ما سجينٞ"
  },
  {
    "id": 5832,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 9,
    "text_tashkeel": "كِتَٰبٞ مَّرْقُومٞۖ ٩",
    "text_simple": "كتبٞ مرقومٞ"
  },
  {
    "id": 5833,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 10,
    "text_tashkeel": " وَيْلٞ يَوْمَئِذٖ لِّلْمُكَذِّبِينَ ١٠",
    "text_simple": "ويلٞ يومئذٖ للمكذبين"
  },
  {
    "id": 5834,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 11,
    "text_tashkeel": "اَ۬لذِينَ يُكَذِّبُونَ بِيَوْمِ اِ۬لدِّينِۖ ١١",
    "text_simple": "الذين يكذبون بيوم الدين"
  },
  {
    "id": 5835,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 12,
    "text_tashkeel": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعْتَدٍ اَثِيمٍ ١٢",
    "text_simple": "وما يكذب بهٓ إلا كل معتد اثيم"
  },
  {
    "id": 5836,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 13,
    "text_tashkeel": "اِذَا تُتْل۪ىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ اُ۬لَاوَّلِينَۖ ١٣",
    "text_simple": "اذا تتلى عليه ءايتنا قال أسطير الاولين"
  },
  {
    "id": 5837,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 14,
    "text_tashkeel": "۞ كَلَّا بَل رَّانَ عَلَىٰ قُلُوبِهِم مَّا كَانُواْ يَكْسِبُونَۖ ١٤",
    "text_simple": "كلا بل ران على قلوبهم ما كانوا يكسبون"
  },
  {
    "id": 5838,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 15,
    "text_tashkeel": "كَلَّآ إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٖ لَّمَحْجُوبُونَۖ ١٥",
    "text_simple": "كلآ إنهم عن ربهم يومئذٖ لمحجوبون"
  },
  {
    "id": 5839,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 16,
    "text_tashkeel": "ثُمَّ إِنَّهُمْ لَصَالُواْ اُ۬لْجَحِيمِۖ ١٦",
    "text_simple": "ثم إنهم لصالوا الجحيم"
  },
  {
    "id": 5840,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 17,
    "text_tashkeel": " ثُمَّ يُقَالُ هَٰذَا اَ۬لذِے كُنتُم بِهِۦ تُكَذِّبُونَۖ ١٧",
    "text_simple": "ثم يقال هذا الذے كنتم به تكذبون"
  },
  {
    "id": 5841,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 18,
    "text_tashkeel": "كَلَّآ إِنَّ كِتَٰبَ اَ۬لَابْر۪ارِ لَفِے عِلِّيِّينَۖ ١٨",
    "text_simple": "كلآ إن كتب الابرار لفے عليين"
  },
  {
    "id": 5842,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 19,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا عِلِّيُّونَ ١٩",
    "text_simple": "ومآ أدريك ما عليون"
  },
  {
    "id": 5843,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 20,
    "text_tashkeel": "كِتَٰبٞ مَّرْقُومٞ ٢٠",
    "text_simple": "كتبٞ مرقومٞ"
  },
  {
    "id": 5844,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 21,
    "text_tashkeel": " يَشْهَدُهُ اُ۬لْمُقَرَّبُونَۖ ٢١",
    "text_simple": "يشهده المقربون"
  },
  {
    "id": 5845,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 22,
    "text_tashkeel": "إِنَّ اَ۬لَابْرَارَ لَفِے نَعِيمٍ ٢٢",
    "text_simple": "إن الابرار لفے نعيم"
  },
  {
    "id": 5846,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 23,
    "text_tashkeel": "عَلَى اَ۬لَارَآئِكِ يَنظُرُونَ ٢٣",
    "text_simple": "على الارآئك ينظرون"
  },
  {
    "id": 5847,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 24,
    "text_tashkeel": "تَعْرِفُ فِے وُجُوهِهِمْ نَضْرَةَ اَ۬لنَّعِيمِ ٢٤",
    "text_simple": "تعرف فے وجوههم نضرة النعيم"
  },
  {
    "id": 5848,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 25,
    "text_tashkeel": "يُسْقَوْنَ مِن رَّحِيقٖ مَّخْتُومٍ ٢٥",
    "text_simple": "يسقون من رحيقٖ مختوم"
  },
  {
    "id": 5849,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 26,
    "text_tashkeel": "خِتَٰمُهُۥ مِسْكٞۖ وَفِے ذَٰلِكَ فَلْيَتَنَافَسِ اِ۬لْمُتَنَٰفِسُونَۖ ٢٦",
    "text_simple": "ختمه مسكٞ وفے ذلك فليتنافس المتنفسون"
  },
  {
    "id": 5850,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 27,
    "text_tashkeel": " وَمِزَاجُهُۥ مِن تَسْنِيمٍ ٢٧",
    "text_simple": "ومزاجه من تسنيم"
  },
  {
    "id": 5851,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 28,
    "text_tashkeel": "عَيْناٗ يَشْرَبُ بِهَا اَ۬لْمُقَرَّبُونَۖ ٢٨",
    "text_simple": "عيناٗ يشرب بها المقربون"
  },
  {
    "id": 5852,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 29,
    "text_tashkeel": "إِنَّ اَ۬لذِينَ أَجْرَمُواْ كَانُواْ مِنَ اَ۬لذِينَ ءَامَنُواْ يَضْحَكُونَ ٢٩",
    "text_simple": "إن الذين أجرموا كانوا من الذين ءامنوا يضحكون"
  },
  {
    "id": 5853,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 30,
    "text_tashkeel": "وَإِذَا مَرُّواْ بِهِمْ يَتَغَامَزُونَ ٣٠",
    "text_simple": "وإذا مروا بهم يتغامزون"
  },
  {
    "id": 5854,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 31,
    "text_tashkeel": "وَإِذَا اَ۪نقَلَبُوٓاْ إِلَىٰٓ أَهْلِهِمُ اُ۪نقَلَبُواْ فَٰكِهِينَ ٣١",
    "text_simple": "وإذا انقلبوٓا إلىٓ أهلهم انقلبوا فكهين"
  },
  {
    "id": 5855,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 32,
    "text_tashkeel": "وَإِذَا رَأَوْهُمْ قَالُوٓاْ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ ٣٢",
    "text_simple": "وإذا رأوهم قالوٓا إن هٓؤلآء لضآلون"
  },
  {
    "id": 5856,
    "jozz": 30,
    "page": "588",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 33,
    "text_tashkeel": "وَمَآ أُرْسِلُواْ عَلَيْهِمْ حَٰفِظِينَۖ ٣٣",
    "text_simple": "ومآ أرسلوا عليهم حفظين"
  },
  {
    "id": 5857,
    "jozz": 30,
    "page": "589",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 34,
    "text_tashkeel": " فَالْيَوْمَ اَ۬لذِينَ ءَامَنُواْ مِنَ اَ۬لْكُفّ۪ارِ يَضْحَكُونَ ٣٤",
    "text_simple": "فاليوم الذين ءامنوا من الكفار يضحكون"
  },
  {
    "id": 5858,
    "jozz": 30,
    "page": "589",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 35,
    "text_tashkeel": "عَلَى اَ۬لَارَآئِكِۖ يَنظُرُونَ ٣٥",
    "text_simple": "على الارآئك ينظرون"
  },
  {
    "id": 5859,
    "jozz": 30,
    "page": "589",
    "sura_no": 83,
    "sura_name_en": "Al-Muṭaffifīn",
    "sura_name_ar": "المُطَففين ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 36,
    "text_tashkeel": "هَلْ ثُوِّبَ اَ۬لْكُفَّارُ مَا كَانُواْ يَفْعَلُونَۖ ٣٦",
    "text_simple": "هل ثوب الكفار ما كانوا يفعلون"
  }
]