[
  {
    "id": 6036,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 1,
    "text_tashkeel": "وَاليْلِ إِذَا يَغْش۪ىٰ ١",
    "text_simple": "واليل إذا يغشى"
  },
  {
    "id": 6037,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 2,
    "text_tashkeel": "وَالنَّه۪ارِ إِذَا تَجَلّ۪ىٰ ٢",
    "text_simple": "والنهار إذا تجلى"
  },
  {
    "id": 6038,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 3,
    "text_tashkeel": "وَمَا خَلَقَ اَ۬لذَّكَرَ وَالُانث۪ىٰٓ ٣",
    "text_simple": "وما خلق الذكر والانثىٓ"
  },
  {
    "id": 6039,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 4,
    "text_tashkeel": " إِنَّ سَعْيَكُمْ لَشَتّ۪ىٰۖ ٤",
    "text_simple": "إن سعيكم لشتى"
  },
  {
    "id": 6040,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 5,
    "text_tashkeel": "فَأَمَّا مَنَ اَعْط۪ىٰ وَاتَّق۪ىٰ ٥",
    "text_simple": "فأما من اعطى واتقى"
  },
  {
    "id": 6041,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 6,
    "text_tashkeel": "وَصَدَّقَ بِالْحُسْن۪ىٰ ٦",
    "text_simple": "وصدق بالحسنى"
  },
  {
    "id": 6042,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 7,
    "text_tashkeel": " فَسَنُيَسِّرُهُۥ لِلْيُسْر۪ىٰۖ ٧",
    "text_simple": "فسنيسره لليسرى"
  },
  {
    "id": 6043,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 8,
    "text_tashkeel": "وَأَمَّا مَنۢ بَخِلَ وَاسْتَغْن۪ىٰ ٨",
    "text_simple": "وأما من بخل واستغنى"
  },
  {
    "id": 6044,
    "jozz": 30,
    "page": "595",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 9,
    "text_tashkeel": "وَكَذَّبَ بِالْحُسْن۪ىٰ ٩",
    "text_simple": "وكذب بالحسنى"
  },
  {
    "id": 6045,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 10,
    "text_tashkeel": " فَسَنُيَسِّرُهُۥ لِلْعُسْر۪ىٰۖ ١٠",
    "text_simple": "فسنيسره للعسرى"
  },
  {
    "id": 6046,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 11,
    "text_tashkeel": "وَمَا يُغْنِے عَنْهُ مَالُهُۥٓ إِذَا تَرَدّ۪ىٰٓۖ ١١",
    "text_simple": "وما يغنے عنه مالهٓ إذا تردىٓ"
  },
  {
    "id": 6047,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 12,
    "text_tashkeel": "إِنَّ عَلَيْنَا لَلْهُد۪ىٰ ١٢",
    "text_simple": "إن علينا للهدى"
  },
  {
    "id": 6048,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 13,
    "text_tashkeel": "وَإِنَّ لَنَا لَلَاخِرَةَ وَالُاول۪ىٰۖ ١٣",
    "text_simple": "وإن لنا للاخرة والاولى"
  },
  {
    "id": 6049,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 14,
    "text_tashkeel": "فَأَنذَرْتُكُمْ نَاراٗ تَلَظّ۪ىٰ ١٤",
    "text_simple": "فأنذرتكم ناراٗ تلظى"
  },
  {
    "id": 6050,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 15,
    "text_tashkeel": " لَا يَصْلَيٰهَآ إِلَّا اَ۬لَاشْقَي ١٥",
    "text_simple": "لا يصليهآ إلا الاشقي"
  },
  {
    "id": 6051,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 16,
    "text_tashkeel": "اَ۬لذِے كَذَّبَ وَتَوَلّ۪ىٰۖ ١٦",
    "text_simple": "الذے كذب وتولى"
  },
  {
    "id": 6052,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 17,
    "text_tashkeel": "وَسَيُجَنَّبُهَا اَ۬لَاتْقَي ١٧",
    "text_simple": "وسيجنبها الاتقي"
  },
  {
    "id": 6053,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 18,
    "text_tashkeel": "اَ۬لذِے يُوتِے مَالَهُۥ يَتَزَكّ۪ىٰۖ ١٨",
    "text_simple": "الذے يوتے ماله يتزكى"
  },
  {
    "id": 6054,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 19,
    "text_tashkeel": "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٖ تُجْز۪ىٰٓ ١٩",
    "text_simple": "وما لأحد عنده من نعمةٖ تجزىٓ"
  },
  {
    "id": 6055,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 20,
    "text_tashkeel": "إِلَّا اَ۪بْتِغَآءَ وَجْهِ رَبِّهِ اِ۬لَاعْل۪ىٰۖ ٢٠",
    "text_simple": "إلا ابتغآء وجه ربه الاعلى"
  },
  {
    "id": 6056,
    "jozz": 30,
    "page": "596",
    "sura_no": 92,
    "sura_name_en": "Al-Lail",
    "sura_name_ar": "اللَّيل ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 21,
    "text_tashkeel": "وَلَسَوْفَ يَرْض۪ىٰۖ ٢١",
    "text_simple": "ولسوف يرضى"
  }
]