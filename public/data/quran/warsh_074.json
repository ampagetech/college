[
  {
    "id": 5474,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 1,
    "text_tashkeel": "يَٰٓأَيُّهَا اَ۬لْمُدَّثِّرُ ١",
    "text_simple": "يٓأيها المدثر"
  },
  {
    "id": 5475,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 2,
    "text_tashkeel": "قُمْ فَأَنذِرْۖ ٢",
    "text_simple": "قم فأنذر"
  },
  {
    "id": 5476,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 3,
    "text_tashkeel": "وَرَبَّكَ فَكَبِّرْۖ ٣",
    "text_simple": "وربك فكبر"
  },
  {
    "id": 5477,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 4,
    "text_tashkeel": "وَثِيَابَكَ فَطَهِّرْۖ ٤",
    "text_simple": "وثيابك فطهر"
  },
  {
    "id": 5478,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 5,
    "text_tashkeel": " وَالرِّجْزَ فَاهْجُرْۖ ٥",
    "text_simple": "والرجز فاهجر"
  },
  {
    "id": 5479,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 6,
    "text_tashkeel": "وَلَا تَمْنُن تَسْتَكْثِرُۖ ٦",
    "text_simple": "ولا تمنن تستكثر"
  },
  {
    "id": 5480,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "وَلِرَبِّكَ فَاصْبِرْۖ ٧",
    "text_simple": "ولربك فاصبر"
  },
  {
    "id": 5481,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 8,
    "text_tashkeel": "فَإِذَا نُقِرَ فِے اِ۬لنَّاقُورِ ٨",
    "text_simple": "فإذا نقر فے الناقور"
  },
  {
    "id": 5482,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 9,
    "text_tashkeel": "فَذَٰلِكَ يَوْمَئِذٖ يَوْمٌ عَسِيرٌ ٩",
    "text_simple": "فذلك يومئذٖ يوم عسير"
  },
  {
    "id": 5483,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "عَلَى اَ۬لْكٰ۪فِرِينَ غَيْرُ يَسِيرٖۖ ١٠",
    "text_simple": "على الكفرين غير يسيرٖ"
  },
  {
    "id": 5484,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 11,
    "text_tashkeel": " ذَرْنِے وَمَنْ خَلَقْتُ وَحِيداٗ ١١",
    "text_simple": "ذرنے ومن خلقت وحيداٗ"
  },
  {
    "id": 5485,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 12,
    "text_tashkeel": "وَجَعَلْتُ لَهُۥ مَالاٗ مَّمْدُوداٗ ١٢",
    "text_simple": "وجعلت له مالاٗ ممدوداٗ"
  },
  {
    "id": 5486,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 13,
    "text_tashkeel": "وَبَنِينَ شُهُوداٗ ١٣",
    "text_simple": "وبنين شهوداٗ"
  },
  {
    "id": 5487,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 14,
    "text_tashkeel": "وَمَهَّدتُّ لَهُۥ تَمْهِيداٗ ١٤",
    "text_simple": "ومهدت له تمهيداٗ"
  },
  {
    "id": 5488,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 15,
    "text_tashkeel": "ثُمَّ يَطْمَعُ أَنَ اَزِيدَ ١٥",
    "text_simple": "ثم يطمع أن ازيد"
  },
  {
    "id": 5489,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 16,
    "text_tashkeel": "كَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيداٗۖ ١٦",
    "text_simple": "كلآ إنه كان لأٓيتنا عنيداٗ"
  },
  {
    "id": 5490,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 17,
    "text_tashkeel": "سَأُرْهِقُهُۥ صَعُوداًۖ ١٧",
    "text_simple": "سأرهقه صعودا"
  },
  {
    "id": 5491,
    "jozz": 29,
    "page": "575",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": "اِنَّهُۥ فَكَّرَ وَقَدَّرَ ١٨",
    "text_simple": "انه فكر وقدر"
  },
  {
    "id": 5492,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 19,
    "text_tashkeel": " فَقُتِلَ كَيْفَ قَدَّرَ ١٩",
    "text_simple": "فقتل كيف قدر"
  },
  {
    "id": 5493,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ ٢٠",
    "text_simple": "ثم قتل كيف قدر"
  },
  {
    "id": 5494,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 21,
    "text_tashkeel": "ثُمَّ نَظَرَ ٢١",
    "text_simple": "ثم نظر"
  },
  {
    "id": 5495,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 22,
    "text_tashkeel": "ثُمَّ عَبَسَ وَبَسَرَ ٢٢",
    "text_simple": "ثم عبس وبسر"
  },
  {
    "id": 5496,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "ثُمَّ أَدْبَرَ وَاسْتَكْبَرَ ٢٣",
    "text_simple": "ثم أدبر واستكبر"
  },
  {
    "id": 5497,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 24,
    "text_tashkeel": "فَقَالَ إِنْ هَٰذَآ إِلَّا سِحْرٞ يُوثَرُ ٢٤",
    "text_simple": "فقال إن هذآ إلا سحرٞ يوثر"
  },
  {
    "id": 5498,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": " إِنْ هَٰذَآ إِلَّا قَوْلُ اُ۬لْبَشَرِۖ ٢٥",
    "text_simple": "إن هذآ إلا قول البشر"
  },
  {
    "id": 5499,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 26,
    "text_tashkeel": "سَأُصْلِيهِ سَقَرَۖ ٢٦",
    "text_simple": "سأصليه سقر"
  },
  {
    "id": 5500,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 27,
    "text_tashkeel": "وَمَآ أَدْر۪يٰكَ مَا سَقَرُۖ ٢٧",
    "text_simple": "ومآ أدريك ما سقر"
  },
  {
    "id": 5501,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": " لَا تُبْقِے وَلَا تَذَرُۖ ٢٨",
    "text_simple": "لا تبقے ولا تذر"
  },
  {
    "id": 5502,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 29,
    "text_tashkeel": "لَوَّاحَةٞ لِّلْبَشَرِۖ ٢٩",
    "text_simple": "لواحةٞ للبشر"
  },
  {
    "id": 5503,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 30,
    "text_tashkeel": "عَلَيْهَا تِسْعَةَ عَشَرَۖ ٣٠",
    "text_simple": "عليها تسعة عشر"
  },
  {
    "id": 5504,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 10,
    "aya_no": 31,
    "text_tashkeel": "۞ وَمَا جَعَلْنَآ أَصْحَٰبَ اَ۬لنّ۪ارِ إِلَّا مَلَٰٓئِكَةٗۖ وَمَا جَعَلْنَا عِدَّتَهُمُۥٓ إِلَّا فِتْنَةٗ لِّلذِينَ كَفَرُواْ لِيَسْتَيْقِنَ اَ۬لذِينَ أُوتُواْ اُ۬لْكِتَٰبَ وَيَزْدَادَ اَ۬لذِينَ ءَامَنُوٓاْ إِيمَٰناٗ وَلَا يَرْتَابَ اَ۬لذِينَ أُوتُواْ اُ۬لْكِتَٰبَ وَالْمُومِنُونَ وَلِيَقُولَ اَ۬لذِينَ فِے قُلُوبِهِم مَّرَضٞ وَالْكَٰفِرُونَ مَاذَآ أَرَادَ اَ۬للَّهُ بِهَٰذَا مَثَلاٗۖ كَذَٰلِكَ يُضِلُّ اُ۬للَّهُ مَنْ يَّشَآءُ وَيَهْدِے مَنْ يَّشَآءُۖ وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَۖ وَمَا هِيَ إِلَّا ذِكْر۪ىٰ لِلْبَشَرِۖ ٣١",
    "text_simple": "وما جعلنآ أصحب النار إلا ملٓئكةٗ وما جعلنا عدتهمٓ إلا فتنةٗ للذين كفروا ليستيقن الذين أوتوا الكتب ويزداد الذين ءامنوٓا إيمناٗ ولا يرتاب الذين أوتوا الكتب والمومنون وليقول الذين فے قلوبهم مرضٞ والكفرون ماذآ أراد الله بهذا مثلاٗ كذلك يضل الله من يشآء ويهدے من يشآء وما يعلم جنود ربك إلا هو وما هي إلا ذكرى للبشر"
  },
  {
    "id": 5505,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 32,
    "text_tashkeel": "كَلَّا وَالْقَمَرِ ٣٢",
    "text_simple": "كلا والقمر"
  },
  {
    "id": 5506,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 33,
    "text_tashkeel": "وَاليْلِ إِذَ اَدْبَرَ ٣٣",
    "text_simple": "واليل إذ ادبر"
  },
  {
    "id": 5507,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 34,
    "text_tashkeel": "وَالصُّبْحِ إِذَآ أَسْفَرَ ٣٤",
    "text_simple": "والصبح إذآ أسفر"
  },
  {
    "id": 5508,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 35,
    "text_tashkeel": "إِنَّهَا لَإِحْدَى اَ۬لْكُبَرِ ٣٥",
    "text_simple": "إنها لإحدى الكبر"
  },
  {
    "id": 5509,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 36,
    "text_tashkeel": "نَذِيراٗ لِّلْبَشَرِ ٣٦",
    "text_simple": "نذيراٗ للبشر"
  },
  {
    "id": 5510,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 37,
    "text_tashkeel": "لِمَن شَآءَ مِنكُمُۥٓ أَنْ يَّتَقَدَّمَ أَوْ يَتَأَخَّرَۖ ٣٧",
    "text_simple": "لمن شآء منكمٓ أن يتقدم أو يتأخر"
  },
  {
    "id": 5511,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 38,
    "text_tashkeel": " كُلُّ نَفْسِۢ بِمَا كَسَبَتْ رَهِينَةٌ ٣٨",
    "text_simple": "كل نفس بما كسبت رهينة"
  },
  {
    "id": 5512,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 39,
    "text_tashkeel": "اِلَّآ أَصْحَٰبَ اَ۬لْيَمِينِۖ ٣٩",
    "text_simple": "الآ أصحب اليمين"
  },
  {
    "id": 5513,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 40,
    "text_tashkeel": "فِے جَنَّٰتٖ يَتَسَآءَلُونَ عَنِ اِ۬لْمُجْرِمِينَ ٤٠",
    "text_simple": "فے جنتٖ يتسآءلون عن المجرمين"
  },
  {
    "id": 5514,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 41,
    "text_tashkeel": "مَا سَلَكَكُمْ فِے سَقَرَۖ ٤١",
    "text_simple": "ما سلككم فے سقر"
  },
  {
    "id": 5515,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 42,
    "text_tashkeel": "قَالُواْ لَمْ نَكُ مِنَ اَ۬لْمُصَلِّينَ ٤٢",
    "text_simple": "قالوا لم نك من المصلين"
  },
  {
    "id": 5516,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 43,
    "text_tashkeel": "وَلَمْ نَكُ نُطْعِمُ اُ۬لْمِسْكِينَ ٤٣",
    "text_simple": "ولم نك نطعم المسكين"
  },
  {
    "id": 5517,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 44,
    "text_tashkeel": "وَكُنَّا نَخُوضُ مَعَ اَ۬لْخَآئِضِينَ ٤٤",
    "text_simple": "وكنا نخوض مع الخآئضين"
  },
  {
    "id": 5518,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 45,
    "text_tashkeel": "وَكُنَّا نُكَذِّبُ بِيَوْمِ اِ۬لدِّينِ ٤٥",
    "text_simple": "وكنا نكذب بيوم الدين"
  },
  {
    "id": 5519,
    "jozz": 29,
    "page": "576",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 46,
    "text_tashkeel": "حَتَّىٰٓ أَت۪يٰنَا اَ۬لْيَقِينُۖ ٤٦",
    "text_simple": "حتىٓ أتينا اليقين"
  },
  {
    "id": 5520,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 47,
    "text_tashkeel": " فَمَا تَنفَعُهُمْ شَفَٰعَةُ اُ۬لشَّٰفِعِينَۖ ٤٧",
    "text_simple": "فما تنفعهم شفعة الشفعين"
  },
  {
    "id": 5521,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 48,
    "text_tashkeel": "فَمَا لَهُمْ عَنِ اِ۬لتَّذْكِرَةِ مُعْرِضِينَ ٤٨",
    "text_simple": "فما لهم عن التذكرة معرضين"
  },
  {
    "id": 5522,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 49,
    "text_tashkeel": "كَأَنَّهُمْ حُمُرٞ مُّسْتَنفَرَةٞ ٤٩",
    "text_simple": "كأنهم حمرٞ مستنفرةٞ"
  },
  {
    "id": 5523,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 50,
    "text_tashkeel": "فَرَّتْ مِن قَسْوَرَةِۢۖ ٥٠",
    "text_simple": "فرت من قسورة"
  },
  {
    "id": 5524,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 51,
    "text_tashkeel": " بَلْ يُرِيدُ كُلُّ اُ۪مْرِےٕٖ مِّنْهُمُۥٓ أَنْ يُّوت۪ىٰ صُحُفاٗ مُّنَشَّرَةٗۖ ٥١",
    "text_simple": "بل يريد كل امرےٕٖ منهمٓ أن يوتى صحفاٗ منشرةٗ"
  },
  {
    "id": 5525,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 52,
    "text_tashkeel": "كَلَّا بَل لَّا يَخَافُونَ اَ۬لَاخِرَةَۖ ٥٢",
    "text_simple": "كلا بل لا يخافون الاخرة"
  },
  {
    "id": 5526,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 53,
    "text_tashkeel": "كَلَّآ إِنَّهُۥ تَذْكِرَةٞۖ ٥٣",
    "text_simple": "كلآ إنه تذكرةٞ"
  },
  {
    "id": 5527,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 54,
    "text_tashkeel": "فَمَن شَآءَ ذَكَرَهُۥۖ ٥٤",
    "text_simple": "فمن شآء ذكره"
  },
  {
    "id": 5528,
    "jozz": 29,
    "page": "577",
    "sura_no": 74,
    "sura_name_en": "Al-Muddaththir",
    "sura_name_ar": "المُدثر ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 55,
    "text_tashkeel": "وَمَا تَذْكُرُونَ إِلَّآ أَنْ يَّشَآءَ اَ۬للَّهُۖ هُوَ أَهْلُ اُ۬لتَّقْو۪ىٰ وَأَهْلُ اُ۬لْمَغْفِرَةِۖ ٥٥",
    "text_simple": "وما تذكرون إلآ أن يشآء الله هو أهل التقوى وأهل المغفرة"
  }
]