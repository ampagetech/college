[
  {
    "id": 5885,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 1,
    "text_tashkeel": "وَالسَّمَآءِ ذَاتِ اِ۬لْبُرُوجِ ١",
    "text_simple": "والسمآء ذات البروج"
  },
  {
    "id": 5886,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 2,
    "text_tashkeel": "وَالْيَوْمِ اِ۬لْمَوْعُودِ ٢",
    "text_simple": "واليوم الموعود"
  },
  {
    "id": 5887,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَشَاهِدٖ وَمَشْهُودٖ ٣",
    "text_simple": "وشاهدٖ ومشهودٖ"
  },
  {
    "id": 5888,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": " قُتِلَ أَصْحَٰبُ اُ۬لُاخْدُودِ ٤",
    "text_simple": "قتل أصحب الاخدود"
  },
  {
    "id": 5889,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "text_tashkeel": "اِ۬لنّ۪ارِ ذَاتِ اِ۬لْوَقُودِ ٥",
    "text_simple": "النار ذات الوقود"
  },
  {
    "id": 5890,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "إِذْ هُمْ عَلَيْهَا قُعُودٞ ٦",
    "text_simple": "إذ هم عليها قعودٞ"
  },
  {
    "id": 5891,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُومِنِينَ شُهُودٞۖ ٧",
    "text_simple": "وهم على ما يفعلون بالمومنين شهودٞ"
  },
  {
    "id": 5892,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "وَمَا نَقَمُواْ مِنْهُمُۥٓ إِلَّآ أَنْ يُّومِنُواْ بِاللَّهِ اِ۬لْعَزِيزِ اِ۬لْحَمِيدِ ٨",
    "text_simple": "وما نقموا منهمٓ إلآ أن يومنوا بالله العزيز الحميد"
  },
  {
    "id": 5893,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 9,
    "text_tashkeel": "اِ۬لذِے لَهُۥ مُلْكُ اُ۬لسَّمَٰوَٰتِ وَالَارْضِۖ وَاللَّهُ عَلَىٰ كُلِّ شَےْءٖ شَهِيدٌۖ ٩",
    "text_simple": "الذے له ملك السموت والارض والله على كل شےءٖ شهيد"
  },
  {
    "id": 5894,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 8,
    "line_end": 10,
    "aya_no": 10,
    "text_tashkeel": "اِنَّ اَ۬لذِينَ فَتَنُواْ اُ۬لْمُومِنِينَ وَالْمُومِنَٰتِ ثُمَّ لَمْ يَتُوبُواْ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ اُ۬لْحَرِيقِۖ ١٠",
    "text_simple": "ان الذين فتنوا المومنين والمومنت ثم لم يتوبوا فلهم عذاب جهنم ولهم عذاب الحريق"
  },
  {
    "id": 5895,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 11,
    "text_tashkeel": "إِنَّ اَ۬لذِينَ ءَامَنُواْ وَعَمِلُواْ اُ۬لصَّٰلِحَٰتِ لَهُمْ جَنَّٰتٞ تَجْرِے مِن تَحْتِهَا اَ۬لَانْهَٰرُۖ ذَٰلِكَ اَ۬لْفَوْزُ اُ۬لْكَبِيرُۖ ١١",
    "text_simple": "إن الذين ءامنوا وعملوا الصلحت لهم جنتٞ تجرے من تحتها الانهر ذلك الفوز الكبير"
  },
  {
    "id": 5896,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "۞ إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌۖ ١٢",
    "text_simple": "إن بطش ربك لشديد"
  },
  {
    "id": 5897,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 13,
    "text_tashkeel": "اِنَّهُۥ هُوَ يُبْدِۓُ وَيُعِيدُۖ ١٣",
    "text_simple": "انه هو يبدۓ ويعيد"
  },
  {
    "id": 5898,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 14,
    "text_tashkeel": "وَهُوَ اَ۬لْغَفُورُ اُ۬لْوَدُودُ ١٤",
    "text_simple": "وهو الغفور الودود"
  },
  {
    "id": 5899,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 15,
    "text_tashkeel": " ذُو اُ۬لْعَرْشِۖ اِ۬لْمَجِيدُ ١٥",
    "text_simple": "ذو العرش المجيد"
  },
  {
    "id": 5900,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 16,
    "text_tashkeel": "فَعَّالٞ لِّمَا يُرِيدُۖ ١٦",
    "text_simple": "فعالٞ لما يريد"
  },
  {
    "id": 5901,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 17,
    "text_tashkeel": "هَلَ اَت۪يٰكَ حَدِيثُ اُ۬لْجُنُودِ ١٧",
    "text_simple": "هل اتيك حديث الجنود"
  },
  {
    "id": 5902,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 18,
    "text_tashkeel": "فِرْعَوْنَ وَثَمُودَۖ ١٨",
    "text_simple": "فرعون وثمود"
  },
  {
    "id": 5903,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 19,
    "text_tashkeel": "بَلِ اِ۬لذِينَ كَفَرُواْ فِے تَكْذِيبٖ ١٩",
    "text_simple": "بل الذين كفروا فے تكذيبٖ"
  },
  {
    "id": 5904,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 20,
    "text_tashkeel": "وَاللَّهُ مِنْ وَّرَآئِهِم مُّحِيطُۢۖ ٢٠",
    "text_simple": "والله من ورآئهم محيط"
  },
  {
    "id": 5905,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 21,
    "text_tashkeel": "بَلْ هُوَ قُرْءَانٞ مَّجِيدٞ ٢١",
    "text_simple": "بل هو قرءانٞ مجيدٞ"
  },
  {
    "id": 5906,
    "jozz": 30,
    "page": "590",
    "sura_no": 85,
    "sura_name_en": "Al-Burūj",
    "sura_name_ar": "البُرُوج ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 22,
    "text_tashkeel": "فِے لَوْحٖ مَّحْفُوظٞۖ ٢٢",
    "text_simple": "فے لوحٖ محفوظٞ"
  }
]