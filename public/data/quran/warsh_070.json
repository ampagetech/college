[
  {
    "id": 5354,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 1,
    "text_tashkeel": "سَالَ سَآئِلُۢ بِعَذَابٖ وَاقِعٖ ١",
    "text_simple": "سال سآئل بعذابٖ واقعٖ"
  },
  {
    "id": 5355,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 2,
    "text_tashkeel": "لِّلْكٰ۪فِرِينَ لَيْسَ لَهُۥ دَافِعٞ ٢",
    "text_simple": "للكفرين ليس له دافعٞ"
  },
  {
    "id": 5356,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 3,
    "text_tashkeel": " مِّنَ اَ۬للَّهِ ذِے اِ۬لْمَعَارِجِۖ ٣",
    "text_simple": "من الله ذے المعارج"
  },
  {
    "id": 5357,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 4,
    "text_tashkeel": "تَعْرُجُ اُ۬لْمَلَٰٓئِكَةُ وَالرُّوحُ إِلَيْهِ فِے يَوْمٖ كَانَ مِقْدَارُهُۥ خَمْسِينَ أَلْفَ سَنَةٖۖ ٤",
    "text_simple": "تعرج الملٓئكة والروح إليه فے يومٖ كان مقداره خمسين ألف سنةٖ"
  },
  {
    "id": 5358,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 5,
    "text_tashkeel": "فَاصْبِرْ صَبْراٗ جَمِيلاًۖ ٥",
    "text_simple": "فاصبر صبراٗ جميلا"
  },
  {
    "id": 5359,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 6,
    "text_tashkeel": "اِنَّهُمْ يَرَوْنَهُۥ بَعِيداٗ ٦",
    "text_simple": "انهم يرونه بعيداٗ"
  },
  {
    "id": 5360,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 7,
    "text_tashkeel": "وَنَر۪يٰهُ قَرِيباٗۖ ٧",
    "text_simple": "ونريه قريباٗ"
  },
  {
    "id": 5361,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 8,
    "text_tashkeel": "يَوْمَ تَكُونُ اُ۬لسَّمَآءُ كَالْمُهْلِ ٨",
    "text_simple": "يوم تكون السمآء كالمهل"
  },
  {
    "id": 5362,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 9,
    "text_tashkeel": " وَتَكُونُ اُ۬لْجِبَالُ كَالْعِهْنِ ٩",
    "text_simple": "وتكون الجبال كالعهن"
  },
  {
    "id": 5363,
    "jozz": 29,
    "page": "568",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 10,
    "text_tashkeel": "وَلَا يَسْـَٔلُ حَمِيمٌ حَمِيماٗ ١٠",
    "text_simple": "ولا يسٔل حميم حميماٗ"
  },
  {
    "id": 5364,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 11,
    "text_tashkeel": " يُبَصَّرُونَهُمْۖ يَوَدُّ اُ۬لْمُجْرِمُ لَوْ يَفْتَدِے مِنْ عَذَابِ يَوْمَئِذِۢ بِبَنِيهِ ١١",
    "text_simple": "يبصرونهم يود المجرم لو يفتدے من عذاب يومئذ ببنيه"
  },
  {
    "id": 5365,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 12,
    "text_tashkeel": " وَصَٰحِبَتِهِۦ وَأَخِيهِ ١٢",
    "text_simple": "وصحبته وأخيه"
  },
  {
    "id": 5366,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 13,
    "text_tashkeel": "وَفَصِيلَتِهِ اِ۬لتِے تُـْٔوِيهِ ١٣",
    "text_simple": "وفصيلته التے تٔويه"
  },
  {
    "id": 5367,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 14,
    "text_tashkeel": "وَمَن فِے اِ۬لَارْضِ جَمِيعاٗ ثُمَّ يُنجِيهِ ١٤",
    "text_simple": "ومن فے الارض جميعاٗ ثم ينجيه"
  },
  {
    "id": 5368,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 15,
    "text_tashkeel": "كَلَّآۖ إِنَّهَا لَظ۪ىٰ ١٥",
    "text_simple": "كلآ إنها لظى"
  },
  {
    "id": 5369,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 16,
    "text_tashkeel": "نَزَّاعَةٞ لِّلشَّو۪ىٰ ١٦",
    "text_simple": "نزاعةٞ للشوى"
  },
  {
    "id": 5370,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 17,
    "text_tashkeel": "تَدْعُواْ مَنَ اَدْبَرَ وَتَوَلّ۪ىٰ ١٧",
    "text_simple": "تدعوا من ادبر وتولى"
  },
  {
    "id": 5371,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 18,
    "text_tashkeel": "وَجَمَعَ فَأَوْع۪ىٰٓۖ ١٨",
    "text_simple": "وجمع فأوعىٓ"
  },
  {
    "id": 5372,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 19,
    "text_tashkeel": "إِنَّ اَ۬لِانسَٰنَ خُلِقَ هَلُوعاً ١٩",
    "text_simple": "إن الانسن خلق هلوعا"
  },
  {
    "id": 5373,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 20,
    "text_tashkeel": "اِذَا مَسَّهُ اُ۬لشَّرُّ جَزُوعاٗ ٢٠",
    "text_simple": "اذا مسه الشر جزوعاٗ"
  },
  {
    "id": 5374,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 21,
    "text_tashkeel": "وَإِذَا مَسَّهُ اُ۬لْخَيْرُ مَنُوعاً ٢١",
    "text_simple": "وإذا مسه الخير منوعا"
  },
  {
    "id": 5375,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 22,
    "text_tashkeel": "اِلَّا اَ۬لْمُصَلِّينَ ٢٢",
    "text_simple": "الا المصلين"
  },
  {
    "id": 5376,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 23,
    "text_tashkeel": "اَ۬لذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَآئِمُونَۖ ٢٣",
    "text_simple": "الذين هم على صلاتهم دآئمون"
  },
  {
    "id": 5377,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 24,
    "text_tashkeel": "وَالذِينَ فِےٓ أَمْوَٰلِهِمْ حَقّٞ مَّعْلُومٞ ٢٤",
    "text_simple": "والذين فےٓ أمولهم حقٞ معلومٞ"
  },
  {
    "id": 5378,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 25,
    "text_tashkeel": "لِّلسَّآئِلِ وَالْمَحْرُومِۖ ٢٥",
    "text_simple": "للسآئل والمحروم"
  },
  {
    "id": 5379,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 26,
    "text_tashkeel": "وَالذِينَ يُصَدِّقُونَ بِيَوْمِ اِ۬لدِّينِۖ ٢٦",
    "text_simple": "والذين يصدقون بيوم الدين"
  },
  {
    "id": 5380,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 27,
    "text_tashkeel": "وَالذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ ٢٧",
    "text_simple": "والذين هم من عذاب ربهم مشفقون"
  },
  {
    "id": 5381,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 28,
    "text_tashkeel": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَامُونٖۖ ٢٨",
    "text_simple": "إن عذاب ربهم غير مامونٖ"
  },
  {
    "id": 5382,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 29,
    "text_tashkeel": "وَالذِينَ هُمْ لِفُرُوجِهِمْ حَٰفِظُونَ ٢٩",
    "text_simple": "والذين هم لفروجهم حفظون"
  },
  {
    "id": 5383,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 30,
    "text_tashkeel": "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمُۥٓ أَوْ مَا مَلَكَتَ اَيْمَٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَۖ ٣٠",
    "text_simple": "إلا علىٓ أزوجهمٓ أو ما ملكت ايمنهم فإنهم غير ملومين"
  },
  {
    "id": 5384,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 31,
    "text_tashkeel": "فَمَنِ اِ۪بْتَغ۪ىٰ وَرَآءَ ذَٰلِكَ فَأُوْلَٰٓئِكَ هُمُ اُ۬لْعَادُونَۖ ٣١",
    "text_simple": "فمن ابتغى ورآء ذلك فأولٓئك هم العادون"
  },
  {
    "id": 5385,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 32,
    "text_tashkeel": "وَالذِينَ هُمْ لِأَمَٰنَٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَۖ ٣٢",
    "text_simple": "والذين هم لأمنتهم وعهدهم رعون"
  },
  {
    "id": 5386,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 33,
    "text_tashkeel": "وَالذِينَ هُم بِشَهَٰدَتِهِمْ قَآئِمُونَۖ ٣٣",
    "text_simple": "والذين هم بشهدتهم قآئمون"
  },
  {
    "id": 5387,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 34,
    "text_tashkeel": " وَالذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ ٣٤",
    "text_simple": "والذين هم على صلاتهم يحافظون"
  },
  {
    "id": 5388,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 35,
    "text_tashkeel": "أُوْلَٰٓئِكَ فِے جَنَّٰتٖ مُّكْرَمُونَۖ ٣٥",
    "text_simple": "أولٓئك فے جنتٖ مكرمون"
  },
  {
    "id": 5389,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 36,
    "text_tashkeel": " فَمَالِ اِ۬لذِينَ كَفَرُواْ قِبَلَكَ مُهْطِعِينَ ٣٦",
    "text_simple": "فمال الذين كفروا قبلك مهطعين"
  },
  {
    "id": 5390,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 37,
    "text_tashkeel": "عَنِ اِ۬لْيَمِينِ وَعَنِ اِ۬لشِّمَالِ عِزِينَۖ ٣٧",
    "text_simple": "عن اليمين وعن الشمال عزين"
  },
  {
    "id": 5391,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
    "sura_name_ar": "المَعَارج ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 38,
    "text_tashkeel": "أَيَطْمَعُ كُلُّ اُ۪مْرِےٕٖ مِّنْهُمُۥٓ أَنْ يُّدْخَلَ جَنَّةَ نَعِيمٖ ٣٨",
    "text_simple": "أيطمع كل امرےٕٖ منهمٓ أن يدخل جنة نعيمٖ"
  },
  {
    "id": 5392,
    "jozz": 29,
    "page": "569",
    "sura_no": 70,
    "sura_name_en": "Al-Ma‘ārij",
