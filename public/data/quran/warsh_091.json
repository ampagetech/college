    "line_end": 4,
    "aya_no": 1,
    "text_tashkeel": "وَالشَّمْسِ وَضُحَيٰهَا ١",
    "text_simple": "والشمس وضحيها"
  },
  {
    "id": 6022,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 2,
    "text_tashkeel": "وَالْقَمَرِ إِذَا تَلَيٰهَا ٢",
    "text_simple": "والقمر إذا تليها"
  },
  {
    "id": 6023,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَالنَّه۪ارِ إِذَا جَلَّيٰهَا ٣",
    "text_simple": "والنهار إذا جليها"
  },
  {
    "id": 6024,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 4,
    "text_tashkeel": " وَاليْلِ إِذَا يَغْشَيٰهَا ٤",
    "text_simple": "واليل إذا يغشيها"
  },
  {
    "id": 6025,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "text_tashkeel": "وَالسَّمَآءِ وَمَا بَنَيٰهَا ٥",
    "text_simple": "والسمآء وما بنيها"
  },
  {
    "id": 6026,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 6,
    "text_tashkeel": "وَالَارْضِ وَمَا طَحَيٰهَا ٦",
    "text_simple": "والارض وما طحيها"
  },
  {
    "id": 6027,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 7,
    "text_tashkeel": "وَنَفْسٖ وَمَا سَوَّيٰهَا ٧",
    "text_simple": "ونفسٖ وما سويها"
  },
  {
    "id": 6028,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 8,
    "text_tashkeel": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَيٰهَا ٨",
    "text_simple": "فألهمها فجورها وتقويها"
  },
  {
    "id": 6029,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 9,
    "text_tashkeel": "قَدَ اَفْلَحَ مَن زَكَّيٰهَا ٩",
    "text_simple": "قد افلح من زكيها"
  },
  {
    "id": 6030,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 10,
    "text_tashkeel": "وَقَدْ خَابَ مَن دَسَّيٰهَاۖ ١٠",
    "text_simple": "وقد خاب من دسيها"
  },
  {
    "id": 6031,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 11,
    "text_tashkeel": " كَذَّبَتْ ثَمُودُ بِطَغْوَيٰهَآ ١١",
    "text_simple": "كذبت ثمود بطغويهآ"
  },
  {
    "id": 6032,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 12,
    "text_tashkeel": "إِذِ اِ۪نۢبَعَثَ أَشْقَيٰهَا ١٢",
    "text_simple": "إذ انبعث أشقيها"
  },
  {
    "id": 6033,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 13,
    "text_tashkeel": "فَقَالَ لَهُمْ رَسُولُ اُ۬للَّهِ نَاقَةَ اَ۬للَّهِ وَسُقْيَاهَا ١٣",
    "text_simple": "فقال لهم رسول الله ناقة الله وسقياها"
  },
  {
    "id": 6034,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 14,
    "text_tashkeel": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّيٰهَاۖ ١٤",
    "text_simple": "فكذبوه فعقروها فدمدم عليهم ربهم بذنبهم فسويها"
  },
  {
    "id": 6035,
    "jozz": 30,
    "page": "595",
    "sura_no": 91,
    "sura_name_en": "Ash-Shams",
    "sura_name_ar": "الشَّمس ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 15,
    "text_tashkeel": "فَلَا يَخَافُ عُقْبَٰهَاۖ ١٥",
    "text_simple": "فلا يخاف عقبها"
  }
]