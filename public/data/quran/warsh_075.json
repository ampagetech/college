[
  {
    "id": 5529,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 1,
    "text_tashkeel": "لَآ أُقْسِمُ بِيَوْمِ اِ۬لْقِيَٰمَةِ ١",
    "text_simple": "لآ أقسم بيوم القيمة"
  },
  {
    "id": 5530,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 2,
    "text_tashkeel": "وَلَآ أُقْسِمُ بِالنَّفْسِ اِ۬للَّوَّامَةِۖ ٢",
    "text_simple": "ولآ أقسم بالنفس اللوامة"
  },
  {
    "id": 5531,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 3,
    "text_tashkeel": "أَيَحْسِبُ اُ۬لِانسَٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ ٣",
    "text_simple": "أيحسب الانسن ألن نجمع عظامه"
  },
  {
    "id": 5532,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 4,
    "text_tashkeel": "بَل۪ىٰۖ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّيَ بَنَانَهُۥۖ ٤",
    "text_simple": "بلى قدرين علىٓ أن نسوي بنانه"
  },
  {
    "id": 5533,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 5,
    "text_tashkeel": "بَلْ يُرِيدُ اُ۬لِانسَٰنُ لِيَفْجُرَ أَمَامَهُۥۖ ٥",
    "text_simple": "بل يريد الانسن ليفجر أمامه"
  },
  {
    "id": 5534,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 6,
    "text_tashkeel": "يَسْـَٔلُ أَيَّانَ يَوْمُ اُ۬لْقِيَٰمَةِۖ ٦",
    "text_simple": "يسٔل أيان يوم القيمة"
  },
  {
    "id": 5535,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 7,
    "text_tashkeel": "فَإِذَا بَرَقَ اَ۬لْبَصَرُ ٧",
    "text_simple": "فإذا برق البصر"
  },
  {
    "id": 5536,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 8,
    "text_tashkeel": "وَخَسَفَ اَ۬لْقَمَرُ ٨",
    "text_simple": "وخسف القمر"
  },
  {
    "id": 5537,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 9,
    "text_tashkeel": "وَجُمِعَ اَ۬لشَّمْسُ وَالْقَمَرُ ٩",
    "text_simple": "وجمع الشمس والقمر"
  },
  {
    "id": 5538,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 10,
    "text_tashkeel": "يَقُولُ اُ۬لِانسَٰنُ يَوْمَئِذٍ اَيْنَ اَ۬لْمَفَرُّۖ ١٠",
    "text_simple": "يقول الانسن يومئذ اين المفر"
  },
  {
    "id": 5539,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 11,
    "text_tashkeel": "كَلَّا لَا وَزَرَۖ ١١",
    "text_simple": "كلا لا وزر"
  },
  {
    "id": 5540,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 12,
    "text_tashkeel": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ اِ۬لْمُسْتَقَرُّۖ ١٢",
    "text_simple": "إلى ربك يومئذ المستقر"
  },
  {
    "id": 5541,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 12,
    "line_end": 13,
    "aya_no": 13,
    "text_tashkeel": "يُنَبَّؤُاْ اُ۬لِانسَٰنُ يَوْمَئِذِۢ بِمَا قَدَّمَ وَأَخَّرَۖ ١٣",
    "text_simple": "ينبؤا الانسن يومئذ بما قدم وأخر"
  },
  {
    "id": 5542,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 14,
    "text_tashkeel": "بَلِ اِ۬لِانسَٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٞ ١٤",
    "text_simple": "بل الانسن على نفسه بصيرةٞ"
  },
  {
    "id": 5543,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 15,
    "text_tashkeel": " وَلَوَ اَلْق۪ىٰ مَعَاذِيرَهُۥۖ ١٥",
    "text_simple": "ولو القى معاذيره"
  },
  {
    "id": 5544,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 16,
    "text_tashkeel": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓۖ إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥۖ ١٦",
    "text_simple": "لا تحرك به لسانك لتعجل بهٓ إن علينا جمعه وقرءانه"
  },
  {
    "id": 5545,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 17,
    "text_tashkeel": "فَإِذَا قَرَأْنَٰهُ فَاتَّبِعْ قُرْءَانَهُۥ ١٧",
    "text_simple": "فإذا قرأنه فاتبع قرءانه"
  },
  {
    "id": 5546,
    "jozz": 29,
    "page": "577",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 18,
    "text_tashkeel": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥۖ ١٨",
    "text_simple": "ثم إن علينا بيانه"
  },
  {
    "id": 5547,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 19,
    "text_tashkeel": " كَلَّا بَلْ تُحِبُّونَ اَ۬لْعَاجِلَةَ ١٩",
    "text_simple": "كلا بل تحبون العاجلة"
  },
  {
    "id": 5548,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 20,
    "text_tashkeel": "وَتَذَرُونَ اَ۬لَاخِرَةَۖ ٢٠",
    "text_simple": "وتذرون الاخرة"
  },
  {
    "id": 5549,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 1,
    "line_end": 2,
    "aya_no": 21,
    "text_tashkeel": "وُجُوهٞ يَوْمَئِذٖ نَّاضِرَةٌ ٢١",
    "text_simple": "وجوهٞ يومئذٖ ناضرة"
  },
  {
    "id": 5550,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 22,
    "text_tashkeel": "اِلَىٰ رَبِّهَا نَاظِرَةٞۖ ٢٢",
    "text_simple": "الى ربها ناظرةٞ"
  },
  {
    "id": 5551,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 23,
    "text_tashkeel": "وَوُجُوهٞ يَوْمَئِذِۢ بَاسِرَةٞ ٢٣",
    "text_simple": "ووجوهٞ يومئذ باسرةٞ"
  },
  {
    "id": 5552,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 24,
    "text_tashkeel": "تَظُنُّ أَنْ يُّفْعَلَ بِهَا فَاقِرَةٞۖ ٢٤",
    "text_simple": "تظن أن يفعل بها فاقرةٞ"
  },
  {
    "id": 5553,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 25,
    "text_tashkeel": "كَلَّآ إِذَا بَلَغَتِ اِ۬لتَّرَاقِيَ ٢٥",
    "text_simple": "كلآ إذا بلغت التراقي"
  },
  {
    "id": 5554,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 26,
    "text_tashkeel": "وَقِيلَ مَن رَّاقٖ ٢٦",
    "text_simple": "وقيل من راقٖ"
  },
  {
    "id": 5555,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 27,
    "text_tashkeel": "وَظَنَّ أَنَّهُ اُ۬لْفِرَاقُ ٢٧",
    "text_simple": "وظن أنه الفراق"
  },
  {
    "id": 5556,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": "وَالْتَفَّتِ اِ۬لسَّاقُ بِالسَّاقِ ٢٨",
    "text_simple": "والتفت الساق بالساق"
  },
  {
    "id": 5557,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 29,
    "text_tashkeel": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ اِ۬لْمَسَاقُۖ ٢٩",
    "text_simple": "إلى ربك يومئذ المساق"
  },
  {
    "id": 5558,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 30,
    "text_tashkeel": " فَلَا صَدَّقَ وَلَا صَلّ۪ىٰۖ ٣٠",
    "text_simple": "فلا صدق ولا صلى"
  },
  {
    "id": 5559,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 31,
    "text_tashkeel": "وَلَٰكِن كَذَّبَ وَتَوَلّ۪ىٰۖ ٣١",
    "text_simple": "ولكن كذب وتولى"
  },
  {
    "id": 5560,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 32,
    "text_tashkeel": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطّ۪ىٰٓۖ ٣٢",
    "text_simple": "ثم ذهب إلىٓ أهله يتمطىٓ"
  },
  {
    "id": 5561,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 33,
    "text_tashkeel": "أَوْل۪ىٰ لَكَ فَأَوْل۪ىٰ ٣٣",
    "text_simple": "أولى لك فأولى"
  },
  {
    "id": 5562,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 34,
    "text_tashkeel": "ثُمَّ أَوْل۪ىٰ لَكَ فَأَوْل۪ىٰٓۖ ٣٤",
    "text_simple": "ثم أولى لك فأولىٓ"
  },
  {
    "id": 5563,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 35,
    "text_tashkeel": "أَيَحْسِبُ اُ۬لِانسَٰنُ أَنْ يُّتْرَكَ سُدىًۖ ٣٥",
    "text_simple": "أيحسب الانسن أن يترك سدى"
  },
  {
    "id": 5564,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 36,
    "text_tashkeel": "اَلَمْ يَكُ نُطْفَةٗ مِّن مَّنِيّٖ تُمْن۪ىٰ ٣٦",
    "text_simple": "الم يك نطفةٗ من منيٖ تمنى"
  },
  {
    "id": 5565,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 37,
    "text_tashkeel": " ثُمَّ كَانَ عَلَقَةٗ فَخَلَقَ فَسَوّ۪ىٰ ٣٧",
    "text_simple": "ثم كان علقةٗ فخلق فسوى"
  },
  {
    "id": 5566,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 38,
    "text_tashkeel": "فَجَعَلَ مِنْهُ اُ۬لزَّوْجَيْنِ اِ۬لذَّكَرَ وَالُانث۪ىٰٓۖ ٣٨",
    "text_simple": "فجعل منه الزوجين الذكر والانثىٓ"
  },
  {
    "id": 5567,
    "jozz": 29,
    "page": "578",
    "sura_no": 75,
    "sura_name_en": "Al-Qiyāmah",
    "sura_name_ar": "القِيَامة ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 39,
    "text_tashkeel": "أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَنْ يُّحْـِۧيَ اَ۬لْمَوْت۪ىٰۖ ٣٩",
    "text_simple": "أليس ذلك بقدر علىٓ أن يحي الموتى"
  }
]