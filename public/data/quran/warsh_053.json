[
  {
    "id": 4763,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 1,
    "text_tashkeel": "وَالنَّجْمِ إِذَا هَو۪ىٰ ١",
    "text_simple": "والنجم إذا هوى"
  },
  {
    "id": 4764,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 2,
    "text_tashkeel": "مَا ضَلَّ صَٰحِبُكُمْ وَمَا غَو۪ىٰ ٢",
    "text_simple": "ما ضل صحبكم وما غوى"
  },
  {
    "id": 4765,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 3,
    "text_tashkeel": "وَمَا يَنطِقُ عَنِ اِ۬لْهَو۪ىٰٓ ٣",
    "text_simple": "وما ينطق عن الهوىٓ"
  },
  {
    "id": 4766,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "text_tashkeel": "إِنْ هُوَ إِلَّا وَحْيٞ يُوح۪ىٰۖ ٤",
    "text_simple": "إن هو إلا وحيٞ يوحى"
  },
  {
    "id": 4767,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 5,
    "text_tashkeel": "عَلَّمَهُۥ شَدِيدُ اُ۬لْقُو۪ىٰ ٥",
    "text_simple": "علمه شديد القوى"
  },
  {
    "id": 4768,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 6,
    "text_tashkeel": "ذُو مِرَّةٖۖ فَاسْتَو۪ىٰ ٦",
    "text_simple": "ذو مرةٖ فاستوى"
  },
  {
    "id": 4769,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 7,
    "text_tashkeel": "وَهُوَ بِالُافُقِ اِ۬لَاعْل۪ىٰۖ ٧",
    "text_simple": "وهو بالافق الاعلى"
  },
  {
    "id": 4770,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 8,
    "text_tashkeel": "ثُمَّ دَنَا فَتَدَلّ۪ىٰ ٨",
    "text_simple": "ثم دنا فتدلى"
  },
  {
    "id": 4771,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 9,
    "text_tashkeel": "فَكَانَ قَابَ قَوْسَيْنِ أَوَ اَدْن۪ىٰۖ ٩",
    "text_simple": "فكان قاب قوسين أو ادنى"
  },
  {
    "id": 4772,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 10,
    "text_tashkeel": "فَأَوْح۪ىٰٓ إِلَىٰ عَبْدِهِۦ مَآ أَوْح۪ىٰۖ ١٠",
    "text_simple": "فأوحىٓ إلى عبده مآ أوحى"
  },
  {
    "id": 4773,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 6,
    "line_end": 7,
    "aya_no": 11,
    "text_tashkeel": "مَا كَذَبَ اَ۬لْفُؤَادُ مَا ر۪أ۪ىٰٓۖ ١١",
    "text_simple": "ما كذب الفؤاد ما رأىٓ"
  },
  {
    "id": 4774,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 12,
    "text_tashkeel": "أَفَتُمَٰرُونَهُۥ عَلَىٰ مَا يَر۪ىٰۖ ١٢",
    "text_simple": "أفتمرونه على ما يرى"
  },
  {
    "id": 4775,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 13,
    "text_tashkeel": "وَلَقَدْ ر۪ء۪اهُ نَزْلَةً ا۟خْر۪ىٰ ١٣",
    "text_simple": "ولقد رءاه نزلة اخرى"
  },
  {
    "id": 4776,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 14,
    "text_tashkeel": "عِندَ سِدْرَةِ اِ۬لْمُنتَه۪ىٰ ١٤",
    "text_simple": "عند سدرة المنتهى"
  },
  {
    "id": 4777,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 15,
    "text_tashkeel": "عِندَهَا جَنَّةُ اُ۬لْمَأْو۪ىٰٓ ١٥",
    "text_simple": "عندها جنة المأوىٓ"
  },
  {
    "id": 4778,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 8,
    "line_end": 9,
    "aya_no": 16,
    "text_tashkeel": "إِذْ يَغْشَى اَ۬لسِّدْرَةَ مَا يَغْش۪ىٰۖ ١٦",
    "text_simple": "إذ يغشى السدرة ما يغشى"
  },
  {
    "id": 4779,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 17,
    "text_tashkeel": "مَا زَاغَ اَ۬لْبَصَرُ وَمَا طَغ۪ىٰۖ ١٧",
    "text_simple": "ما زاغ البصر وما طغى"
  },
  {
    "id": 4780,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 9,
    "line_end": 10,
    "aya_no": 18,
    "text_tashkeel": "لَقَدْ ر۪أ۪ىٰ مِنَ اٰيَٰتِ رَبِّهِ اِ۬لْكُبْر۪ىٰٓۖ ١٨",
    "text_simple": "لقد رأى من ايت ربه الكبرىٓ"
  },
  {
    "id": 4781,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 19,
    "text_tashkeel": "أَفَرَٰٓيْتُمُ اُ۬للَّٰتَ وَالْعُزّ۪ىٰ ١٩",
    "text_simple": "أفرٓيتم اللت والعزى"
  },
  {
    "id": 4782,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 20,
    "text_tashkeel": "وَمَنَوٰةَ اَ۬لثَّالِثَةَ اَ۬لُاخْر۪ىٰٓ ٢٠",
    "text_simple": "ومنوة الثالثة الاخرىٓ"
  },
  {
    "id": 4783,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 10,
    "line_end": 11,
    "aya_no": 21,
    "text_tashkeel": "أَلَكُمُ اُ۬لذَّكَرُ وَلَهُ اُ۬لُانث۪ىٰۖ ٢١",
    "text_simple": "ألكم الذكر وله الانثى"
  },
  {
    "id": 4784,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 22,
    "text_tashkeel": "تِلْكَ إِذاٗ قِسْمَةٞ ضِيز۪ىٰٓۖ ٢٢",
    "text_simple": "تلك إذاٗ قسمةٞ ضيزىٓ"
  },
  {
    "id": 4785,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 11,
    "line_end": 13,
    "aya_no": 23,
    "text_tashkeel": "إِنْ هِيَ إِلَّآ أَسْمَآءٞ سَمَّيْتُمُوهَآ أَنتُمْ وَءَابَآؤُكُم مَّآ أَنزَلَ اَ۬للَّهُ بِهَا مِن سُلْطَٰنٍۖ اِنْ يَّتَّبِعُونَ إِلَّا اَ۬لظَّنَّ وَمَا تَهْوَى اَ۬لَانفُسُۖ وَلَقَدْ جَآءَهُم مِّن رَّبِّهِمُ اُ۬لْهُد۪ىٰٓۖ ٢٣",
    "text_simple": "إن هي إلآ أسمآءٞ سميتموهآ أنتم وءابآؤكم مآ أنزل الله بها من سلطن ان يتبعون إلا الظن وما تهوى الانفس ولقد جآءهم من ربهم الهدىٓ"
  },
  {
    "id": 4786,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 13,
    "line_end": 14,
    "aya_no": 24,
    "text_tashkeel": "أَمْ لِلِانسَٰنِ مَا تَمَنّ۪ىٰۖ ٢٤",
    "text_simple": "أم للانسن ما تمنى"
  },
  {
    "id": 4787,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 25,
    "text_tashkeel": "فَلِلهِ اِ۬لَاخِرَةُ وَالُاول۪ىٰۖ ٢٥",
    "text_simple": "فلله الاخرة والاولى"
  },
  {
    "id": 4788,
    "jozz": 27,
    "page": "526",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 14,
    "line_end": 15,
    "aya_no": 26,
    "text_tashkeel": "۞ وَكَم مِّن مَّلَكٖ فِے اِ۬لسَّمَٰوَٰتِ لَا تُغْنِے شَفَٰعَتُهُمْ شَئْاً اِلَّا مِنۢ بَعْدِ أَنْ يَّاذَنَ اَ۬للَّهُ لِمَنْ يَّشَآءُ وَيَرْض۪ىٰٓۖ ٢٦",
    "text_simple": "وكم من ملكٖ فے السموت لا تغنے شفعتهم شئا الا من بعد أن ياذن الله لمن يشآء ويرضىٓ"
  },
  {
    "id": 4789,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 27,
    "text_tashkeel": " إِنَّ اَ۬لذِينَ لَا يُومِنُونَ بِالَاخِرَةِ لَيُسَمُّونَ اَ۬لْمَلَٰٓئِكَةَ تَسْمِيَةَ اَ۬لُانث۪ىٰۖ ٢٧",
    "text_simple": "إن الذين لا يومنون بالاخرة ليسمون الملٓئكة تسمية الانثى"
  },
  {
    "id": 4790,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 2,
    "line_end": 4,
    "aya_no": 28,
    "text_tashkeel": " وَمَا لَهُم بِهِۦ مِنْ عِلْمٍۖ اِنْ يَّتَّبِعُونَ إِلَّا اَ۬لظَّنَّۖ وَإِنَّ اَ۬لظَّنَّ لَا يُغْنِے مِنَ اَ۬لْحَقِّ شَئْاٗۖ فَأَعْرِضْ عَن مَّن تَوَلّ۪ىٰ عَن ذِكْرِنَا وَلَمْ يُرِدِ اِلَّا اَ۬لْحَيَوٰةَ اَ۬لدُّنْي۪اۖ ٢٨",
    "text_simple": "وما لهم به من علم ان يتبعون إلا الظن وإن الظن لا يغنے من الحق شئاٗ فأعرض عن من تولى عن ذكرنا ولم يرد الا الحيوة الدنيا"
  },
  {
    "id": 4791,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 4,
    "line_end": 5,
    "aya_no": 29,
    "text_tashkeel": "ذَٰلِكَ مَبْلَغُهُم مِّنَ اَ۬لْعِلْمِۖ إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦۖ وَهُوَ أَعْلَمُ بِمَنِ اِ۪هْتَد۪ىٰۖ ٢٩",
    "text_simple": "ذلك مبلغهم من العلم إن ربك هو أعلم بمن ضل عن سبيله وهو أعلم بمن اهتدى"
  },
  {
    "id": 4792,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 7,
    "aya_no": 30,
    "text_tashkeel": "وَلِلهِ مَا فِے اِ۬لسَّمَٰوَٰتِ وَمَا فِے اِ۬لَارْضِ لِيَجْزِيَ اَ۬لذِينَ أَسَٰٓـُٔواْ بِمَا عَمِلُواْ وَيَجْزِيَ اَ۬لذِينَ أَحْسَنُواْ بِالْحُسْنَىۖ ٣٠",
    "text_simple": "ولله ما فے السموت وما فے الارض ليجزي الذين أسٓٔوا بما عملوا ويجزي الذين أحسنوا بالحسنى"
  },
  {
    "id": 4793,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 10,
    "aya_no": 31,
    "text_tashkeel": "اَ۬لذِينَ يَجْتَنِبُونَ كَبَٰٓئِرَ اَ۬لِاثْمِ وَالْفَوَٰحِشَ إِلَّا اَ۬للَّمَمَۖ إِنَّ رَبَّكَ وَٰسِعُ اُ۬لْمَغْفِرَةِۖ هُوَ أَعْلَمُ بِكُمُۥٓ إِذَ اَنشَأَكُم مِّنَ اَ۬لَارْضِ وَإِذَ اَنتُمُۥٓ أَجِنَّةٞ فِے بُطُونِ أُمَّهَٰتِكُمْۖ فَلَا تُزَكُّوٓاْ أَنفُسَكُمْۖ هُوَ أَعْلَمُ بِمَنِ اِ۪تَّق۪ىٰٓۖ ٣١",
    "text_simple": "الذين يجتنبون كبٓئر الاثم والفوحش إلا اللمم إن ربك وسع المغفرة هو أعلم بكمٓ إذ انشأكم من الارض وإذ انتمٓ أجنةٞ فے بطون أمهتكم فلا تزكوٓا أنفسكم هو أعلم بمن اتقىٓ"
  },
  {
    "id": 4794,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 32,
    "text_tashkeel": "أَفَرَٰٓيْتَ اَ۬لذِے تَوَلّ۪ىٰ ٣٢",
    "text_simple": "أفرٓيت الذے تولى"
  },
  {
    "id": 4795,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 10,
    "line_end": 10,
    "aya_no": 33,
    "text_tashkeel": "وَأَعْط۪ىٰ قَلِيلاٗ وَأَكْد۪ىٰٓ ٣٣",
    "text_simple": "وأعطى قليلاٗ وأكدىٓ"
  },
  {
    "id": 4796,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 11,
    "line_end": 11,
    "aya_no": 34,
    "text_tashkeel": " أَعِندَهُۥ عِلْمُ اُ۬لْغَيْبِ فَهُوَ يَر۪ىٰٓۖ ٣٤",
    "text_simple": "أعنده علم الغيب فهو يرىٓ"
  },
  {
    "id": 4797,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 11,
    "line_end": 12,
    "aya_no": 35,
    "text_tashkeel": "أَمْ لَمْ يُنَبَّأْ بِمَا فِے صُحُفِ مُوس۪ىٰ ٣٥",
    "text_simple": "أم لم ينبأ بما فے صحف موسى"
  },
  {
    "id": 4798,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 36,
    "text_tashkeel": "وَإِبْرَٰهِيمَ اَ۬لذِے وَفّ۪ىٰٓ ٣٦",
    "text_simple": "وإبرهيم الذے وفىٓ"
  },
  {
    "id": 4799,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 12,
    "line_end": 12,
    "aya_no": 37,
    "text_tashkeel": "أَلَّا تَزِرُ وَازِرَةٞ وِزْرَ أُخْر۪ىٰۖ ٣٧",
    "text_simple": "ألا تزر وازرةٞ وزر أخرى"
  },
  {
    "id": 4800,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 38,
    "text_tashkeel": " وَأَن لَّيْسَ لِلِانسَٰنِ إِلَّا مَا سَع۪ىٰۖ ٣٨",
    "text_simple": "وأن ليس للانسن إلا ما سعى"
  },
  {
    "id": 4801,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 13,
    "line_end": 13,
    "aya_no": 39,
    "text_tashkeel": "وَأَنَّ سَعْيَهُۥ سَوْفَ يُر۪ىٰۖ ٣٩",
    "text_simple": "وأن سعيه سوف يرى"
  },
  {
    "id": 4802,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 40,
    "text_tashkeel": " ثُمَّ يُجْز۪يٰهُ اُ۬لْجَزَآءَ اَ۬لَاوْف۪ىٰۖ ٤٠",
    "text_simple": "ثم يجزيه الجزآء الاوفى"
  },
  {
    "id": 4803,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 14,
    "line_end": 14,
    "aya_no": 41,
    "text_tashkeel": "وَأَنَّ إِلَىٰ رَبِّكَ اَ۬لْمُنتَه۪ىٰۖ ٤١",
    "text_simple": "وأن إلى ربك المنتهى"
  },
  {
    "id": 4804,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 42,
    "text_tashkeel": " وَأَنَّهُۥ هُوَ أَضْحَكَ وَأَبْك۪ىٰۖ ٤٢",
    "text_simple": "وأنه هو أضحك وأبكى"
  },
  {
    "id": 4805,
    "jozz": 27,
    "page": "527",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 15,
    "line_end": 15,
    "aya_no": 43,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ أَمَاتَ وَأَحْي۪اۖ ٤٣",
    "text_simple": "وأنه هو أمات وأحيا"
  },
  {
    "id": 4806,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 44,
    "text_tashkeel": " وَأَنَّهُۥ خَلَقَ اَ۬لزَّوْجَيْنِ اِ۬لذَّكَرَ وَالُانث۪ىٰ ٤٤",
    "text_simple": "وأنه خلق الزوجين الذكر والانثى"
  },
  {
    "id": 4807,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 45,
    "text_tashkeel": "مِن نُّطْفَةٍ اِذَا تُمْن۪ىٰۖ ٤٥",
    "text_simple": "من نطفة اذا تمنى"
  },
  {
    "id": 4808,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 46,
    "text_tashkeel": " ۞ وَأَنَّ عَلَيْهِ اِ۬لنَّشْأَةَ اَ۬لُاخْر۪ىٰۖ ٤٦",
    "text_simple": "وأن عليه النشأة الاخرى"
  },
  {
    "id": 4809,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 47,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ أَغْن۪ىٰ وَأَقْن۪ىٰۖ ٤٧",
    "text_simple": "وأنه هو أغنى وأقنى"
  },
  {
    "id": 4810,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 2,
    "line_end": 3,
    "aya_no": 48,
    "text_tashkeel": "وَأَنَّهُۥ هُوَ رَبُّ اُ۬لشِّعْر۪ىٰۖ ٤٨",
    "text_simple": "وأنه هو رب الشعرى"
  },
  {
    "id": 4811,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 49,
    "text_tashkeel": "وَأَنَّهُۥٓ أَهْلَكَ عَاداٗ اَ۬لُّاول۪ىٰ ٤٩",
    "text_simple": "وأنهٓ أهلك عاداٗ الاولى"
  },
  {
    "id": 4812,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 3,
    "line_end": 4,
    "aya_no": 50,
    "text_tashkeel": "وَثَمُوداٗ فَمَآ أَبْق۪ىٰ ٥٠",
    "text_simple": "وثموداٗ فمآ أبقى"
  },
  {
    "id": 4813,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 51,
    "text_tashkeel": "وَقَوْمَ نُوحٖ مِّن قَبْلُۖ إِنَّهُمْ كَانُواْ هُمُۥٓ أَظْلَمَ وَأَطْغ۪ىٰۖ ٥١",
    "text_simple": "وقوم نوحٖ من قبل إنهم كانوا همٓ أظلم وأطغى"
  },
  {
    "id": 4814,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 52,
    "text_tashkeel": " وَالْمُوتَفِكَةَ أَهْو۪ىٰۖ ٥٢",
    "text_simple": "والموتفكة أهوى"
  },
  {
    "id": 4815,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 53,
    "text_tashkeel": "فَغَشّ۪يٰهَا مَا غَشّ۪ىٰۖ ٥٣",
    "text_simple": "فغشيها ما غشى"
  },
  {
    "id": 4816,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 5,
    "line_end": 6,
    "aya_no": 54,
    "text_tashkeel": "فَبِأَيِّ ءَالَآءِ رَبِّكَ تَتَمَار۪ىٰۖ ٥٤",
    "text_simple": "فبأي ءالآء ربك تتمارى"
  },
  {
    "id": 4817,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 55,
    "text_tashkeel": "هَٰذَا نَذِيرٞ مِّنَ اَ۬لنُّذُرِ اِ۬لُاول۪ىٰٓۖ ٥٥",
    "text_simple": "هذا نذيرٞ من النذر الاولىٓ"
  },
  {
    "id": 4818,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 56,
    "text_tashkeel": "أَزِفَتِ اِ۬لَازِفَةُۖ ٥٦",
    "text_simple": "أزفت الازفة"
  },
  {
    "id": 4819,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 57,
    "text_tashkeel": " لَيْسَ لَهَا مِن دُونِ اِ۬للَّهِ كَاشِفَةٌۖ ٥٧",
    "text_simple": "ليس لها من دون الله كاشفة"
  },
  {
    "id": 4820,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 7,
    "line_end": 8,
    "aya_no": 58,
    "text_tashkeel": "اَفَمِنْ هَٰذَا اَ۬لْحَدِيثِ تَعْجَبُونَ ٥٨",
    "text_simple": "افمن هذا الحديث تعجبون"
  },
  {
    "id": 4821,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 59,
    "text_tashkeel": "وَتَضْحَكُونَ وَلَا تَبْكُونَ ٥٩",
    "text_simple": "وتضحكون ولا تبكون"
  },
  {
    "id": 4822,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 8,
    "line_end": 8,
    "aya_no": 60,
    "text_tashkeel": "وَأَنتُمْ سَٰمِدُونَۖ ٦٠",
    "text_simple": "وأنتم سمدون"
  },
  {
    "id": 4823,
    "jozz": 27,
    "page": "528",
    "sura_no": 53,
    "sura_name_en": "An-Najm",
    "sura_name_ar": "النَّجم ",
    "line_start": 9,
    "line_end": 9,
    "aya_no": 61,
    "text_tashkeel": " فَاسْجُدُواْ لِلهِ وَاعْبُدُواْۖ ٦١",
    "text_simple": "فاسجدوا لله واعبدوا"
  }
]